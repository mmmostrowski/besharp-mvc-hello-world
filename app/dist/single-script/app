besharp_oop_type_LayersWidget_methods['showLayer']='showLayer'
besharp_oop_type_LayersWidget_method_is_returning["showLayer"]=false
besharp_oop_type_LayersWidget_method_body["showLayer"]='    local layerKey="${1}";
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    __be__visibleWidgets $this.visibleWidgets;
    __be__alreadyVisible @sets.makeOf $visibleWidgets;
    $visibleWidgets.removeAll;
    while @iterate @of $this.layersWidget @in layerWidget; do
        if @true $alreadyVisible.has $layerWidget || @same $layerWidget $widget; then
            $visibleWidgets.add $layerWidget;
        fi;
    done;
    @unset $alreadyVisible;
    __be__coordinator $this.coordinator;
    $coordinator.makeWidgetVisible $widget'
besharp_oop_type_LayersWidget_method_locals["showLayer"]='local alreadyVisible
local coordinator
local key2widget
local layerWidget
local visibleWidgets
local widget'
