    __be__activeWidgets $this.activeWidgets;
    $activeWidgets.remove $widget;
    __be__coordinator $this.coordinator;
    $coordinator.deactivateWidget $widget;
    __be__activeLayers $this.activeLayers;
    $activeLayers.remove "${layerKey}"'
besharp_oop_type_LayersWidget_method_locals["deactivateLayer"]='local activeLayers
local activeWidgets
local coordinator
local key2widget
local widget'
besharp_oop_type_LayersWidget_method_is_using_iterators["deactivateLayer"]=false
besharp_oop_type_LayersWidget_method_is_calling_parent["deactivateLayer"]=false
besharp_oop_type_LayersWidget_method_is_calling_this["deactivateLayer"]=true
besharp_oop_type_LayersWidget_methods['draw']='draw'
besharp_oop_type_LayersWidget_method_is_returning["draw"]=false
besharp_oop_type_LayersWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__visibleWidgets $this.visibleWidgets;
    __be__visibleWidgetsSize $visibleWidgets.size;
    local idx=0;
    while (( idx < visibleWidgetsSize )); do
        __be__widget $visibleWidgets.get "$(( idx++ ))";
        $widgetsLayout.drawWidget $widget $canvas;
    done'
besharp_oop_type_LayersWidget_method_locals["draw"]='local visibleWidgets
local visibleWidgetsSize
local widget'
besharp_oop_type_LayersWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_LayersWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_LayersWidget_method_is_calling_this["draw"]=true
besharp_oop_type_LayersWidget_methods['hideLayer']='hideLayer'
besharp_oop_type_LayersWidget_method_is_returning["hideLayer"]=false
besharp_oop_type_LayersWidget_method_body["hideLayer"]='    local layerKey="${1}";
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    __be__visibleWidgets $this.visibleWidgets;
    $visibleWidgets.remove $widget;
    __be__coordinator $this.coordinator;
    $coordinator.makeWidgetInvisible $widget'
besharp_oop_type_LayersWidget_method_locals["hideLayer"]='local coordinator
