besharp_oop_type_FrameWidget_method_is_using_iterators["organizeChildrenOnLayout"]=false
besharp_oop_type_FrameWidget_method_is_calling_parent["organizeChildrenOnLayout"]=false
besharp_oop_type_FrameWidget_method_is_calling_this["organizeChildrenOnLayout"]=true

