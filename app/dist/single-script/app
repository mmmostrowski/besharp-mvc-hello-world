besharp_oop_type_WidgetsLayout_method_is_calling_this["setupContextWindow"]=true
besharp_oop_type_WidgetsLayout_methods['setupWidgetFocusable']='setupWidgetFocusable'
besharp_oop_type_WidgetsLayout_method_is_returning["setupWidgetFocusable"]=false
besharp_oop_type_WidgetsLayout_method_body["setupWidgetFocusable"]='    local widget="${1}";
    local orderIndex="${2}";
    if ! @is $widget FocusableWidget; then
        besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) is expected to be implementing FocusableWidget interface!";
    fi;
    __be__widget2FocusIndex $this.widget2FocusIndex;
    if @true $widget2FocusIndex.has "${orderIndex}"; then
        besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) is expected to have unique focus order index! '"'"'${orderIndex}'"'"' is already taken!";
    fi;
    $widget2FocusIndex.set $widget "${orderIndex}"'
besharp_oop_type_WidgetsLayout_method_locals["setupWidgetFocusable"]='local widget2FocusIndex'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["setupWidgetFocusable"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["setupWidgetFocusable"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["setupWidgetFocusable"]=true
besharp_oop_type_WidgetsLayout_methods['setupWidgetPlacement']='setupWidgetPlacement'
besharp_oop_type_WidgetsLayout_method_is_returning["setupWidgetPlacement"]=false
besharp_oop_type_WidgetsLayout_method_body["setupWidgetPlacement"]='    local widget="${1}";
    local placementLeft="${2}";
    local placementUp="${3}";
    local placementRight="${4}";
    local placementDown="${5}";
    local placementWidth="${6}";
    local placementHeight="${7}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    $context.placementLeft = "${placementLeft}";
    $context.placementRight = "${placementRight}";
    $context.placementUp = "${placementUp}";
    $context.placementDown = "${placementDown}";
    $context.placementWidth = "${placementWidth}";
    $context.placementHeight = "${placementHeight}"'
besharp_oop_type_WidgetsLayout_method_locals["setupWidgetPlacement"]='local context
local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["setupWidgetPlacement"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["setupWidgetPlacement"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["setupWidgetPlacement"]=true
besharp_oop_type_WidgetsLayout_methods['updateChildrenLayout']='updateChildrenLayout'
besharp_oop_type_WidgetsLayout_method_is_returning["updateChildrenLayout"]=false
besharp_oop_type_WidgetsLayout_method_body["updateChildrenLayout"]='    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    __be__placementLeft $context.placementLeft;
    __be__placementUp $context.placementUp;
    __be__placementRight $context.placementRight;
    __be__placementDown $context.placementDown;
    if [[ "${placementLeft}" != '"'"'0'"'"' ]] || [[ "${placementRight}" != '"'"'0'"'"' ]] || [[ "${placementUp}" != '"'"'0'"'"' ]] || [[ "${placementDown}" != '"'"'0'"'"' ]]; then
        $this.updateChildrenPlacementHorizontal $widget $context "${placementLeft}" "${placementRight}";
        $this.updateChildrenPlacementVertical $widget $context "${placementUp}" "${placementDown}";
    fi;
    if @is $widget ContainerWidget; then
        $widget.organizeChildrenOnLayout $context $this;
    fi;
    if @is $widget LayoutChangesListeningWidget; then
        $widget.processLayoutChanges $context $this;
    fi'
besharp_oop_type_WidgetsLayout_method_locals["updateChildrenLayout"]='local context
local placementDown
local placementLeft
local placementRight
local placementUp
local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["updateChildrenLayout"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["updateChildrenLayout"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["updateChildrenLayout"]=true
besharp_oop_type_WidgetsLayout_methods['updateChildrenPlacementHorizontal']='updateChildrenPlacementHorizontal'
besharp_oop_type_WidgetsLayout_method_is_returning["updateChildrenPlacementHorizontal"]=true
besharp_oop_type_WidgetsLayout_method_body["updateChildrenPlacementHorizontal"]='    local widget="${1}";
    local context="${2}";
    local left="${3}";
    local right="${4}";
    if [[ "${left}" == '"'"'0'"'"' ]] && [[ "${right}" == '"'"'0'"'"' ]]; then
        return;
    fi;
    __be__ctxX1 $context.x1;
    if [[ "${left}" == '"'"'auto'"'"' ]]; then
        __be__widgetWidth $context.placementWidth;
        if (( widgetWidth < 0 )); then
            if ! @is $widget WidgetWithSize; then
                besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '"'"'auto'"'"' placement!";
            fi;
            __be__widgetWidth $widget.widgetWidth;
        fi;
        if [[ "${right}" == '"'"'auto'"'"' ]]; then
            __be__width $context.width;
            $context.x1 = $(( ctxX1 + ( ( width - widgetWidth ) / 2 )));
            $context.width = "${widgetWidth}";
        else
            __be__width $context.width;
            $context.x1 = $(( ctxX1 + width - widgetWidth - right ));
            $context.width = $(( right + widgetWidth ));
        fi;
    else
        if [[ "${right}" == '"'"'auto'"'"' ]]; then
            __be__widgetWidth $context.placementWidth;
            if (( widgetWidth < 0 )); then
                if ! @is $widget WidgetWithSize; then
                    besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '"'"'*'"'"' placement!";
                fi;
                __be__widgetWidth $widget.widgetWidth;
            fi;
            $context.x1 = $(( ctxX1 + left ));
            __be__width $context.width;
            $context.width = $(( left + widgetWidth ));
        else
            __be__x1 $context.x1;
            $context.x1 = $(( x1 + left));
            __be__width $context.width;
            (( width -= ( left + right ) ));
            $context.width = "${width}";
        fi;
    fi'
besharp_oop_type_WidgetsLayout_method_locals["updateChildrenPlacementHorizontal"]='local ctxX1
local widgetWidth
local width
local x1'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["updateChildrenPlacementHorizontal"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["updateChildrenPlacementHorizontal"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["updateChildrenPlacementHorizontal"]=false
besharp_oop_type_WidgetsLayout_methods['updateChildrenPlacementVertical']='updateChildrenPlacementVertical'
besharp_oop_type_WidgetsLayout_method_is_returning["updateChildrenPlacementVertical"]=true
besharp_oop_type_WidgetsLayout_method_body["updateChildrenPlacementVertical"]='    local widget="${1}";
    local context="${2}";
    local up="${3}";
    local down="${4}";
    if [[ "${up}" == '"'"'0'"'"' ]] && [[ "${down}" == '"'"'0'"'"' ]]; then
        return;
    fi;
    __be__ctxY1 $context.y1;
    if [[ "${up}" == '"'"'auto'"'"' ]]; then
        __be__widgetHeight $context.placementHeight;
        if (( widgetHeight < 0 )); then
            if ! @is $widget WidgetWithSize; then
                besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '"'"'*'"'"' placement!";
            fi;
            __be__widgetHeight $widget.widgetHeight;
        fi;
        if [[ "${down}" == '"'"'auto'"'"' ]]; then
            __be__height $context.height;
            $context.y1 = $(( ctxY1 + ( ( height - widgetHeight ) / 2 ) ));
            $context.height = "${widgetHeight}";
        else
            __be__height $context.height;
            $context.y1 = $(( ctxY1 + height - widgetHeight - down ));
            $context.height = $(( down + widgetHeight ));
        fi;
    else
        if [[ "${down}" == '"'"'auto'"'"' ]]; then
            __be__widgetHeight $context.placementHeight;
            if (( widgetHeight < 0 )); then
                if ! @is $widget WidgetWithSize; then
                    besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '"'"'*'"'"' placement!";
                fi;
                __be__widgetHeight $widget.widgetHeight;
            fi;
            $context.y1 = $(( ctxY1 + up ));
            __be__height $context.height;
            $context.height = $(( up + widgetHeight ));
        else
            __be__y1 $context.y1;
            $context.y1 = $(( y1 + up ));
            __be__height $context.height;
            (( height -= ( up + down ) ));
            $context.height = "${height}";
        fi;
    fi'
besharp_oop_type_WidgetsLayout_method_locals["updateChildrenPlacementVertical"]='local ctxY1
local height
local widgetHeight
local y1'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["updateChildrenPlacementVertical"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["updateChildrenPlacementVertical"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["updateChildrenPlacementVertical"]=false
besharp_oop_type_WidgetsLayout_methods['updateLayoutFor']='updateLayoutFor'
besharp_oop_type_WidgetsLayout_method_is_returning["updateLayoutFor"]=true
besharp_oop_type_WidgetsLayout_method_body["updateLayoutFor"]='    local widget="${1}";
    local canvas="${2}";
    __be__w $canvas.width;
    __be__h $canvas.height;
    if @false $this.needsUpdate; then
        __be__lastSize $this.lastSize;
        $this.lastSize = "${w}x${h}";
        if [[ "${w}x${h}" == "${lastSize}" ]]; then
            return;
        fi;
    fi;
    $this.needsUpdate = false;
    __be__widget2Context $this.widget2Context;
    __be__context $this.widgetContext $widget;
    $context.setupWindow 0 0 "${w}" "${h}";
    __be__tree $this.tree;
    $tree.walkDownTree $widget $this.updateChildrenLayout'
besharp_oop_type_WidgetsLayout_method_locals["updateLayoutFor"]='local context
local h
local lastSize
local tree
local w
local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["updateLayoutFor"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["updateLayoutFor"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["updateLayoutFor"]=true
besharp_oop_type_WidgetsLayout_methods['widgetContext']='widgetContext'
besharp_oop_type_WidgetsLayout_method_is_returning["widgetContext"]=true
besharp_oop_type_WidgetsLayout_method_body["widgetContext"]='    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    besharp.returningOf $widget2Context.get $widget'
besharp_oop_type_WidgetsLayout_method_locals["widgetContext"]='local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["widgetContext"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["widgetContext"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["widgetContext"]=true
besharp_oop_type_WidgetsLayout_methods['widgetId']='widgetId'
besharp_oop_type_WidgetsLayout_method_is_returning["widgetId"]=true
besharp_oop_type_WidgetsLayout_method_body["widgetId"]='    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    besharp.returningOf $context.widgetId'
besharp_oop_type_WidgetsLayout_method_locals["widgetId"]='local context
local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["widgetId"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["widgetId"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["widgetId"]=true
besharp_oop_type_WidgetsLayout_methods['widgetSortIndex']='widgetSortIndex'
besharp_oop_type_WidgetsLayout_method_is_returning["widgetSortIndex"]=true
besharp_oop_type_WidgetsLayout_method_body["widgetSortIndex"]='    local widget="${1}";
    __be__widget2FocusIndex $this.widget2FocusIndex;
    besharp.returningOf $widget2FocusIndex.get $widget'
besharp_oop_type_WidgetsLayout_method_locals["widgetSortIndex"]='local widget2FocusIndex'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["widgetSortIndex"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_parent["widgetSortIndex"]=false
besharp_oop_type_WidgetsLayout_method_is_calling_this["widgetSortIndex"]=true

fi
if ${beshfile_section__code:-false}; then :; 
