besharp_oop_type_FpsWidget_method_is_calling_parent["FpsWidget"]=true
besharp_oop_type_FpsWidget_method_is_calling_this["FpsWidget"]=false
besharp_oop_type_FpsWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_FpsWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_FpsWidget_method_body["calculateMinSize"]='    local calculator="${1}"'
besharp_oop_type_FpsWidget_method_locals["calculateMinSize"]=''
besharp_oop_type_FpsWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_FpsWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_FpsWidget_method_is_calling_this["calculateMinSize"]=false
