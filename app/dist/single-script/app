  "${@}"
  widgetContext="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgetsLayout() {
  "${@}"
  widgetsLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetAnimationController () 
{ 
    __be___this_activeAnimatedWidgets @vectors.make;
    __be___this_cloneOfActiveWidgets @vectors.make
}
WidgetAnimationController.activateWidgetAnimation () 
{ 
    local widget="${1}";
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    if @true $activeAnimatedWidgets.has $widget; then
        return;
    fi;
    $activeAnimatedWidgets.add $widget
}
WidgetAnimationController.deactivateWidgetAnimation () 
{ 
    local widget="${1}";
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    $activeAnimatedWidgets.remove $widget
}
WidgetAnimationController.processAnimationStep () 
{ 
    __be__widgetsLayout $this.widgetsLayout;
    $this.isAnimationActive = false;
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    if @true $activeAnimatedWidgets.isEmpty; then
        return;
    fi;
    __be__cloneOfActiveWidgets $this.cloneOfActiveWidgets;
    @collections.cloneTo $cloneOfActiveWidgets $activeAnimatedWidgets;
    __be__count $cloneOfActiveWidgets.size;
    while (( --count >= 0 )); do
        __be__widget $cloneOfActiveWidgets.get "${count}";
        __be__widgetContext $widgetsLayout.widgetContext $widget;
        if @is $widget StoppableAnimationWidget; then
            __be__appState $this.appState;
            if @false $appState.isAnimationEnabled; then
                continue;
            fi;
        fi;
        __be__animationFramesNum $widget.processAnimationFrame $widgetContext $widgetsLayout;
        if (( animationFramesNum > 0 )); then
            $this.isAnimationActive = true;
        fi;
    done
}
WidgetAnimationController.registerAnimatedWidget () 
{ 
    local widget="${1}";
    __be__widgetsLayout $this.widgetsLayout;
    __be__widgetContext $widgetsLayout.widgetContext $widget;
    $widget.initializeAnimation $widgetContext $widgetsLayout
}
WidgetAnimationController.unregisterAnimatedWidget () 
{ 
    local widget="${1}"
}

fi
