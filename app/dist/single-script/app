function __be__CanvasPrinter() {
  "${@}"
  CanvasPrinter="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__color() {
  "${@}"
  color="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__fps() {
  "${@}"
  fps="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__frameCounter() {
  "${@}"
  frameCounter="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__lastSecond() {
  "${@}"
  lastSecond="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x() {
  "${@}"
  x="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y() {
  "${@}"
  y="${besharp_rcrvs[besharp_rcsl + 1]}"
}
FpsWidget () 
{ 
    @parent "${@}"
}
FpsWidget.calculateMinSize () 
{ 
    local calculator="${1}"
}
FpsWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__x $context.x1;
    __be__y $context.y1;
    __be__CanvasPrinter $this.CanvasPrinter;
    __be__fps $this.fps;
    __be__color @pixel_modes.dGray;
    $CanvasPrinter.printText $canvas "${x}" "${y}" "FPS ${fps}" "${color}";
    local currentSecond="";
    currentSecond="$( date +%s 2> /dev/null || true )";
    if [[ -z "${currentSecond}" ]]; then
        return;
    fi;
    __be__frameCounter $this.frameCounter;
    __be__lastSecond $this.lastSecond;
    (( ++frameCounter ));
    if (( currentSecond > lastSecond )); then
        $this.fps = $(( frameCounter / ( currentSecond - lastSecond ) ));
        $this.frameCounter = 0;
        $this.lastSecond = "${currentSecond}";
    else
        $this.frameCounter = "${frameCounter}";
    fi
}
FpsWidget.initialize () 
{ 
    $this.lastSecond = "$( date +%s < /dev/null )"
}

fi
