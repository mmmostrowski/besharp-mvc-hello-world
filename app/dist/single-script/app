if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["NamedArgs"]='class'
besharp_oop_types["NamedArgs"]="NamedArgs"
declare -Ag besharp_oop_type_NamedArgs_interfaces
besharp_oop_classes["NamedArgs"]="NamedArgs"
besharp_oop_type_parent["NamedArgs"]="Object"
declare -Ag besharp_oop_type_NamedArgs_fields
declare -Ag besharp_oop_type_NamedArgs_injectable_fields
declare -Ag besharp_oop_type_NamedArgs_field_type
declare -Ag besharp_oop_type_NamedArgs_field_default
declare -Ag besharp_oop_type_NamedArgs_methods
declare -Ag besharp_oop_type_NamedArgs_method_body
declare -Ag besharp_oop_type_NamedArgs_method_locals
declare -Ag besharp_oop_type_NamedArgs_method_is_returning
declare -Ag besharp_oop_type_NamedArgs_method_is_abstract
declare -Ag besharp_oop_type_NamedArgs_method_is_using_iterators
declare -Ag besharp_oop_type_NamedArgs_method_is_calling_parent
declare -Ag besharp_oop_type_NamedArgs_method_is_calling_this
besharp_oop_type_static["NamedArgs"]='NamedArgs'
besharp_oop_type_static_accessor["NamedArgs"]='@args'
besharp_oop_type_NamedArgs_fields['currentArgs']='currentArgs'
besharp_oop_type_NamedArgs_field_type['currentArgs']="Map"
besharp_oop_type_NamedArgs_field_default['currentArgs']=""
besharp_oop_type_NamedArgs_methods['NamedArgs']='NamedArgs'
besharp_oop_type_NamedArgs_method_is_returning["NamedArgs"]=false
besharp_oop_type_NamedArgs_method_body["NamedArgs"]='    __be___this_currentArgs @maps.make'
besharp_oop_type_NamedArgs_method_locals["NamedArgs"]=''
besharp_oop_type_NamedArgs_method_is_using_iterators["NamedArgs"]=false
besharp_oop_type_NamedArgs_method_is_calling_parent["NamedArgs"]=false
besharp_oop_type_NamedArgs_method_is_calling_this["NamedArgs"]=true
besharp_oop_type_NamedArgs_methods['make']='make'
besharp_oop_type_NamedArgs_method_is_returning["make"]=false
besharp_oop_type_NamedArgs_method_body["make"]='    __be__args @vectors.make "${@}";
    __be__markerIndex $args.findIndex "@from";
    if (( markerIndex < 0 )); then
        besharp.runtime.error "Missing '"'"'@from'"'"' item!";
    fi;
    $this.reset;
    __be__currentArgs $this.currentArgs;
    local i;
    for ((i = 0; i < markerIndex; ++i ))
    do
        __be__name $args.get "${i}";
        $currentArgs.set "${name}" "";
    done;
    __be__argsCount $args.size;
    for ((i = markerIndex + 1; i < argsCount; ++i ))
    do
        __be__item $args.get "${i}";
        if [[ "${item%:}" != "${item}" ]] && $currentArgs.hasKey "${item%:}"; then
            __be__currentVector @vectors.make;
            $currentArgs.set "${item%:}" $currentVector;
            continue;
        fi;
        $currentVector.add "${item}";
    done;
    @unset $args'
besharp_oop_type_NamedArgs_method_locals["make"]='local args
local argsCount
local currentArgs
local currentVector
local item
local markerIndex
local name'
besharp_oop_type_NamedArgs_method_is_using_iterators["make"]=false
besharp_oop_type_NamedArgs_method_is_calling_parent["make"]=false
besharp_oop_type_NamedArgs_method_is_calling_this["make"]=true
besharp_oop_type_NamedArgs_methods['optionalInt']='optionalInt'
besharp_oop_type_NamedArgs_method_is_returning["optionalInt"]=true
besharp_oop_type_NamedArgs_method_body["optionalInt"]='    local name="${1}";
    local default="${2}";
    __be__currentArgs $this.currentArgs;
    __be__vector $currentArgs.get "${name}";
    if [[ -z "${vector}" ]] || @true $vector.isEmpty; then
        besharp_rcrvs[besharp_rcsl]="${default}";
        return;
    fi;
    __be__vectorSize $vector.size;
    if (( vectorSize > 1 )); then
        besharp.runtime.error "'"'"'${name}'"'"' argument is expected to be a single number!";
    fi;
    __be__value $vector.get 0;
    if ! [[ "${value}" =~ ^[0-9]+$ ]]; then
        besharp.runtime.error "'"'"'${name}'"'"' argument is expected to be a number!";
    fi;
    besharp_rcrvs[besharp_rcsl]="${value}"'
besharp_oop_type_NamedArgs_method_locals["optionalInt"]='local currentArgs
local value
local vector
local vectorSize'
besharp_oop_type_NamedArgs_method_is_using_iterators["optionalInt"]=false
besharp_oop_type_NamedArgs_method_is_calling_parent["optionalInt"]=false
besharp_oop_type_NamedArgs_method_is_calling_this["optionalInt"]=true
besharp_oop_type_NamedArgs_methods['optionalText']='optionalText'
besharp_oop_type_NamedArgs_method_is_returning["optionalText"]=true
