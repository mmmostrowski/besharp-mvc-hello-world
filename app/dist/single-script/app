CompositionWidget () 
{ 
    __be___this_widgets @vectors.make "${@}"
}
CompositionWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    local maxWidth=-1;
    local maxHeight=-1;
    while @iterate @of $this.widgets @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    $calculator.setupSize "${maxWidth}" "${maxHeight}"
}
CompositionWidget.childrenWidgets () 
{ 
    besharp.returningOf $this.widgets
}
CompositionWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__widgets $this.widgets;
    __be__widgetsSize $widgets.size;
    local idx=0;
    while (( idx < widgetsSize )); do
        __be__widget $widgets.get "$(( idx++ ))";
        $widgetsLayout.drawWidget $widget $canvas;
    done
}
CompositionWidget.initialize () 
{ 
    :
}
CompositionWidget.operativeChildrenWidget () 
{ 
    besharp.returningOf $this.widgets
}
CompositionWidget.organizeChildrenOnLayout () 
{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    while @iterate @of $this.widgets @in children; do
        $widgetsLayout.setupContextWindow $children "${x1}" "${y1}" "${width}" "${height}";
    done
}

