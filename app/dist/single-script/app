function __be__y() {
  "${@}"
  y="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___this_boldFormat() {
  "${@}"
  eval $this"_boldFormat=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
