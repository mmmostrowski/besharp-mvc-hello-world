besharp_oop_type_WidgetsLayout_method_is_returning["initialize"]=false
besharp_oop_type_WidgetsLayout_method_body["initialize"]='    __be___this_widget2FocusIndex @maps.make;
    __be___this_widget2Context @maps.make;
    __be___this_context2Widget @maps.make;
    __be___this_widgetId2widget @maps.make'
besharp_oop_type_WidgetsLayout_method_locals["initialize"]=''
