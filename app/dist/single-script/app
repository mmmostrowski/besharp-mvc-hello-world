besharp_oop_type_AppController_methods['removeAllBalls']='removeAllBalls'
besharp_oop_type_AppController_method_is_returning["removeAllBalls"]=false
besharp_oop_type_AppController_method_body["removeAllBalls"]='    while @iterate @of $this.ballsWidgets @in widget; do
        $this.runWidgetAction $widget '"'"'removeAllBalls'"'"';
    done'
besharp_oop_type_AppController_method_locals["removeAllBalls"]='local widget'
besharp_oop_type_AppController_method_is_using_iterators["removeAllBalls"]=true
besharp_oop_type_AppController_method_is_calling_parent["removeAllBalls"]=false
besharp_oop_type_AppController_method_is_calling_this["removeAllBalls"]=true
besharp_oop_type_AppController_methods['removeBallsSection']='removeBallsSection'
besharp_oop_type_AppController_method_is_returning["removeBallsSection"]=true
besharp_oop_type_AppController_method_body["removeBallsSection"]='    __be__appLayout $this.appLayout;
    __be__oldHandler $appLayout.ballsHandlerWidget;
    if @is $oldHandler BallsWidget; then
        return;
    else
        if @is $oldHandler TwoRowsWidget; then
            __be__topBalls $oldHandler.downWidget;
            __be__newHandler $oldHandler.upWidget;
        else
            if @is $oldHandler TwoColumnsWidget; then
                __be__topBalls $oldHandler.rightWidget;
                __be__newHandler $oldHandler.leftWidget;
            fi;
        fi;
    fi;
    $appLayout.replaceTopBallsSection $newHandler;
    $this.removeWidgetOnTheFly $topBalls;
    $this.removeWidgetOnTheFly $oldHandler;
    __be__ballsWidgets $this.ballsWidgets;
    $ballsWidgets.remove $topBalls;
    @unset $topBalls;
    @unset $oldHandler'
besharp_oop_type_AppController_method_locals["removeBallsSection"]='local appLayout
local ballsWidgets
local newHandler
local oldHandler
local topBalls'
besharp_oop_type_AppController_method_is_using_iterators["removeBallsSection"]=false
besharp_oop_type_AppController_method_is_calling_parent["removeBallsSection"]=false
besharp_oop_type_AppController_method_is_calling_this["removeBallsSection"]=true
besharp_oop_type_AppController_methods['removeOneBall']='removeOneBall'
besharp_oop_type_AppController_method_is_returning["removeOneBall"]=true
besharp_oop_type_AppController_method_body["removeOneBall"]='    local count=0;
    __be__nonEmptyBallsWidgets @vectors.make;
    while @iterate @of $this.ballsWidgets @in widget; do
        if @returned @of $widget.ballsNum @gt 0; then
            (( ++count ));
            $nonEmptyBallsWidgets.add $widget;
        fi;
    done;
    if (( count > 0 )); then
        __be__randomBallsWidget $nonEmptyBallsWidgets.get $(( RANDOM % count ));
        $this.runWidgetAction $randomBallsWidget '"'"'removeBall'"'"';
    fi;
    @unset $nonEmptyBallsWidgets'
besharp_oop_type_AppController_method_locals["removeOneBall"]='local nonEmptyBallsWidgets
local randomBallsWidget
local widget'
besharp_oop_type_AppController_method_is_using_iterators["removeOneBall"]=true
besharp_oop_type_AppController_method_is_calling_parent["removeOneBall"]=false
besharp_oop_type_AppController_method_is_calling_this["removeOneBall"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be__appLayout() {
  "${@}"
  appLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__balls() {
  "${@}"
  balls="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__ballsWidgets() {
  "${@}"
  ballsWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__wrapper() {
  "${@}"
  wrapper="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__count() {
  "${@}"
  count="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__randomBallsWidget() {
  "${@}"
  randomBallsWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgets() {
  "${@}"
  widgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__appLayout() {
  "${@}"
  appLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__layers() {
  "${@}"
  layers="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgetsLayout() {
  "${@}"
  widgetsLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__appLayout() {
  "${@}"
  appLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__layers() {
  "${@}"
  layers="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgetsLayout() {
  "${@}"
  widgetsLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__appLayout() {
  "${@}"
  appLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__initialBalls() {
  "${@}"
  initialBalls="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___this_ballsWidgets() {
  "${@}"
  eval $this"_ballsWidgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__appLayout() {
  "${@}"
  appLayout="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__ballsWidgets() {
  "${@}"
  ballsWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__newHandler() {
  "${@}"
  newHandler="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__oldHandler() {
  "${@}"
  oldHandler="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__topBalls() {
  "${@}"
  topBalls="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__nonEmptyBallsWidgets() {
  "${@}"
  nonEmptyBallsWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__randomBallsWidget() {
  "${@}"
  randomBallsWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
AppController () 
{ 
    @parent "${@}"
}
AppController.addBallsSection () 
{ 
    local layoutCallback="${1}";
    __be__appLayout $this.appLayout;
    __be__balls $appLayout.${layoutCallback};
    __be__wrapper $appLayout.ballsHandlerWidget;
    $this.addNewWidgetOnTheFly $wrapper true;
    $this.addNewWidgetOnTheFly $balls true;
    __be__ballsWidgets $this.ballsWidgets;
    $ballsWidgets.add $balls
}
AppController.addHorizontalBallsSection () 
{ 
    $this.addBallsSection 'makeNewHorizontalBallsSection'
}
AppController.addOneBall () 
{ 
    __be__widgets $this.ballsWidgets;
    __be__count $widgets.size;
    __be__randomBallsWidget $widgets.get $(( RANDOM % count ));
    $this.runWidgetAction $randomBallsWidget 'addBall'
}
AppController.addVerticalBallsSection () 
{ 
    $this.addBallsSection 'makeNewVerticalBallsSection'
}
AppController.closeAppSettings () 
{ 
    $this.closePopup "settings"
}
AppController.closeAreYouSureQuit () 
{ 
    $this.closePopup "are_you_sure_quit"
}
AppController.closeHelp () 
{ 
    $this.closePopup "help"
}
AppController.closePopup () 
{ 
    local layer="${1}";
    __be__appLayout $this.appLayout;
    __be__layers $appLayout.layers;
    if @false $layers.isLayerActive "${layer}"; then
        return;
    fi;
    __be__widgetsLayout $this.widgetsLayout;
    $layers.turnOnLayer 'main';
    $layers.turnOffLayer "${layer}";
    $layers.popLastFocusIndex;
    $widgetsLayout.needsUpdate = true
}
AppController.onEscapeCommandRequested () 
{ 
    $this.openAreYouSureQuit
}
AppController.onHelpCommandRequested () 
{ 
    $this.openHelp
}
AppController.openAppSettings () 
{ 
    $this.openPopup "settings"
}
AppController.openAreYouSureQuit () 
{ 
    $this.openPopup "are_you_sure_quit"
}
AppController.openHelp () 
{ 
    $this.openPopup "help"
}
AppController.openPopup () 
{ 
    local layer="${1}";
    __be__appLayout $this.appLayout;
    __be__layers $appLayout.layers;
    if @true $layers.isLayerActive "${layer}"; then
        return;
    fi;
    __be__widgetsLayout $this.widgetsLayout;
    $layers.deactivateLayer 'main';
    $layers.turnOnLayer "${layer}";
    $layers.pushLayerFocusIndex "${layer}";
    $widgetsLayout.needsUpdate = true
}
AppController.prepareRootWidget () 
{ 
    @parent;
    __be__appLayout $this.appLayout;
    __be__initialBalls $appLayout.ballsHandlerWidget;
    __be___this_ballsWidgets @vectors.make $initialBalls
}
AppController.quitApp () 
{ 
    $this.active = false
}
AppController.removeAllBalls () 
{ 
    while @iterate @of $this.ballsWidgets @in widget; do
        $this.runWidgetAction $widget 'removeAllBalls';
    done
}
AppController.removeBallsSection () 
{ 
    __be__appLayout $this.appLayout;
    __be__oldHandler $appLayout.ballsHandlerWidget;
    if @is $oldHandler BallsWidget; then
        return;
    else
        if @is $oldHandler TwoRowsWidget; then
            __be__topBalls $oldHandler.downWidget;
            __be__newHandler $oldHandler.upWidget;
        else
            if @is $oldHandler TwoColumnsWidget; then
                __be__topBalls $oldHandler.rightWidget;
                __be__newHandler $oldHandler.leftWidget;
            fi;
        fi;
    fi;
    $appLayout.replaceTopBallsSection $newHandler;
    $this.removeWidgetOnTheFly $topBalls;
    $this.removeWidgetOnTheFly $oldHandler;
    __be__ballsWidgets $this.ballsWidgets;
    $ballsWidgets.remove $topBalls;
    @unset $topBalls;
    @unset $oldHandler
}
AppController.removeOneBall () 
{ 
    local count=0;
    __be__nonEmptyBallsWidgets @vectors.make;
    while @iterate @of $this.ballsWidgets @in widget; do
        if @returned @of $widget.ballsNum @gt 0; then
            (( ++count ));
            $nonEmptyBallsWidgets.add $widget;
        fi;
    done;
    if (( count > 0 )); then
        __be__randomBallsWidget $nonEmptyBallsWidgets.get $(( RANDOM % count ));
        $this.runWidgetAction $randomBallsWidget 'removeBall';
    fi;
    @unset $nonEmptyBallsWidgets
}

fi
