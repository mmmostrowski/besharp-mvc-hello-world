besharp_oop_type_PopupFrameWidget_interfaces['Widget']='Widget'
besharp_oop_type_PopupFrameWidget_interfaces['ContainerWidget']='ContainerWidget'
besharp_oop_type_PopupFrameWidget_interfaces['StatefulWidget']='StatefulWidget'
besharp_oop_type_PopupFrameWidget_interfaces['FocusableWidget']='FocusableWidget'
besharp_oop_classes["PopupFrameWidget"]="PopupFrameWidget"
besharp_oop_type_parent["PopupFrameWidget"]="Object"
declare -Ag besharp_oop_type_PopupFrameWidget_fields
declare -Ag besharp_oop_type_PopupFrameWidget_injectable_fields
declare -Ag besharp_oop_type_PopupFrameWidget_field_type
declare -Ag besharp_oop_type_PopupFrameWidget_field_default
