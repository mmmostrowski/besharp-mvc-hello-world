    local widget="${2}";
    __be__key2focusWidget $this.key2focusWidget;
    $key2focusWidget.set "${layerKey}" "${widget}"'
besharp_oop_type_LayersWidget_method_locals["setupInitialFocusWidget"]='local key2focusWidget'
besharp_oop_type_LayersWidget_method_is_using_iterators["setupInitialFocusWidget"]=false
