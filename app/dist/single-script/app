besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["processSingleWidgetInput"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["processSingleWidgetInput"]=false
besharp_oop_type_WidgetsCoordinator_methods['pushFocusIndex']='pushFocusIndex'
besharp_oop_type_WidgetsCoordinator_method_is_returning["pushFocusIndex"]=false
besharp_oop_type_WidgetsCoordinator_method_body["pushFocusIndex"]='    local sortIndex="${1}";
    __be__currentSortIndex $this.currentSortIndex;
    __be__focusIndicesStack $this.focusIndicesStack;
    $focusIndicesStack.add "${currentSortIndex}";
    $this.switchToFocusIndex "${sortIndex}"'
besharp_oop_type_WidgetsCoordinator_method_locals["pushFocusIndex"]='local currentSortIndex
