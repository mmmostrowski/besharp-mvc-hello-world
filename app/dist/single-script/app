}
AppLayout.ballsTab () 
{ 
    __be__appState $this.appState;
    __be__appController $this.appController;
    __be__balls $this.createWidget id: 'balls_tab_balls' construct: BallsWidget 1 placement: 0 0 0 0;
    $this.ballsHandlerWidget = $balls;
    __be__sectionsGroup $this.ballsTabSectionsGroup;
    __be__ballsGroup $this.ballsTabBallsGroup;
    __be__controlPanel $this.createWidget id: 'balls_tab_control_panel' construct: CompositionWidget $sectionsGroup $ballsGroup placement: 0 0 0 0;
    __be___this_ballsContainerWidget $this.createWidget id: 'balls_tab_main_frame' construct: TwoRowsWidget $controlPanel 10 $balls auto placement: 0 0 0 0;
    besharp.returningOf $this.ballsContainerWidget
}
