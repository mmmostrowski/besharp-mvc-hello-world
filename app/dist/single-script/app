besharp_oop_type_CheckboxWidget_method_is_calling_this["bindWidgetState"]=true
besharp_oop_type_CheckboxWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_CheckboxWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_CheckboxWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "${#text}" 1'
besharp_oop_type_CheckboxWidget_method_locals["calculateMinSize"]='local text'
besharp_oop_type_CheckboxWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_CheckboxWidget_methods['check']='check'
besharp_oop_type_CheckboxWidget_method_is_returning["check"]=false
besharp_oop_type_CheckboxWidget_method_body["check"]='    __be__state $this.state;
    $state.writeWidgetState $this '"'"'checked'"'"' true'
besharp_oop_type_CheckboxWidget_method_locals["check"]='local state'
besharp_oop_type_CheckboxWidget_method_is_using_iterators["check"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_parent["check"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_this["check"]=true
besharp_oop_type_CheckboxWidget_methods['draw']='draw'
besharp_oop_type_CheckboxWidget_method_is_returning["draw"]=false
besharp_oop_type_CheckboxWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__canvasPrinter $this.canvasPrinter;
    __be__reset @pixel_modes.reset;
    if @true $context.focusedFlag; then
        __be__color @pixel_modes.white;
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" "-[x] ${text}-" "${reset}${color}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" "-[ ] ${text}-" "${reset}${color}";
        fi;
    else
        __be__color @pixel_modes.dGray;
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" " [x] ${text} " "${reset}${color}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" " [ ] ${text} " "${reset}${color}";
        fi;
    fi'
besharp_oop_type_CheckboxWidget_method_locals["draw"]='local canvasPrinter
local color
local reset
local text
local x
local y'
besharp_oop_type_CheckboxWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_this["draw"]=true
besharp_oop_type_CheckboxWidget_methods['initialize']='initialize'
besharp_oop_type_CheckboxWidget_method_is_returning["initialize"]=false
besharp_oop_type_CheckboxWidget_method_body["initialize"]='    :'
besharp_oop_type_CheckboxWidget_method_locals["initialize"]=''
besharp_oop_type_CheckboxWidget_method_is_using_iterators["initialize"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_parent["initialize"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_this["initialize"]=false
besharp_oop_type_CheckboxWidget_methods['isChecked']='isChecked'
