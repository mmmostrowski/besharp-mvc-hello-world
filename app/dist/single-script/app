    __be__key2focusWidget $this.key2focusWidget;
    __be__focusWidget $key2focusWidget.get "${layerKey}";
    __be__coordinator $this.coordinator;
    $coordinator.pushFocusWidget $focusWidget'
besharp_oop_type_LayersWidget_method_locals["pushLayerFocusIndex"]='local coordinator
