    esac
}
MouseDriver.disableMouseCapture () 
{ 
    echo -e "\e[?1000l";
    echo -e "\e[?1002l";
    echo -e "\e[?1003l";
    echo -e "\e[?9l"
}
MouseDriver.enableMouseButtonsCapture () 
{ 
    echo -e "\e[?1000h";
    echo -e "\e[?9h";
    $this.subscribeToShutdown
}
MouseDriver.enableMouseDragCapture () 
{ 
    echo -e "\e[?1002h";
    $this.subscribeToShutdown
}
MouseDriver.enableMouseMovementCapture () 
{ 
    echo -e "\e[?1003h";
    $this.subscribeToShutdown
}
MouseDriver.subscribeToShutdown () 
{ 
    besharp.addShutdownCallback 'mouse_disable_capture' $this.disableMouseCapture
}
MouseDriver.updateMouseStatus () 
{ 
    local input="${1}";
    local target="${2}";
    __be__buttonStatus $this.chrToInt "${input:3:1}";
    __be___target_mouseX $this.chrToInt "${input:4:1}";
    __be___target_mouseY $this.chrToInt "${input:5:1}";
    case ${buttonStatus} in 
        0)
            $target.mouseButtonLeft = true;
            $target.mouseButtonRight = false
        ;;
        1)
            $target.mouseButtonLeft = false;
            $target.mouseButtonRight = true
        ;;
        2)
            $target.mouseButtonLeft = false;
            $target.mouseButtonRight = false
        ;;
    esac
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["MouseKeys"]='class'
besharp_oop_types["MouseKeys"]="MouseKeys"
declare -Ag besharp_oop_type_MouseKeys_interfaces
besharp_oop_classes["MouseKeys"]="MouseKeys"
