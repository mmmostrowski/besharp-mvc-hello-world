    __be__resultCollection @new ArrayVector;
    besharp_rcrvs[besharp_rcsl]=$resultCollection;
    local var="${this}[@]";
    if [[ "${!var+isset}" != "isset" ]]; then
        return;
    fi;
    local temp;
    declare -n temp="${this}";
    local key;
    for key in "${!temp[@]}";
