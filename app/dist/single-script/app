besharp_oop_type_RadioWidget_method_body["processKeyboardEvent"]='    local event="${1}";
    if @true $event.consumeWhenCommandRequested '"'"'action'"'"'; then
        $this.check;
    fi'
besharp_oop_type_RadioWidget_method_locals["processKeyboardEvent"]=''
