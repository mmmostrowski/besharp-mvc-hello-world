    if @true $event.consumeWhenCommandRequested '"'"'action'"'"'; then
        $this.toggle;
    fi'
besharp_oop_type_CheckboxWidget_method_locals["processKeyboardEvent"]=''
besharp_oop_type_CheckboxWidget_method_is_using_iterators["processKeyboardEvent"]=false
