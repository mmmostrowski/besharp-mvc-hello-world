if ${beshfile_section__code:-false}; then :; 
function __be__width() {
  "${@}"
  width="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__canvasPrinter() {
  "${@}"
  canvasPrinter="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__color() {
  "${@}"
  color="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__cursor() {
  "${@}"
  cursor="${besharp_rcrvs[besharp_rcsl + 1]}"
}
