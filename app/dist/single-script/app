if ${beshfile_section__code:-false}; then :; 
function __be__input() {
  "${@}"
  input="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetInputEvent () 
{ 
    $this.input = "${1}";
    $this.widgetContext = "${2}"
}
WidgetInputEvent.consumeWhenCommandRequested () 
{ 
    __be__input $this.input;
    besharp_rcrvs[besharp_rcsl]=false;
    local command;
    for command in "${@}";
    do
        if @returned @of $input.requestedCommand == "${command}"; then
            $this.isConsumed = true;
            besharp_rcrvs[besharp_rcsl]=true;
            return;
        fi;
    done
}

