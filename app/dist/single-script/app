    $tree.walkDownTreeOperatives $widget $this.makeSingleWidgetVisible "true"'
besharp_oop_type_WidgetsCoordinator_method_locals["makeWidgetVisible"]='local tree'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["makeWidgetVisible"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["makeWidgetVisible"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["makeWidgetVisible"]=true
besharp_oop_type_WidgetsCoordinator_methods['popFocusIndex']='popFocusIndex'
besharp_oop_type_WidgetsCoordinator_method_is_returning["popFocusIndex"]=true
besharp_oop_type_WidgetsCoordinator_method_body["popFocusIndex"]='    __be__focusIndicesStack $this.focusIndicesStack;
    __be__sortIndex $focusIndicesStack.pull;
    $this.switchToFocusIndex "${sortIndex}";
    besharp_rcrvs[besharp_rcsl]="${sortIndex}"'
besharp_oop_type_WidgetsCoordinator_method_locals["popFocusIndex"]='local focusIndicesStack
