function __be___this_currentArgs() {
  "${@}"
  eval $this"_currentArgs=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__args() {
  "${@}"
  args="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__argsCount() {
  "${@}"
  argsCount="${besharp_rcrvs[besharp_rcsl + 1]}"
}
