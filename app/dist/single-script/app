besharp_oop_type_PopupFrameWidget_field_type['widgets']="Vector"
besharp_oop_type_PopupFrameWidget_field_default['widgets']=""
besharp_oop_type_PopupFrameWidget_fields['state']='state'
besharp_oop_type_PopupFrameWidget_field_type['state']="WidgetStateKeeper"
besharp_oop_type_PopupFrameWidget_field_default['state']=""
besharp_oop_type_PopupFrameWidget_methods['PopupFrameWidget']='PopupFrameWidget'
besharp_oop_type_PopupFrameWidget_method_is_returning["PopupFrameWidget"]=false
besharp_oop_type_PopupFrameWidget_method_body["PopupFrameWidget"]='    __be___this_widgets @vectors.make "${@}"'
besharp_oop_type_PopupFrameWidget_method_locals["PopupFrameWidget"]=''
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["PopupFrameWidget"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["PopupFrameWidget"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["PopupFrameWidget"]=true
besharp_oop_type_PopupFrameWidget_methods['bindWidgetState']='bindWidgetState'
besharp_oop_type_PopupFrameWidget_method_is_returning["bindWidgetState"]=false
besharp_oop_type_PopupFrameWidget_method_body["bindWidgetState"]='    $this.state = "${1}"'
besharp_oop_type_PopupFrameWidget_method_locals["bindWidgetState"]=''
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["bindWidgetState"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["bindWidgetState"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["bindWidgetState"]=true
besharp_oop_type_PopupFrameWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_PopupFrameWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_PopupFrameWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    local maxWidth=-1;
    local maxHeight=-1;
    while @iterate @of $this.widgets @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    (( ++maxWidth));
    (( ++maxHeight ));
    $calculator.setupSize "${maxWidth}" "${maxHeight}"'
besharp_oop_type_PopupFrameWidget_method_locals["calculateMinSize"]='local children
local maxHeight
local maxWidth'
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["calculateMinSize"]=true
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_PopupFrameWidget_methods['childrenWidgets']='childrenWidgets'
besharp_oop_type_PopupFrameWidget_method_is_returning["childrenWidgets"]=true
besharp_oop_type_PopupFrameWidget_method_body["childrenWidgets"]='    besharp.returningOf $this.widgets'
besharp_oop_type_PopupFrameWidget_method_locals["childrenWidgets"]=''
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["childrenWidgets"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["childrenWidgets"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["childrenWidgets"]=true
besharp_oop_type_PopupFrameWidget_methods['draw']='draw'
besharp_oop_type_PopupFrameWidget_method_is_returning["draw"]=false
besharp_oop_type_PopupFrameWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    eval "
            local x1=\"\${${context}_x1}\"
            local y1=\"\${${context}_y1}\"
            local width=\"\${${context}_width}\"
            local height=\"\${${context}_height}\"
            local canvasPrinter=\"\${${this}_canvasPrinter}\"
        ";
    __be__dimReset @pixel_modes.dimReset;
    __be__backgroundColor @pixel_modes.blue;
    $canvas.putPixelsBox "$(( x1 + 1 ))" "$(( y1 + 1 ))" "$(( width - 2 ))" "$(( height - 2 ))" "." "${backgroundColor}";
    if @true $this.isShadowsRenderingEnabled; then
        __be__shadowColor @pixel_modes.dGray;
        $canvas.putPixelsHorizModes "$(( x1 + 1  ))" "$(( y1 + height ))" "${width}" "${shadowColor}";
        $canvas.putPixelsVertModes "$(( x1 + width ))" "$(( y1 + 1 ))" "${height}" "${shadowColor}";
        $canvas.putPixelsVertModes "$(( x1 + width + 1))" "$(( y1 + 1 ))" "${height}" "${shadowColor}";
        $canvas.putPixelModes "$(( x1 + width - 1 ))" "$(( y1 + height - 1 ))" "${shadowColor}";
    fi;
    __be__title $this.title;
    if @true $context.isHighlighted; then
        $canvasPrinter.printHighlightFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    else
        $canvasPrinter.printFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    fi;
    $canvasPrinter.printText $canvas $(( x1 + 3 )) "${y1}" " ${title} ";
    __be__widgets $this.widgets;
    __be__widgetsSize $widgets.size;
    local idx=0;
    while (( idx < widgetsSize )); do
        __be__widget $widgets.get "$(( idx++ ))";
        $widgetsLayout.drawWidget $widget $canvas;
    done'
besharp_oop_type_PopupFrameWidget_method_locals["draw"]='local backgroundColor
local dimReset
local shadowColor
local title
local widget
local widgets
local widgetsSize'
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["draw"]=true
besharp_oop_type_PopupFrameWidget_methods['initialize']='initialize'
besharp_oop_type_PopupFrameWidget_method_is_returning["initialize"]=false
besharp_oop_type_PopupFrameWidget_method_body["initialize"]='    :'
besharp_oop_type_PopupFrameWidget_method_locals["initialize"]=''
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["initialize"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_parent["initialize"]=false
besharp_oop_type_PopupFrameWidget_method_is_calling_this["initialize"]=false
besharp_oop_type_PopupFrameWidget_methods['isShadowsRenderingEnabled']='isShadowsRenderingEnabled'
besharp_oop_type_PopupFrameWidget_method_is_returning["isShadowsRenderingEnabled"]=true
besharp_oop_type_PopupFrameWidget_method_body["isShadowsRenderingEnabled"]='    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this '"'"'shadowsEnabled'"'"' true'
besharp_oop_type_PopupFrameWidget_method_locals["isShadowsRenderingEnabled"]='local state'
