if ${beshfile_section__code:-false}; then :; 
function __be__mouse() {
  "${@}"
  mouse="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__keyboard() {
  "${@}"
  keyboard="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__keyboardInput() {
  "${@}"
  keyboardInput="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__keyboardKeys() {
  "${@}"
  keyboardKeys="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__keyboardPressedKey() {
  "${@}"
  keyboardPressedKey="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__mouse() {
  "${@}"
  mouse="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___this_requestedCommand() {
  "${@}"
  eval $this"_requestedCommand=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__keyboard() {
  "${@}"
  keyboard="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__keyboardInput() {
  "${@}"
  keyboardInput="${besharp_rcrvs[besharp_rcsl + 1]}"
}
InputDriver () 
{ 
    @parent "${@}"
}
InputDriver.enableMouseCapture () 
{ 
    __be__mouse $this.mouse;
    $mouse.enableMouseMovementCapture
}
InputDriver.isAnyKeyboardKeyPressed () 
{ 
    if @returned @of $this.keyboardPressedKey != ""; then
        besharp_rcrvs[besharp_rcsl]=true;
    else
        besharp_rcrvs[besharp_rcsl]=false;
    fi
}
InputDriver.isKeyboardKeyPressed () 
{ 
    local key="${1}";
    if @returned @of $this.keyboardPressedKey == "${key}"; then
        besharp_rcrvs[besharp_rcsl]=true;
    else
        besharp_rcrvs[besharp_rcsl]=false;
    fi
}
InputDriver.scanInput () 
{ 
    __be__keyboard $this.keyboard;
    __be__keyboardInput $keyboard.scanKeys;
    $this.updateStatus "${keyboardInput}"
}
InputDriver.updateStatus () 
{ 
    local input="${1}";
    if [[ "${input:0:3}" == '[M' ]]; then
        __be__mouse $this.mouse;
        $mouse.updateMouseStatus "${input}" "${this}";
    else
        if [[ -z "${input}" ]]; then
            $this.keyboardPressedKey = "";
            $this.requestedCommand = "";
            return;
        fi;
        __be__keyboardKeys $this.keyboardKeys;
        __be__keyboardPressedKey $keyboardKeys.keyFromInput "${input}";
        __be___this_requestedCommand $keyboardKeys.commandFromKey "${keyboardPressedKey}";
        $this.keyboardPressedKey = "${keyboardPressedKey}";
    fi
}
InputDriver.waitForInput () 
{ 
    __be__keyboard $this.keyboard;
    __be__keyboardInput $keyboard.waitForKeys;
    $this.updateStatus "${keyboardInput}"
}

fi
