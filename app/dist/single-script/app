besharp_oop_type_WidgetsLayout_method_body["drawWidget"]='    local widget="${1}";
    local canvas="${2}";
    __be__monitor $this.monitor;
    if @false $monitor.isWidgetVisible $widget; then
        return;
    fi;
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    if (( width <= 0 )) || (( height <= 0 )) || (( x1 < 0 )) || (( y1 < 0 )); then
        return;
    fi;
    $widget.draw $context $canvas $this'
besharp_oop_type_WidgetsLayout_method_locals["drawWidget"]='local context
local height
local monitor
local widget2Context
local width
local x1
local y1'
