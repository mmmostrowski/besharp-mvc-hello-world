BaseLayout () 
{ 
    @parent "${@}"
}
BaseLayout.createWidget () 
{ 
    @args.make id construct placement placementSize focusIndex focusGroup bindAppState @from "${@}";
    __be__widgetsFactory $this.widgetsFactory;
    __be__idVector @args.requireVector id;
    __be__constructVector @args.requireVector construct;
    __be__widget $constructVector.call $widgetsFactory.create;
    __be__id $idVector.get 0;
    __be__widgetsLayout $this.widgetsLayout;
    $widgetsLayout.addWidget $widget "${id}";
    $widget.initialize $widgetsLayout;
    __be__focusIndex @args.optionalInt focusIndex -1;
    if (( focusIndex >= 0 )); then
        __be__focusGroup @args.optionalText focusGroup "";
        $widgetsLayout.setupWidgetFocusable $widget "${focusIndex}" "${focusGroup}";
    fi;
    __be__placementVector @args.optionalVector placement;
    if [[ -z "${placementVector}" ]]; then
        local placementLeft=0;
        local placementUp=0;
        local placementRight=0;
        local placementDown=0;
    else
        __be__placementLeft $placementVector.get 0;
        __be__placementUp $placementVector.get 1;
        __be__placementRight $placementVector.get 2;
        __be__placementDown $placementVector.get 3;
    fi;
    __be__placementSizeVector @args.optionalVector placementSize;
    if [[ -z "${placementSizeVector}" ]]; then
        local placementWidth=-1;
        local placementHeight=-1;
    else
        __be__placementWidth $placementSizeVector.get 0;
        __be__placementHeight $placementSizeVector.get 1;
    fi;
    $widgetsLayout.setupWidgetPlacement $widget "${placementLeft}" "${placementUp}" "${placementRight}" "${placementDown}" "${placementWidth}" "${placementHeight}";
    __be__bindingVector @args.optionalVector bindAppState;
    if [[ -n "${bindingVector}" ]]; then
        __be__bindingMap @maps.make;
        while @iterate $bindingVector @in entry; do
            local entryArr=(${entry//:/ });
            $bindingMap.set "${entryArr[0]}" "${entryArr[1]}";
        done;
        __be__widgetsState $this.widgetsState;
        $widgetsState.setupWidgetBindings $widget $bindingMap;
    fi;
    besharp_rcrvs[besharp_rcsl]=$widget
}
BaseLayout.prepare () 
{ 
    __be___this_rootWidget $this.prepareRootWidget
}

fi
