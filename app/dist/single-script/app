function __be__currentArgs() {
  "${@}"
  currentArgs="${besharp_rcrvs[besharp_rcsl + 1]}"
}
NamedArgs () 
{ 
    __be___this_currentArgs @maps.make
}
NamedArgs.make () 
{ 
    __be__args @vectors.make "${@}";
    __be__markerIndex $args.findIndex "@from";
    if (( markerIndex < 0 )); then
        besharp.runtime.error "Missing '@from' item!";
    fi;
    $this.reset;
    __be__currentArgs $this.currentArgs;
    local i;
    for ((i = 0; i < markerIndex; ++i ))
    do
        __be__name $args.get "${i}";
        $currentArgs.set "${name}" "";
    done;
    __be__argsCount $args.size;
    for ((i = markerIndex + 1; i < argsCount; ++i ))
    do
        __be__item $args.get "${i}";
        if [[ "${item%:}" != "${item}" ]] && $currentArgs.hasKey "${item%:}"; then
            __be__currentVector @vectors.make;
            $currentArgs.set "${item%:}" $currentVector;
            continue;
        fi;
        $currentVector.add "${item}";
    done;
    @unset $args
}
NamedArgs.optionalInt () 
{ 
    local name="${1}";
    local default="${2}";
    __be__currentArgs $this.currentArgs;
    __be__vector $currentArgs.get "${name}";
    if [[ -z "${vector}" ]] || @true $vector.isEmpty; then
        besharp_rcrvs[besharp_rcsl]="${default}";
        return;
    fi;
    __be__vectorSize $vector.size;
    if (( vectorSize > 1 )); then
        besharp.runtime.error "'${name}' argument is expected to be a single number!";
    fi;
    __be__value $vector.get 0;
    if ! [[ "${value}" =~ ^[0-9]+$ ]]; then
        besharp.runtime.error "'${name}' argument is expected to be a number!";
    fi;
    besharp_rcrvs[besharp_rcsl]="${value}"
}
NamedArgs.optionalText () 
{ 
    local name="${1}";
    local default="${2}";
    __be__currentArgs $this.currentArgs;
    __be__vector $currentArgs.get "${name}";
    if [[ -z "${vector}" ]] || @true $vector.isEmpty; then
        besharp_rcrvs[besharp_rcsl]="${default}";
        return;
    fi;
    __be__vectorSize $vector.size;
    if (( vectorSize > 1 )); then
        besharp.runtime.error "'${name}' argument is expected to be a single item!";
    fi;
    besharp.returningOf $vector.get 0
}
NamedArgs.optionalVector () 
{ 
    local name="${1}";
    __be__currentArgs $this.currentArgs;
    __be__vector $currentArgs.get "${name}";
    if [[ -z "${vector}" ]]; then
        besharp_rcrvs[besharp_rcsl]='';
        return;
    fi;
    besharp.returningOf $currentArgs.get "${name}"
}
NamedArgs.requireVector () 
{ 
    local name="${1}";
    __be__currentArgs $this.currentArgs;
    __be__vector $currentArgs.get "${name}";
    if [[ -z "${vector}" ]]; then
        besharp.runtime.error "Named argument '${name}:' is required!";
    fi;
    besharp_rcrvs[besharp_rcsl]=$vector
}
NamedArgs.reset () 
{ 
    __be__currentArgs $this.currentArgs;
    while @iterate $currentArgs @in vector; do
        if [[ -z "${vector}" ]]; then
            continue;
        fi;
        @unset $vector;
    done;
    $currentArgs.removeAll
}

fi
