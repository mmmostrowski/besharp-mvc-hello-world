besharp_oop_type_FullscreenCanvas_methods['setupTerminalWindow']='setupTerminalWindow'
besharp_oop_type_FullscreenCanvas_method_is_returning["setupTerminalWindow"]=false
besharp_oop_type_FullscreenCanvas_method_body["setupTerminalWindow"]='    local terminalWidth=$( tput cols 2> /dev/null || true);
    local terminalHeight=$( tput lines 2> /dev/null || true );
    terminalWidth="${terminalWidth:-80}";
    terminalHeight="${terminalHeight:-25}";
    __be__minWidth $this.minWidth;
    __be__minHeight $this.minHeight;
    local width="${terminalWidth}";
    if (( terminalWidth < minWidth )); then
        width="${minWidth}";
    fi;
    local height="${terminalHeight}";
    if (( terminalHeight < minHeight )); then
        height="${minHeight}";
    fi;
    $this.width = "${width}";
    $this.w2 = "$(( width * 2 ))";
    $this.height = "${height}";
    $this.setupClearBuffer "${width}" "${height}";
    __be__pixelBuffer $this.pixelBuffer;
    local y=0;
    local body=;
    local cell=0;
    local cellStep=$(( ( width - terminalWidth ) * 2 ));
    while (( ++y <= terminalHeight )); do
        local x=-1;
        local linePrinter='"'"''"'"';
        while (( ++x < terminalWidth )); do
            linePrinter+="\${$pixelBuffer[${cell}]}";
            (( ++cell ));
            linePrinter+="\${$pixelBuffer[${cell}]}";
            (( ++cell ));
        done;
        (( cell += cellStep ));
        body+="echo -en \"\033[${y};1H${linePrinter}\";";
    done;
    eval "function FullscreenCanvas.renderQuick() { ${body} }"'
besharp_oop_type_FullscreenCanvas_method_locals["setupTerminalWindow"]='local minHeight
local minWidth
local pixelBuffer'
besharp_oop_type_FullscreenCanvas_method_is_using_iterators["setupTerminalWindow"]=false
besharp_oop_type_FullscreenCanvas_method_is_calling_parent["setupTerminalWindow"]=false
besharp_oop_type_FullscreenCanvas_method_is_calling_this["setupTerminalWindow"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be__minHeight() {
  "${@}"
  minHeight="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__minWidth() {
  "${@}"
  minWidth="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__pixelBuffer() {
  "${@}"
  pixelBuffer="${besharp_rcrvs[besharp_rcsl + 1]}"
}
FullscreenCanvas () 
{ 
    @parent;
    trap $this.onWindowsSizeUpdate WINCH
}
FullscreenCanvas.beginFrame () 
{ 
    if @true $this.resizeOnNextFrame; then
        $this.resizeOnNextFrame = false;
        $this.clearTerminal;
        $this.setupTerminalWindow;
    fi;
    @parent
}
FullscreenCanvas.initialize () 
{ 
    @parent;
    $this.setupTerminalWindow
}
FullscreenCanvas.onWindowsSizeUpdate () 
{ 
    $this.clearTerminal;
    $this.resizeOnNextFrame = true
}
FullscreenCanvas.render () 
{ 
    FullscreenCanvas.renderQuick
}
FullscreenCanvas.setupMinimalSize () 
{ 
    @parent "${@}";
    $this.setupTerminalWindow
}
FullscreenCanvas.setupTerminalWindow () 
{ 
    local terminalWidth=$( tput cols 2> /dev/null || true);
    local terminalHeight=$( tput lines 2> /dev/null || true );
    terminalWidth="${terminalWidth:-80}";
    terminalHeight="${terminalHeight:-25}";
    __be__minWidth $this.minWidth;
    __be__minHeight $this.minHeight;
    local width="${terminalWidth}";
    if (( terminalWidth < minWidth )); then
        width="${minWidth}";
    fi;
    local height="${terminalHeight}";
    if (( terminalHeight < minHeight )); then
        height="${minHeight}";
    fi;
    $this.width = "${width}";
    $this.w2 = "$(( width * 2 ))";
    $this.height = "${height}";
    $this.setupClearBuffer "${width}" "${height}";
    __be__pixelBuffer $this.pixelBuffer;
    local y=0;
    local body=;
    local cell=0;
    local cellStep=$(( ( width - terminalWidth ) * 2 ));
    while (( ++y <= terminalHeight )); do
        local x=-1;
        local linePrinter='';
        while (( ++x < terminalWidth )); do
            linePrinter+="\${$pixelBuffer[${cell}]}";
            (( ++cell ));
            linePrinter+="\${$pixelBuffer[${cell}]}";
            (( ++cell ));
        done;
        (( cell += cellStep ));
        body+="echo -en \"\033[${y};1H${linePrinter}\";";
    done;
    eval "function FullscreenCanvas.renderQuick() { ${body} }"
}

fi
