if ${beshfile_section__code:-false}; then :; 
function __be__newWidget() {
  "${@}"
  newWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgetsTree() {
  "${@}"
  widgetsTree="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetsFactory () 
{ 
    @parent "${@}"
}
WidgetsFactory.create () 
{ 
    local widgetClassName="${1}";
    shift 1;
    __be__widgetsTree $this.widgetsTree;
    __be__newWidget @new "${widgetClassName}" "${@}";
    $widgetsTree.addWidget $newWidget false;
    besharp_rcrvs[besharp_rcsl]=$newWidget
}

fi
