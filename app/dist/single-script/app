besharp_oop_type_WidgetsCoordinator_method_locals["popFocusWidget"]=''
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["popFocusWidget"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["popFocusWidget"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["popFocusWidget"]=true
besharp_oop_type_WidgetsCoordinator_methods['processInput']='processInput'
besharp_oop_type_WidgetsCoordinator_method_is_returning["processInput"]=true
besharp_oop_type_WidgetsCoordinator_method_body["processInput"]='    local input="${1}";
    __be__focusedWidget $this.focusedWidget;
    if [[ -n "${focusedWidget}" ]]; then
        __be__layout $this.layout;
        __be__context $layout.widgetContext $focusedWidget;
        __be__event @new WidgetInputEvent $input $context;
        __be__tree $this.tree;
        $tree.walkUpTreeUntil $focusedWidget $this.processSingleWidgetInput $focusedWidget $event;
        if @true $event.isConsumed; then
            besharp_rcrvs[besharp_rcsl]=true;
            return;
        fi;
        @unset $event;
    fi;
    $this.processOrphanInputEvent $input;
    besharp_rcrvs[besharp_rcsl]=false'
besharp_oop_type_WidgetsCoordinator_method_locals["processInput"]='local context
local event
local focusedWidget
local layout
local tree'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["processInput"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["processInput"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["processInput"]=true
besharp_oop_type_WidgetsCoordinator_methods['processOrphanInputEvent']='processOrphanInputEvent'
besharp_oop_type_WidgetsCoordinator_method_is_returning["processOrphanInputEvent"]=false
besharp_oop_type_WidgetsCoordinator_method_body["processOrphanInputEvent"]='    local input="${1}";
    __be__command $input.requestedCommand;
    if [[ "${command}" == '"'"'next'"'"' ]]; then
        $this.switchWidgetFocusToNext;
    else
        if [[ "${command}" == '"'"'prev'"'"' ]]; then
            $this.switchWidgetFocusToPrev;
        fi;
    fi'
besharp_oop_type_WidgetsCoordinator_method_locals["processOrphanInputEvent"]='local command'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["processOrphanInputEvent"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["processOrphanInputEvent"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["processOrphanInputEvent"]=true
besharp_oop_type_WidgetsCoordinator_methods['processSingleWidgetInput']='processSingleWidgetInput'
besharp_oop_type_WidgetsCoordinator_method_is_returning["processSingleWidgetInput"]=true
besharp_oop_type_WidgetsCoordinator_method_body["processSingleWidgetInput"]='    local widget="${1}";
    local sourceWidget="${2}";
    local event="${3}";
    if ! @is $widget FocusableWidget; then
        besharp_rcrvs[besharp_rcsl]=true;
        return;
    fi;
    if @same $widget $sourceWidget; then
        $widget.processKeyboardEvent $event true;
    else
        $widget.processKeyboardEvent $event false;
    fi;
    if @true $event.isConsumed; then
        besharp_rcrvs[besharp_rcsl]=false;
    else
        besharp_rcrvs[besharp_rcsl]=true;
    fi'
besharp_oop_type_WidgetsCoordinator_method_locals["processSingleWidgetInput"]=''
