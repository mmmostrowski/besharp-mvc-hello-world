besharp_oop_type_CheckboxWidget_method_body["isChecked"]='    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this '"'"'checked'"'"' false'
besharp_oop_type_CheckboxWidget_method_locals["isChecked"]='local state'
besharp_oop_type_CheckboxWidget_method_is_using_iterators["isChecked"]=false
besharp_oop_type_CheckboxWidget_method_is_calling_parent["isChecked"]=false
