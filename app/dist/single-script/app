besharp_oop_type_WidgetsLayout_method_body["calculateCanvasMinimalSize"]='    local canvas="${1}";
    local widget="${2}";
    __be__calculator @new WidgetMinSizeCalculator;
    $calculator.calculateMinSize $widget;
    __be__minWidth $calculator.width;
    __be__minHeight $calculator.height;
    $canvas.setupMinimalSize "${minWidth}" "${minHeight}";
