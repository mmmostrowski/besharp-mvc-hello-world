besharp_oop_type_RadioWidget_method_body["widgetWidth"]='    __be__text $this.text;
    besharp_rcrvs[besharp_rcsl]=$(( ${#text} + 6 ))'
besharp_oop_type_RadioWidget_method_locals["widgetWidth"]='local text'
besharp_oop_type_RadioWidget_method_is_using_iterators["widgetWidth"]=false
