besharp_oop_type_AbstractCanvas_methods['restoreTerminal']='restoreTerminal'
besharp_oop_type_AbstractCanvas_method_is_returning["restoreTerminal"]=false
besharp_oop_type_AbstractCanvas_method_body["restoreTerminal"]='    __be__cursor $this.cursor;
    $cursor.show'
besharp_oop_type_AbstractCanvas_method_locals["restoreTerminal"]='local cursor'
besharp_oop_type_AbstractCanvas_method_is_using_iterators["restoreTerminal"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["restoreTerminal"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["restoreTerminal"]=true
besharp_oop_type_AbstractCanvas_methods['setupClearBuffer']='setupClearBuffer'
besharp_oop_type_AbstractCanvas_method_is_returning["setupClearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_body["setupClearBuffer"]='    local width="${1}";
    local height="${2}";
    __be__pixelBufferClean $this.pixelBufferClean;
    __be__modeReset @pixel_modes.reset;
    $pixelBufferClean.removeAll;
    local total=$(( width * height));
    local cell=0;
    while (( total-- )); do
        eval "$pixelBufferClean[$cell]=\"\${modeReset}\"";
        (( ++cell ));
        eval "$pixelBufferClean[$cell]='"'"' '"'"'";
        (( ++cell ));
    done'
besharp_oop_type_AbstractCanvas_method_locals["setupClearBuffer"]='local modeReset
local pixelBufferClean'
besharp_oop_type_AbstractCanvas_method_is_using_iterators["setupClearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["setupClearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["setupClearBuffer"]=true
besharp_oop_type_AbstractCanvas_methods['setupMinimalSize']='setupMinimalSize'
besharp_oop_type_AbstractCanvas_method_is_returning["setupMinimalSize"]=false
besharp_oop_type_AbstractCanvas_method_body["setupMinimalSize"]='    $this.minWidth = "${1}";
    $this.minHeight = "${2}"'
besharp_oop_type_AbstractCanvas_method_locals["setupMinimalSize"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["setupMinimalSize"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["setupMinimalSize"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["setupMinimalSize"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be___this_pixelBuffer() {
  "${@}"
  eval $this"_pixelBuffer=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_pixelBufferClean() {
  "${@}"
  eval $this"_pixelBufferClean=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__pixelBuffer() {
  "${@}"
  pixelBuffer="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__pixelBufferClean() {
  "${@}"
  pixelBufferClean="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__cursor() {
  "${@}"
  cursor="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__cursor() {
  "${@}"
  cursor="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__modeReset() {
  "${@}"
  modeReset="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__pixelBufferClean() {
  "${@}"
  pixelBufferClean="${besharp_rcrvs[besharp_rcsl + 1]}"
}
AbstractCanvas () 
{ 
    __be___this_pixelBuffer @new ArrayVector;
    __be___this_pixelBufferClean @new ArrayVector
}
AbstractCanvas.beginFrame () 
{ 
    $this.clearBuffer
}
AbstractCanvas.clearBuffer () 
{ 
    __be__pixelBufferClean $this.pixelBufferClean;
    __be__pixelBuffer $this.pixelBuffer;
    eval "$pixelBuffer=( \"\${$pixelBufferClean[@]}\" )"
}
AbstractCanvas.clearTerminal () 
{ 
    local cursorMode="${1:-cursor-hide}";
    __be__cursor $this.cursor;
    $cursor.clearAndMoveTo0x0;
    if [[ "${cursorMode}" == "cursor-hide" ]]; then
        $cursor.hide;
    else
        $cursor.show;
    fi
}
AbstractCanvas.initialize () 
{ 
    besharp.addShutdownCallback 'canvas_restore_terminal' $this.restoreTerminal;
    $this.clearTerminal
}
AbstractCanvas.putPixel () 
{ 
    local x="${1}";
    local y="${2}";
    local char="${3}";
    shift 3;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
        ";
    eval "$pixelBuffer[\${cell}]=\"\${modes// /}\"";
    eval "$pixelBuffer[\${cell} + 1]=\"\${char}\""
}
AbstractCanvas.putPixelChar () 
{ 
    local x="${1}";
    local y="${2}";
    local char="${3}";
    eval "
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "$pixelBuffer[\$(( ( y * ${this}_width + x ) * 2 + 1 ))]=\"\${char}\""
}
AbstractCanvas.putPixelModes () 
{ 
    local x="${1}";
    local y="${2}";
    shift 2;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "$pixelBuffer[\$(( ( y * ${this}_width + x ) * 2 ))]=\"\${modes// /}\""
}
AbstractCanvas.putPixelsBox () 
{ 
    local x="${1}";
    local y="${2}";
    local width="${3}";
    local height="${4}";
    local char="${5}";
    shift 5;
    eval "
            local modes=\"\${*:-\e[0m}\"
            modes=\"\${modes// /}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    local rest=$(( width % 8 ));
    width=$(( width / 8 ));
    local line=$height;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( line-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes}\"
                   $pixelBuffer[ c + 2 ]=\"\${modes}\"
                   $pixelBuffer[ c + 4 ]=\"\${modes}\"
                   $pixelBuffer[ c + 6 ]=\"\${modes}\"
                   $pixelBuffer[ c + 8 ]=\"\${modes}\"
                   $pixelBuffer[ c + 10 ]=\"\${modes}\"
                   $pixelBuffer[ c + 12 ]=\"\${modes}\"
                   $pixelBuffer[ c + 14 ]=\"\${modes}\"
                   $pixelBuffer[ c + 1 ]=\"\${char}\"
                   $pixelBuffer[ c + 3 ]=\"\${char}\"
                   $pixelBuffer[ c + 5 ]=\"\${char}\"
                   $pixelBuffer[ c + 7 ]=\"\${char}\"
                   $pixelBuffer[ c + 9 ]=\"\${char}\"
                   $pixelBuffer[ c + 11 ]=\"\${char}\"
                   $pixelBuffer[ c + 13 ]=\"\${char}\"
                   $pixelBuffer[ c + 15 ]=\"\${char}\"
                   (( c += 16 ))
                done
                (( cell += ${this}_w2 ))
            done
        ";
    if (( rest == 0 )); then
        return;
    fi;
    (( x += ( width * 8 ) ));
    width=$rest;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( height-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes// /}\"
                   $pixelBuffer[ c + 1 ]=\"\${char}\"
                   (( c += 2 ))
                done
                (( cell += ${this}_w2 ))
            done
        "
}
AbstractCanvas.putPixelsBoxChar () 
{ 
    local x="${1}";
    local y="${2}";
    local width="${3}";
    local height="${4}";
    local char="${5}";
    eval "
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    local rest=$(( width % 8 ));
    width=$(( width / 8 ));
    local line=$height;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 + 1 ))
            while (( line-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c ]=\"\${char}\"
                   $pixelBuffer[ c + 2 ]=\"\${char}\"
                   $pixelBuffer[ c + 4 ]=\"\${char}\"
                   $pixelBuffer[ c + 6 ]=\"\${char}\"
                   $pixelBuffer[ c + 8 ]=\"\${char}\"
                   $pixelBuffer[ c + 10 ]=\"\${char}\"
                   $pixelBuffer[ c + 12 ]=\"\${char}\"
                   $pixelBuffer[ c + 14 ]=\"\${char}\"
                   (( c += 16 ))
                done
                (( cell += ${this}_w2 ))
            done
        ";
    if (( rest == 0 )); then
        return;
    fi;
    (( x += ( width * 8 ) ));
    width=$rest;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 + 1 ))
            while (( height-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c ]=\"\${char}\"
                   (( c += 2 ))
                done
                (( cell += ${this}_w2 ))
            done
        "
}
AbstractCanvas.putPixelsBoxModes () 
{ 
    local x="${1}";
    local y="${2}";
    local width="${3}";
    local height="${4}";
    shift 4;
    eval "
            local modes=\"\${*:-\e[0m}\"
            modes=\"\${modes// /}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    local rest=$(( width % 8 ));
    width=$(( width / 8 ));
    local line=$height;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( line-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes}\"
                   $pixelBuffer[ c + 2 ]=\"\${modes}\"
                   $pixelBuffer[ c + 4 ]=\"\${modes}\"
                   $pixelBuffer[ c + 6 ]=\"\${modes}\"
                   $pixelBuffer[ c + 8 ]=\"\${modes}\"
                   $pixelBuffer[ c + 10 ]=\"\${modes}\"
                   $pixelBuffer[ c + 12 ]=\"\${modes}\"
                   $pixelBuffer[ c + 14 ]=\"\${modes}\"
                   (( c += 16 ))
                done
                (( cell += ${this}_w2 ))
            done
        ";
    if (( rest == 0 )); then
        return;
    fi;
    (( x += ( width * 8 ) ));
    width=$rest;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( height-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes// /}\"
                   (( c += 2 ))
                done
                (( cell += ${this}_w2 ))
            done
        "
}
AbstractCanvas.putPixelsHoriz () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    local char="${4}";
    shift 4;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[cell]=\"\${modes// /}\"
                $pixelBuffer[cell + 1]=\"\${char}\"
                (( cell += 2))
            done
        "
}
AbstractCanvas.putPixelsHorizChar () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    local char="${4}";
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[cell + 1]=\"\${char}\"
                (( cell += 2))
            done
        "
}
AbstractCanvas.putPixelsHorizModes () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    shift 3;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[cell]=\"\${modes// /}\"
                (( cell += 2))
            done
        "
}
AbstractCanvas.putPixelsVert () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    local char="${4}";
    shift 4;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[ cell ]=\"\${modes// /}\"
                $pixelBuffer[ cell + 1 ]=\"\${char}\"
                (( cell += ( ${this}_w2 ) ))
            done
        "
}
AbstractCanvas.putPixelsVertChar () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    local char="${4}";
    eval "
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[ cell + 1 ]=\"\${char}\"
                (( cell += ( ${this}_w2 ) ))
            done
        "
}
AbstractCanvas.putPixelsVertModes () 
{ 
    local x="${1}";
    local y="${2}";
    local num="${3}";
    shift 3;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( num-- > 0 )); do
                $pixelBuffer[ cell ]=\"\${modes// /}\"
                (( cell += ( ${this}_w2 ) ))
            done
        "
}
AbstractCanvas.restoreTerminal () 
{ 
    __be__cursor $this.cursor;
    $cursor.show
}
AbstractCanvas.setupClearBuffer () 
{ 
    local width="${1}";
    local height="${2}";
    __be__pixelBufferClean $this.pixelBufferClean;
    __be__modeReset @pixel_modes.reset;
    $pixelBufferClean.removeAll;
    local total=$(( width * height));
    local cell=0;
    while (( total-- )); do
        eval "$pixelBufferClean[$cell]=\"\${modeReset}\"";
        (( ++cell ));
        eval "$pixelBufferClean[$cell]=' '";
        (( ++cell ));
    done
}
AbstractCanvas.setupMinimalSize () 
{ 
    $this.minWidth = "${1}";
    $this.minHeight = "${2}"
}

fi
