besharp_oop_type_FpsWidget_field_default['fps']="?"
besharp_oop_type_FpsWidget_fields['frameCounter']='frameCounter'
besharp_oop_type_FpsWidget_field_default['frameCounter']="0"
besharp_oop_type_FpsWidget_fields['lastSecond']='lastSecond'
