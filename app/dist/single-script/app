if ${beshfile_section__code:-false}; then :; 
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__canvasPrinter() {
  "${@}"
  canvasPrinter="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__color() {
  "${@}"
  color="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__reset() {
  "${@}"
  reset="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x() {
  "${@}"
  x="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y() {
  "${@}"
  y="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
CheckboxWidget () 
{ 
    @parent "${@}"
}
CheckboxWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
CheckboxWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "${#text}" 1
}
CheckboxWidget.check () 
{ 
    __be__state $this.state;
    $state.writeWidgetState $this 'checked' true
}
CheckboxWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__canvasPrinter $this.canvasPrinter;
    __be__reset @pixel_modes.reset;
    if @true $context.focusedFlag; then
        __be__color @pixel_modes.white;
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" "-[x] ${text}-" "${reset}${color}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" "-[ ] ${text}-" "${reset}${color}";
        fi;
    else
        __be__color @pixel_modes.dGray;
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" " [x] ${text} " "${reset}${color}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" " [ ] ${text} " "${reset}${color}";
        fi;
    fi
}
CheckboxWidget.initialize () 
{ 
    :
}
CheckboxWidget.isChecked () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'checked' false
}
CheckboxWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    if @true $event.consumeWhenCommandRequested 'action'; then
        $this.toggle;
    fi
}
CheckboxWidget.text () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'text' ''
}
CheckboxWidget.toggle () 
{ 
    if @true $this.isChecked; then
        $this.uncheck;
    else
        $this.check;
    fi
}
CheckboxWidget.uncheck () 
{ 
    __be__state $this.state;
    $state.writeWidgetState $this 'checked' false
}
CheckboxWidget.widgetHeight () 
{ 
    besharp_rcrvs[besharp_rcsl]=1
}
CheckboxWidget.widgetWidth () 
{ 
    __be__text $this.text;
    besharp_rcrvs[besharp_rcsl]=$(( ${#text} + 6 ))
}

fi
