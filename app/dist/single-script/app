WidgetsHelper () 
{ 
    @parent "${@}"
}
WidgetsHelper.debug () 
{ 
    ( local widget="${1}";
    local label="${2:-}";
    __be__layout $this.layout;
    __be__context $layout.widgetContext $widget;
    __be__widgetId $layout.widgetId $widget;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    __be__focusedFlag $context.focusedFlag;
    __be__highlightedFlag $context.highlightedFlag;
    __be__details @maps.make "class" "$( besharp.rtti.objectType $widget )" "id" $widgetId "x1" $x1 "y1" $y1 "width" $width "height" $height "focusedFlag" $focusedFlag "highlightedFlag" $highlightedFlag;
    if [[ -n "${label}" ]]; then
        d "${label}" $details;
    else
        d $details;
    fi;
    @unset $details;
    t )
}
WidgetsHelper.info () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    __be__context $layout.widgetContext $widget;
    __be__widgetId $layout.widgetId $widget;
    besharp_rcrvs[besharp_rcsl]="Widget '${widgetId}' [ '${widget}' object of type '$( besharp.rtti.objectType $widget )' ]"
}

