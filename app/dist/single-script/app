besharp_oop_type_CanvasPrinter_method_is_calling_this["printSparseText"]=false
besharp_oop_type_CanvasPrinter_methods['printText']='printText'
besharp_oop_type_CanvasPrinter_method_is_returning["printText"]=false
besharp_oop_type_CanvasPrinter_method_body["printText"]='    local canvas="${1}";
    local x="${2}";
    local y="${3}";
    local text="${4}";
    shift 4;
    __be__bgBlack @pixel_modes.bgBlack;
    local i;
    for ((i=0; i < ${#text}; i++ ))
    do
        $canvas.putPixel "$(( x++ ))" "${y}" "${text:$i:1}" "${bgBlack}" "${@}";
    done'
besharp_oop_type_CanvasPrinter_method_locals["printText"]='local bgBlack'
