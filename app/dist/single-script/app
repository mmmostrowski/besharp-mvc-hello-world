    __be__key2focusWidget $this.key2focusWidget;
    __be__focusWidget $key2focusWidget.get "${layerKey}";
    $coordinator.switchFocusWidget $focusWidget'
besharp_oop_type_LayersWidget_method_locals["switchLayerFocusIndex"]='local focusWidget
local key2focusWidget'
besharp_oop_type_LayersWidget_method_is_using_iterators["switchLayerFocusIndex"]=false
besharp_oop_type_LayersWidget_method_is_calling_parent["switchLayerFocusIndex"]=false
besharp_oop_type_LayersWidget_method_is_calling_this["switchLayerFocusIndex"]=true
besharp_oop_type_LayersWidget_methods['turnOffLayer']='turnOffLayer'
besharp_oop_type_LayersWidget_method_is_returning["turnOffLayer"]=false
besharp_oop_type_LayersWidget_method_body["turnOffLayer"]='    local layerKey="${1}";
    $layers.hideLayer "${layerKey}";
    $layers.deactivateLayer "${layerKey}"'
besharp_oop_type_LayersWidget_method_locals["turnOffLayer"]=''
besharp_oop_type_LayersWidget_method_is_using_iterators["turnOffLayer"]=false
