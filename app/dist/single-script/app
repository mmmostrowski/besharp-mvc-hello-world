besharp_oop_type_Cursor_methods['hide']='hide'
besharp_oop_type_Cursor_method_is_returning["hide"]=false
besharp_oop_type_Cursor_method_body["hide"]='    echo -e "\e[?25l";
    stty -echo > /dev/null 2> /dev/null || true'
besharp_oop_type_Cursor_method_locals["hide"]=''
besharp_oop_type_Cursor_method_is_using_iterators["hide"]=false
besharp_oop_type_Cursor_method_is_calling_parent["hide"]=false
besharp_oop_type_Cursor_method_is_calling_this["hide"]=false
besharp_oop_type_Cursor_methods['moveTo']='moveTo'
besharp_oop_type_Cursor_method_is_returning["moveTo"]=false
besharp_oop_type_Cursor_method_body["moveTo"]='    local x="${1}";
    local y="${2}";
    echo -en "\033[$(( y + 1 ));$((x + 1 ))H"'
besharp_oop_type_Cursor_method_locals["moveTo"]=''
besharp_oop_type_Cursor_method_is_using_iterators["moveTo"]=false
besharp_oop_type_Cursor_method_is_calling_parent["moveTo"]=false
besharp_oop_type_Cursor_method_is_calling_this["moveTo"]=false
besharp_oop_type_Cursor_methods['show']='show'
besharp_oop_type_Cursor_method_is_returning["show"]=false
besharp_oop_type_Cursor_method_body["show"]='    stty echo > /dev/null 2> /dev/null || true;
    echo -e "\e[?25h";
    tput smam'
besharp_oop_type_Cursor_method_locals["show"]=''
besharp_oop_type_Cursor_method_is_using_iterators["show"]=false
besharp_oop_type_Cursor_method_is_calling_parent["show"]=false
besharp_oop_type_Cursor_method_is_calling_this["show"]=false

fi
if ${beshfile_section__code:-false}; then :; 
Cursor () 
{ 
    @parent "${@}"
}
Cursor.clearAndMoveTo0x0 () 
{ 
    echo -e "\033[2J";
    $this.moveTo 0 0;
    tput rmam
}
Cursor.hide () 
{ 
    echo -e "\e[?25l";
    stty -echo > /dev/null 2> /dev/null || true
}
Cursor.moveTo () 
{ 
    local x="${1}";
    local y="${2}";
    echo -en "\033[$(( y + 1 ));$((x + 1 ))H"
}
Cursor.show () 
{ 
    stty echo > /dev/null 2> /dev/null || true;
    echo -e "\e[?25h";
    tput smam
}

fi
