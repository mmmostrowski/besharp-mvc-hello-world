WidgetsCoordinator () 
{ 
    __be___this_activeWidgets @sets.make;
    __be___this_visibleWidgets @sets.make;
    __be___this_focusIndicesStack @collections.makeStack
}
WidgetsCoordinator.activateSingleWidget () 
{ 
    local widget="${1}";
    local active="${2:-true}";
    __be__activeWidgets $this.activeWidgets;
    if $active; then
        $activeWidgets.add $widget;
    else
        $activeWidgets.remove $widget;
    fi
}
WidgetsCoordinator.activateWidget () 
{ 
    local widget="${1}";
    local active="${2:-true}";
    __be__tree $this.tree;
    $tree.walkDownTreeOperatives $widget $this.activateSingleWidget "${active}"
}
WidgetsCoordinator.deactivateWidget () 
{ 
    local widget="${1}";
    __be__tree $this.tree;
    $tree.walkDownTree $widget $this.activateSingleWidget "false"
}
WidgetsCoordinator.focusWidget () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    if @returned @of $this.focusedWidget != ''; then
        __be__focusedWidget $this.focusedWidget;
        __be__context $layout.widgetContext $focusedWidget;
        $context.focusedFlag = false;
    fi;
    __be__context $layout.widgetContext $widget;
    $context.focusedFlag = true;
    $this.focusedWidget = $widget;
    __be___this_currentSortIndex $layout.widgetSortIndex $widget
}
WidgetsCoordinator.highlightSingleWidget () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    __be__context $layout.widgetContext $widget;
    $context.turnHighlightOn
}
WidgetsCoordinator.highlightWidget () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    while @iterate @of $layout.widget2Context @in context; do
        $context.turnHighlightOff;
    done;
    __be__tree $this.tree;
    $tree.walkUpTree $widget $this.highlightSingleWidget
}
WidgetsCoordinator.initializeSingleWidget () 
{ 
    local widget="${1}";
    $widget.initialize
}
WidgetsCoordinator.initializeWidgets () 
{ 
    local widget="${1}";
    __be__tree $this.tree;
    $tree.walkDownTree $widget $this.initializeSingleWidget
}
WidgetsCoordinator.isWidgetActive () 
{ 
    local widget="${1}";
    __be__activeWidgets $this.activeWidgets;
    besharp.returningOf $activeWidgets.has "${widget}"
}
WidgetsCoordinator.isWidgetVisible () 
{ 
    local widget="${1}";
    __be__visibleWidgets $this.visibleWidgets;
    besharp.returningOf $visibleWidgets.has "${widget}"
}
WidgetsCoordinator.makeSingleWidgetVisible () 
{ 
    local widget="${1}";
    local visible="${2:-true}";
    __be__visibleWidgets $this.visibleWidgets;
    if $visible; then
        $visibleWidgets.add $widget;
    else
        $visibleWidgets.remove $widget;
    fi;
    if @is $widget AnimatedWidget; then
        __be__animationController $this.animationController;
        if $visible; then
            $animationController.activateWidgetAnimation $widget;
        else
            $animationController.deactivateWidgetAnimation $widget;
        fi;
    fi
}
WidgetsCoordinator.makeWidgetInvisible () 
{ 
    local widget="${1}";
    __be__tree $this.tree;
    $tree.walkDownTree $widget $this.makeSingleWidgetVisible "false"
}
WidgetsCoordinator.makeWidgetVisible () 
{ 
    local widget="${1}";
    __be__tree $this.tree;
    $tree.walkDownTreeOperatives $widget $this.makeSingleWidgetVisible "true"
}
WidgetsCoordinator.popFocusIndex () 
{ 
    __be__focusIndicesStack $this.focusIndicesStack;
    __be__sortIndex $focusIndicesStack.pull;
    $this.switchToFocusIndex "${sortIndex}";
    besharp_rcrvs[besharp_rcsl]="${sortIndex}"
}
WidgetsCoordinator.popFocusWidget () 
{ 
    $this.popFocusIndex
}
WidgetsCoordinator.processInput () 
{ 
    local input="${1}";
    __be__focusedWidget $this.focusedWidget;
    if [[ -n "${focusedWidget}" ]]; then
        __be__layout $this.layout;
        __be__context $layout.widgetContext $focusedWidget;
        __be__event @new WidgetInputEvent $input $context;
        __be__tree $this.tree;
        $tree.walkUpTreeUntil $focusedWidget $this.processSingleWidgetInput $focusedWidget $event;
        if @true $event.isConsumed; then
            besharp_rcrvs[besharp_rcsl]=true;
            return;
        fi;
        @unset $event;
    fi;
    $this.processOrphanInputEvent $input;
    besharp_rcrvs[besharp_rcsl]=false
}
WidgetsCoordinator.processOrphanInputEvent () 
{ 
    local input="${1}";
    __be__command $input.requestedCommand;
    if [[ "${command}" == 'next' ]]; then
        $this.switchWidgetFocusToNext;
    else
        if [[ "${command}" == 'prev' ]]; then
            $this.switchWidgetFocusToPrev;
        fi;
    fi
}
WidgetsCoordinator.processSingleWidgetInput () 
{ 
    local widget="${1}";
    local sourceWidget="${2}";
    local event="${3}";
    if ! @is $widget FocusableWidget; then
        besharp_rcrvs[besharp_rcsl]=true;
        return;
    fi;
    if @same $widget $sourceWidget; then
        $widget.processKeyboardEvent $event true;
    else
        $widget.processKeyboardEvent $event false;
    fi;
    if @true $event.isConsumed; then
        besharp_rcrvs[besharp_rcsl]=false;
    else
        besharp_rcrvs[besharp_rcsl]=true;
    fi
}
WidgetsCoordinator.pushFocusIndex () 
{ 
    local sortIndex="${1}";
    __be__currentSortIndex $this.currentSortIndex;
    __be__focusIndicesStack $this.focusIndicesStack;
    $focusIndicesStack.add "${currentSortIndex}";
    $this.switchToFocusIndex "${sortIndex}"
}
WidgetsCoordinator.pushFocusWidget () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    __be__focusIndex $layout.widgetSortIndex $widget;
    $this.pushFocusIndex "${focusIndex}"
}
WidgetsCoordinator.switchFocusWidget () 
{ 
    local widget="${1}";
    __be__layout $this.layout;
    __be__focusIndex $layout.widgetSortIndex $widget;
    $this.switchToFocusIndex "${focusIndex}"
}
WidgetsCoordinator.switchToFocusIndex () 
{ 
    local sortIndex="${1}";
    __be__layout $this.layout;
    $this.currentSortIndex = "${sortIndex}";
    while @iterate @of $this.activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == sortIndex )); then
            $this.focusWidget $widget;
            $this.highlightWidget $widget;
            return;
        fi;
    done
}
WidgetsCoordinator.switchWidgetFocusToNext () 
{ 
    __be__currentSortIndex $this.currentSortIndex;
    __be__activeWidgets $this.activeWidgets;
    __be__layout $this.layout;
    local firstFocusable=;
    local lowestFoundSortIndex=100000000000000;
    local closestGreaterIndex=100000000000000;
    local nextWidget=;
    while @iterate $activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == -1 )); then
            continue;
        fi;
        if (( widgetSortIndex < lowestFoundSortIndex )); then
            lowestFoundSortIndex="${widgetSortIndex}";
            firstFocusable=$widget;
        fi;
        if (( widgetSortIndex > currentSortIndex )) && (( widgetSortIndex < closestGreaterIndex )); then
            closestGreaterIndex="${widgetSortIndex}";
            nextWidget=$widget;
        fi;
    done;
    if [[ -n $nextWidget ]]; then
        $this.focusWidget $nextWidget;
        $this.highlightWidget $nextWidget;
    else
        if [[ -n $firstFocusable ]]; then
            $this.focusWidget $firstFocusable;
            $this.highlightWidget $firstFocusable;
        fi;
    fi
}
WidgetsCoordinator.switchWidgetFocusToPrev () 
{ 
    __be__currentSortIndex $this.currentSortIndex;
    __be__activeWidgets $this.activeWidgets;
    __be__layout $this.layout;
    local lastFocusable=;
    local highestFoundSortIndex=-1;
    local closestLowerIndex=-1;
    local prevWidget=;
    while @iterate $activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == -1 )); then
            continue;
        fi;
        if (( widgetSortIndex > highestFoundSortIndex )); then
            highestFoundSortIndex="${widgetSortIndex}";
            lastFocusable=$widget;
        fi;
        if (( widgetSortIndex < currentSortIndex )) && (( widgetSortIndex > closestLowerIndex )); then
            closestLowerIndex="${widgetSortIndex}";
            prevWidget=$widget;
        fi;
    done;
    if [[ -n $prevWidget ]]; then
        $this.focusWidget $prevWidget;
        $this.highlightWidget $prevWidget;
    else
        if [[ -n $lastFocusable ]]; then
            $this.focusWidget $lastFocusable;
            $this.highlightWidget $lastFocusable;
        fi;
    fi
}

fi
