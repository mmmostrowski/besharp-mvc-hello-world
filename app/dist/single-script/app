besharp_oop_type_VerticalListWidget_method_is_returning["VerticalListWidget"]=false
besharp_oop_type_VerticalListWidget_method_body["VerticalListWidget"]='    @parent "up" "down"'
besharp_oop_type_VerticalListWidget_method_locals["VerticalListWidget"]=''
besharp_oop_type_VerticalListWidget_method_is_using_iterators["VerticalListWidget"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_parent["VerticalListWidget"]=true
besharp_oop_type_VerticalListWidget_method_is_calling_this["VerticalListWidget"]=false
besharp_oop_type_VerticalListWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_VerticalListWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_VerticalListWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    $calculator.setupSize 22 13'
besharp_oop_type_VerticalListWidget_method_locals["calculateMinSize"]=''
besharp_oop_type_VerticalListWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_this["calculateMinSize"]=false
besharp_oop_type_VerticalListWidget_methods['draw']='draw'
besharp_oop_type_VerticalListWidget_method_is_returning["draw"]=false
besharp_oop_type_VerticalListWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__canvasPrinter $this.canvasPrinter;
    __be__height $context.height;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__buttonStyle @pixel_modes.bgDGray;
    __be__selButtonStyle @pixel_modes.bgLGray;
    __be__color1 @pixel_modes.lYellow;
    __be__color2 @pixel_modes.dGray;
    __be__dotsColor @pixel_modes.dGray;
    local style="${buttonStyle}${color1}";
    local selectedStyle="${selButtonStyle}${color2}";
    __be__currentItemIdx $this.currentItemIdx;
    __be__isFocused $context.focusedFlag;
    __be__slideAnimationFrame $this.slideAnimationFrame;
    __be__slideAnimationData $this.slideAnimationData;
    __be__slide $slideAnimationData.get "${slideAnimationFrame}";
    $this.currentSlide = "${slide}";
    local moreIndicatorPlacement="0";
    local y="${y1}";
    (( height -= 3 ));
    local toSlideDown=0;
    local toSlide=0;
    local idx=-1;
    local showDots=false;
    local itemSize="3";
    __be__itemsList $this.itemsList;
    __be__itemsListSize $itemsList.size;
    local itemsListIdx=0;
    while (( itemsListIdx < itemsListSize )); do
        __be__item $itemsList.get "$(( itemsListIdx++ ))";
        if (( ++idx == currentItemIdx )); then
            local isCurrentItem=true;
        else
            local isCurrentItem=false;
        fi;
        if (( y - slide > y1 + height )); then
            showDots=true;
            if $isCurrentItem && (( slideAnimationFrame == 0 )); then
                $this.slideAnimationTo "${toSlide}";
                $widgetsLayout.needsRedraw = true;
            fi;
            (( y+= itemSize ));
            continue;
        fi;
        if (( y - slide < y1 )); then
            if $isCurrentItem && (( slideAnimationFrame == 0 )); then
                $this.slideAnimationTo "${toSlide}";
                $widgetsLayout.needsRedraw = true;
            fi;
        else
            if $isCurrentItem; then
                if $isFocused; then
                    $canvasPrinter.printText $canvas "${x1}" "$(( y - slide ))" " -${item}- " "${selectedStyle}";
                else
                    $canvasPrinter.printText $canvas "${x1}" "$(( y - slide ))" "  ${item}  " "${selectedStyle}";
                fi;
            else
                $canvasPrinter.printText $canvas "${x1}" "$(( y - slide ))" "  ${item}  " "${style}";
            fi;
            moreIndicatorPlacement="$(( y - slide + 2 ))";
        fi;
        (( toSlide += itemSize ));
        (( y+= itemSize ));
    done;
    if $showDots; then
        $canvasPrinter.printText $canvas "${x1}" "${moreIndicatorPlacement}" "..." "${dotsColor}";
    fi'
besharp_oop_type_VerticalListWidget_method_locals["draw"]='local buttonStyle
local canvasPrinter
local color1
local color2
local currentItemIdx
local dotsColor
local height
local isFocused
local item
local itemsList
local itemsListSize
local selButtonStyle
local slide
local slideAnimationData
local slideAnimationFrame
local x1
local y1'
besharp_oop_type_VerticalListWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_this["draw"]=true
besharp_oop_type_VerticalListWidget_methods['processLayoutChanges']='processLayoutChanges'
besharp_oop_type_VerticalListWidget_method_is_returning["processLayoutChanges"]=true
besharp_oop_type_VerticalListWidget_method_body["processLayoutChanges"]='    local context="${1}";
    local layout="${2}";
    local itemSize="3";
    __be__y1 $context.y1;
    __be__height $context.height;
    (( height -= 4 )) || true;
    __be__itemsList $this.itemsList;
    __be__itemsListSize $itemsList.size;
    local totalSlide="$(( itemsListSize * itemSize ))";
    local maxSlide="$(( totalSlide - height - 1 ))";
    if (( maxSlide < 0 )); then
        $this.maxSlide = 0;
        return;
    fi;
    (( maxSlide /= itemSize )) || true;
    (( maxSlide *= itemSize )) || true;
    $this.maxSlide = "${maxSlide}";
    __be__currentSlide $this.currentSlide;
    if (( currentSlide > maxSlide )); then
        $this.slideAnimationTo "${maxSlide}";
    fi'
besharp_oop_type_VerticalListWidget_method_locals["processLayoutChanges"]='local currentSlide
local height
local itemsList
local itemsListSize
local y1'
besharp_oop_type_VerticalListWidget_method_is_using_iterators["processLayoutChanges"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_parent["processLayoutChanges"]=false
besharp_oop_type_VerticalListWidget_method_is_calling_this["processLayoutChanges"]=true

fi
