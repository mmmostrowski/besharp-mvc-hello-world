    shift 1;
    local allPrimitiveSets=true;
    for iterable in "${@}";
    do
        if ! @is $iterable @a Set || ! @is $iterable @a PrimitiveObject; then
            allPrimitiveSets=false;
            break;
        fi;
    done;
    if $allPrimitiveSets; then
        local targetItem;
        local iterable;
