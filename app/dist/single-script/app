    __be__valueKey $this.valueKey;
    $state.writeWidgetState $this '"'"'value'"'"' "${valueKey}"'
besharp_oop_type_RadioWidget_method_locals["check"]='local state
local valueKey'
besharp_oop_type_RadioWidget_method_is_using_iterators["check"]=false
besharp_oop_type_RadioWidget_method_is_calling_parent["check"]=false
besharp_oop_type_RadioWidget_method_is_calling_this["check"]=true
besharp_oop_type_RadioWidget_methods['draw']='draw'
