  "${@}"
  greeting="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subject() {
  "${@}"
  subject="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__suffix() {
  "${@}"
  suffix="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
HelloWorldWidget () 
{ 
    @parent "${@}"
}
HelloWorldWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
HelloWorldWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "${#text}" 1
}
HelloWorldWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__canvasPrinter $this.canvasPrinter;
    __be__state $this.state;
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    local format='';
    if @true $this.isBoldEnabled; then
        __be__format $this.boldFormat;
    fi;
    if @true $this.isColorsEnabled; then
        local i;
        for ((i=0; i < ${#text}; ++i ))
        do
            __be__color @pixel_modes.randomColor;
            $canvas.putPixel "$(( x++ ))" "${y}" "${text:$i:1}" "${format}${color}";
        done;
    else
        __be__color @pixel_modes.white;
        $canvasPrinter.printText $canvas "${x}" "${y}" "${text}" "${format}${color}";
    fi
}
HelloWorldWidget.initialize () 
{ 
    __be___this_boldFormat @pixel_modes.bold
}
HelloWorldWidget.isBoldEnabled () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'boldEnabled' 'false'
}
HelloWorldWidget.isColorsEnabled () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'colorsEnabled' 'false'
}
HelloWorldWidget.text () 
{ 
    __be__state $this.state;
    __be__greeting $state.readWidgetState $this 'greeting' '';
    __be__suffix $state.readWidgetState $this 'suffix' '';
    __be__subject $state.readWidgetState $this 'subject' '';
    besharp_rcrvs[besharp_rcsl]="${greeting} ${subject} ${suffix}"
}
HelloWorldWidget.widgetHeight () 
{ 
    besharp_rcrvs[besharp_rcsl]=1
}
HelloWorldWidget.widgetWidth () 
{ 
    __be__text $this.text;
    besharp_rcrvs[besharp_rcsl]="${#text}"
}

fi
