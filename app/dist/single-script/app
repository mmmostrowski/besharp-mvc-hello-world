  "${@}"
  length="${besharp_rcrvs[besharp_rcsl + 1]}"
}
InputWidget () 
{ 
    $this.length = "${1}";
    local emptyText='';
    local i;
    for ((i = 0; i < ${1}; ++i ))
    do
        emptyText+=" ";
    done;
    $this.emptyText = "${emptyText}"
}
InputWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
InputWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__width $this.widgetWidth;
    $calculator.setupSize "${width}" 1
}
InputWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__length $this.length;
    __be__label $this.label;
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__cursor $this.cursor;
    __be__emptyText $this.emptyText;
    __be__canvasPrinter $this.canvasPrinter;
    if @true $context.focusedFlag; then
        __be__color @pixel_modes.white;
        $canvasPrinter.printText $canvas "${x}" "${y}" " ${label}:-| ${emptyText} |-" "${color}";
    else
        __be__color @pixel_modes.dGray;
        $canvasPrinter.printText $canvas "${x}" "${y}" " ${label}: | ${emptyText} |" "${color}";
    fi;
    local caretPos="$(( x + ${#label} + 5 ))";
    $canvasPrinter.printText $canvas "${caretPos}" "${y}" "${text}" "${color}";
    if @true $this.isEditing; then
        $cursor.show;
        local input="${text}";
        local previousIFS="${IFS}";
        IFS='';
        while true; do
            $cursor.moveTo "${caretPos}" $(( y - 1 ));
            read -r -e -i "${input}" -n $(( length + 1 )) input;
            if (( "${#input}" <= length )); then
                break;
            fi;
            input="${input::-1}";
            $cursor.moveTo "$(( caretPos + length ))" $(( y - 1 ));
            echo -ne ' ';
        done;
        IFS="${previousIFS}";
        $this.updateText "${input}";
        $cursor.hide;
        $this.isEditing = false;
        $widgetsLayout.needsRedraw = true;
    fi
}
InputWidget.initialize () 
{ 
    :
}
InputWidget.label () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'label' ''
}
InputWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    __be__input $event.input;
    if @true $event.consumeWhenCommandRequested 'action'; then
        $this.isEditing = true;
    fi
}
InputWidget.text () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'text' ''
}
InputWidget.updateText () 
{ 
    local text="${1}";
    __be__state $this.state;
    $state.writeWidgetState $this 'text' "${text}"
}
InputWidget.widgetHeight () 
{ 
    besharp_rcrvs[besharp_rcsl]=1
}
InputWidget.widgetWidth () 
{ 
    __be__length $this.length;
    __be__label $this.label;
    besharp_rcrvs[besharp_rcsl]="$(( length + ${#label} + 4 ))"
}

fi
