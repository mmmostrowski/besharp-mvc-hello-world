besharp_oop_type_WidgetAnimationController_field_default['activeAnimatedWidgets']=""
besharp_oop_type_WidgetAnimationController_fields['cloneOfActiveWidgets']='cloneOfActiveWidgets'
besharp_oop_type_WidgetAnimationController_field_type['cloneOfActiveWidgets']="Vector"
besharp_oop_type_WidgetAnimationController_field_default['cloneOfActiveWidgets']=""
besharp_oop_type_WidgetAnimationController_methods['WidgetAnimationController']='WidgetAnimationController'
besharp_oop_type_WidgetAnimationController_method_is_returning["WidgetAnimationController"]=false
besharp_oop_type_WidgetAnimationController_method_body["WidgetAnimationController"]='    __be___this_activeAnimatedWidgets @vectors.make;
    __be___this_cloneOfActiveWidgets @vectors.make'
besharp_oop_type_WidgetAnimationController_method_locals["WidgetAnimationController"]=''
besharp_oop_type_WidgetAnimationController_method_is_using_iterators["WidgetAnimationController"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_parent["WidgetAnimationController"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_this["WidgetAnimationController"]=true
besharp_oop_type_WidgetAnimationController_methods['activateWidgetAnimation']='activateWidgetAnimation'
besharp_oop_type_WidgetAnimationController_method_is_returning["activateWidgetAnimation"]=true
besharp_oop_type_WidgetAnimationController_method_body["activateWidgetAnimation"]='    local widget="${1}";
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    if @true $activeAnimatedWidgets.has $widget; then
        return;
    fi;
    $activeAnimatedWidgets.add $widget'
besharp_oop_type_WidgetAnimationController_method_locals["activateWidgetAnimation"]='local activeAnimatedWidgets'
besharp_oop_type_WidgetAnimationController_method_is_using_iterators["activateWidgetAnimation"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_parent["activateWidgetAnimation"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_this["activateWidgetAnimation"]=true
besharp_oop_type_WidgetAnimationController_methods['deactivateWidgetAnimation']='deactivateWidgetAnimation'
besharp_oop_type_WidgetAnimationController_method_is_returning["deactivateWidgetAnimation"]=false
besharp_oop_type_WidgetAnimationController_method_body["deactivateWidgetAnimation"]='    local widget="${1}";
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    $activeAnimatedWidgets.remove $widget'
besharp_oop_type_WidgetAnimationController_method_locals["deactivateWidgetAnimation"]='local activeAnimatedWidgets'
besharp_oop_type_WidgetAnimationController_method_is_using_iterators["deactivateWidgetAnimation"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_parent["deactivateWidgetAnimation"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_this["deactivateWidgetAnimation"]=true
besharp_oop_type_WidgetAnimationController_methods['processAnimationStep']='processAnimationStep'
besharp_oop_type_WidgetAnimationController_method_is_returning["processAnimationStep"]=true
besharp_oop_type_WidgetAnimationController_method_body["processAnimationStep"]='    __be__widgetsLayout $this.widgetsLayout;
    $this.isAnimationActive = false;
    __be__activeAnimatedWidgets $this.activeAnimatedWidgets;
    if @true $activeAnimatedWidgets.isEmpty; then
        return;
    fi;
    __be__cloneOfActiveWidgets $this.cloneOfActiveWidgets;
    @collections.cloneTo $cloneOfActiveWidgets $activeAnimatedWidgets;
    __be__count $cloneOfActiveWidgets.size;
    while (( --count >= 0 )); do
        __be__widget $cloneOfActiveWidgets.get "${count}";
        __be__widgetContext $widgetsLayout.widgetContext $widget;
        if @is $widget StoppableAnimationWidget; then
            __be__appState $this.appState;
            if @false $appState.isAnimationEnabled; then
                continue;
            fi;
        fi;
        __be__animationFramesNum $widget.processAnimationFrame $widgetContext $widgetsLayout;
        if (( animationFramesNum > 0 )); then
            $this.isAnimationActive = true;
        fi;
    done'
besharp_oop_type_WidgetAnimationController_method_locals["processAnimationStep"]='local activeAnimatedWidgets
local animationFramesNum
local appState
local cloneOfActiveWidgets
local count
local widget
local widgetContext
local widgetsLayout'
besharp_oop_type_WidgetAnimationController_method_is_using_iterators["processAnimationStep"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_parent["processAnimationStep"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_this["processAnimationStep"]=true
besharp_oop_type_WidgetAnimationController_methods['registerAnimatedWidget']='registerAnimatedWidget'
besharp_oop_type_WidgetAnimationController_method_is_returning["registerAnimatedWidget"]=false
besharp_oop_type_WidgetAnimationController_method_body["registerAnimatedWidget"]='    local widget="${1}";
    __be__widgetsLayout $this.widgetsLayout;
    __be__widgetContext $widgetsLayout.widgetContext $widget;
    $widget.initializeAnimation $widgetContext $widgetsLayout'
besharp_oop_type_WidgetAnimationController_method_locals["registerAnimatedWidget"]='local widgetContext
local widgetsLayout'
besharp_oop_type_WidgetAnimationController_method_is_using_iterators["registerAnimatedWidget"]=false
besharp_oop_type_WidgetAnimationController_method_is_calling_parent["registerAnimatedWidget"]=false
