besharp_oop_type_RadioWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__canvasPrinter $this.canvasPrinter;
    if @true $context.focusedFlag; then
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" "-(*) ${text}-";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" "-( ) ${text}-";
        fi;
    else
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" " (*) ${text} ";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" " ( ) ${text} ";
        fi;
    fi'
besharp_oop_type_RadioWidget_method_locals["draw"]='local canvasPrinter
local text
local x
local y'
