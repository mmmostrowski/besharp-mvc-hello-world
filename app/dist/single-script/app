if ${beshfile_section__code:-false}; then :; 
function __be__color() {
  "${@}"
  color="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__color() {
  "${@}"
  color="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__bgBlack() {
  "${@}"
  bgBlack="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__bgBlack() {
  "${@}"
  bgBlack="${besharp_rcrvs[besharp_rcsl + 1]}"
}
CanvasPrinter () 
{ 
    @parent "${@}"
}
CanvasPrinter.printFrame () 
{ 
    local canvas="${1}";
    local x1="${2}";
    local y1="${3}";
    local width="${4}";
    local height="${5}";
    shift 5;
    __be__color @pixel_modes.dGray;
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" "${y1}" "$(( width - 2 ))" "-" "${@}" "${color}";
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" $(( y1 + height - 1)) "$(( width - 2 ))" "-" "${@}" "${color}";
    $canvas.putPixelsVert "${x1}" "$(( y1 + 1 ))" "$(( height - 2 ))" "|" "${@}" "${color}";
    $canvas.putPixelsVert $(( x1 + width - 1 )) "$(( y1 + 1 ))" "$(( height - 2 ))" "|" "${@}" "${color}"
}
CanvasPrinter.printHighlightFrame () 
{ 
    local canvas="${1}";
    local x1="${2}";
    local y1="${3}";
    local width="${4}";
    local height="${5}";
    shift 5;
    __be__color @pixel_modes.white;
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" "${y1}" "$(( width - 2 ))" "=" "${@}" "${color}";
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" $(( y1 + height - 1)) "$(( width - 2 ))" "=" "${@}" "${color}";
    $canvas.putPixelsVert "${x1}" "$(( y1 + 1 ))" "$(( height - 2 ))" "[" "${@}" "${color}";
    $canvas.putPixelsVert $(( x1 + width - 1 )) "$(( y1 + 1 ))" "$(( height - 2 ))" "]" "${@}" "${color}"
}
CanvasPrinter.printSparseText () 
{ 
    local canvas="${1}";
    local x="${2}";
    local y="${3}";
    local text="${4}";
    shift 4;
    __be__bgBlack @pixel_modes.bgBlack;
    local i;
    local c=;
    for ((i=0; i < ${#text}; i++ ))
    do
        c="${text:$i:1}";
        if [[ "${c}" == ' ' ]]; then
            (( x++ ));
            continue;
        fi;
        $canvas.putPixel "$(( x++ ))" "${y}" "${c}" "${bgBlack}" "${@}";
    done
}
CanvasPrinter.printText () 
{ 
    local canvas="${1}";
    local x="${2}";
    local y="${3}";
    local text="${4}";
    shift 4;
    __be__bgBlack @pixel_modes.bgBlack;
    local i;
    for ((i=0; i < ${#text}; i++ ))
    do
        $canvas.putPixel "$(( x++ ))" "${y}" "${text:$i:1}" "${bgBlack}" "${@}";
    done
}

fi
