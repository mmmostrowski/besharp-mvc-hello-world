LayersWidget () 
{ 
    __be___this_layersKey @vectors.make;
    __be___this_layersWidget @vectors.make;
    __be___this_key2idx @maps.make;
    __be___this_key2widget @maps.make;
    __be___this_key2focusWidget @maps.make;
    __be___this_activeWidgets @sets.make;
    __be___this_activeLayers @sets.make;
    __be___this_visibleWidgets @vectors.make;
    __be__layersKey $this.layersKey;
    __be__layersWidget $this.layersWidget;
    __be__key2idx $this.key2idx;
    __be__key2widget $this.key2widget;
    local item;
    local key;
    local isEven=true;
    local idx=0;
    local widget;
    for item in "${@}";
    do
        if $isEven; then
            isEven=false;
            key="${item}";
        else
            isEven=true;
            widget="${item}";
            $layersKey.add "${key}";
            $layersWidget.add $widget;
            $key2widget.set "${key}" $widget;
            $key2idx.set "${key}" "$(( idx++ ))";
        fi;
    done
}
LayersWidget.activateLayer () 
{ 
    local layerKey="${1}";
    __be__activeWidgets $this.activeWidgets;
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    $activeWidgets.add $widget;
    __be__coordinator $this.coordinator;
    $coordinator.activateWidget $widget;
    __be__activeLayers $this.activeLayers;
    $activeLayers.add "${layerKey}"
}
LayersWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    local maxWidth=-1;
    local maxHeight=-1;
    while @iterate @of $this.layersWidget @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    $calculator.setupSize "${maxWidth}" "${maxHeight}"
}
LayersWidget.childrenWidgets () 
{ 
    besharp.returningOf $this.layersWidget
}
LayersWidget.deactivateLayer () 
{ 
    local layerKey="${1}";
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    __be__activeWidgets $this.activeWidgets;
    $activeWidgets.remove $widget;
    __be__coordinator $this.coordinator;
    $coordinator.deactivateWidget $widget;
    __be__activeLayers $this.activeLayers;
    $activeLayers.remove "${layerKey}"
}
LayersWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__visibleWidgets $this.visibleWidgets;
    __be__visibleWidgetsSize $visibleWidgets.size;
    local idx=0;
    while (( idx < visibleWidgetsSize )); do
        __be__widget $visibleWidgets.get "$(( idx++ ))";
        $widgetsLayout.drawWidget $widget $canvas;
    done
}
LayersWidget.hideLayer () 
{ 
    local layerKey="${1}";
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    __be__visibleWidgets $this.visibleWidgets;
    $visibleWidgets.remove $widget;
    __be__coordinator $this.coordinator;
    $coordinator.makeWidgetInvisible $widget
}
LayersWidget.initialize () 
{ 
    __be__coordinator $this.coordinator;
    while @iterate @of $this.layersWidget @in widget; do
        $coordinator.deactivateWidget $widget;
        $coordinator.makeWidgetInvisible $widget;
    done
}
LayersWidget.isLayerActive () 
{ 
    local layerKey="${1}";
    __be__activeLayers $this.activeLayers;
    besharp.returningOf $activeLayers.has "${layerKey}"
}
LayersWidget.operativeChildrenWidget () 
{ 
    besharp.returningOf $this.activeWidgets
}
LayersWidget.organizeChildrenOnLayout () 
{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    while @iterate @of $this.visibleWidgets @in children; do
        $widgetsLayout.setupContextWindow $children "${x1}" "${y1}" "${width}" "${height}";
    done
}
LayersWidget.popLastFocusIndex () 
{ 
    __be__coordinator $this.coordinator;
    $coordinator.popFocusWidget
}
LayersWidget.pushLayerFocusIndex () 
{ 
    local layerKey="${1}";
    __be__key2focusWidget $this.key2focusWidget;
    __be__focusWidget $key2focusWidget.get "${layerKey}";
    __be__coordinator $this.coordinator;
    $coordinator.pushFocusWidget $focusWidget
}
LayersWidget.setupInitialFocusWidget () 
{ 
    local layerKey="${1}";
    local widget="${2}";
    __be__key2focusWidget $this.key2focusWidget;
    $key2focusWidget.set "${layerKey}" "${widget}"
}
LayersWidget.showLayer () 
{ 
    local layerKey="${1}";
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    __be__visibleWidgets $this.visibleWidgets;
    __be__alreadyVisible @sets.makeOf $visibleWidgets;
    $visibleWidgets.removeAll;
    while @iterate @of $this.layersWidget @in layerWidget; do
        if @true $alreadyVisible.has $layerWidget || @same $layerWidget $widget; then
            $visibleWidgets.add $layerWidget;
        fi;
    done;
    @unset $alreadyVisible;
    __be__coordinator $this.coordinator;
    $coordinator.makeWidgetVisible $widget
}
LayersWidget.switchLayerFocusIndex () 
{ 
    local layerKey="${1}";
    __be__key2focusWidget $this.key2focusWidget;
    __be__focusWidget $key2focusWidget.get "${layerKey}";
    $coordinator.switchFocusWidget $focusWidget
}
LayersWidget.turnOffLayer () 
{ 
    local layerKey="${1}";
    $layers.hideLayer "${layerKey}";
    $layers.deactivateLayer "${layerKey}"
}
LayersWidget.turnOnLayer () 
{ 
    local layerKey="${1}";
    $layers.activateLayer "${layerKey}";
    $layers.showLayer "${layerKey}"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["PopupFrameWidget"]='class'
besharp_oop_types["PopupFrameWidget"]="PopupFrameWidget"
