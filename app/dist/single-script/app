if ${beshfile_section__code:-false}; then :; 
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subjects() {
  "${@}"
  subjects="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__currentGreetingIdx() {
  "${@}"
  currentGreetingIdx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__currentGreetingIdx() {
  "${@}"
  currentGreetingIdx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__currentSubjectIdx() {
  "${@}"
  currentSubjectIdx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subjects() {
  "${@}"
  subjects="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetingNum() {
  "${@}"
  greetingNum="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__idx() {
  "${@}"
  idx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__idx() {
  "${@}"
  idx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subjectNum() {
  "${@}"
  subjectNum="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subjects() {
  "${@}"
  subjects="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__idx() {
  "${@}"
  idx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__idx() {
  "${@}"
  idx="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__subjects() {
  "${@}"
  subjects="${besharp_rcrvs[besharp_rcsl + 1]}"
}
AppState () 
{ 
    __be__greetings $this.greetings;
    $greetings.addGreetings "Hello" "!" "Hey" "!" "Hi" "!" "Aloha" "!" "Hola" "!" "Howdy" ":)" "What's up" "?" "Have a good day" "!" "Supp" "?" "How are you doing" "?" "Greetings" "!" "Long time no see" "!" "Morning" "!" "Nice to see you" "!";
    __be__subjects $this.subjects;
    $subjects.addSubjects "World" "Universe" "Y'all" "Everybody" "Programmers" "People" "Boy" "Girl" "Kid"
}
AppState.currentGreetingSuffix () 
{ 
    __be__greetings $this.greetings;
    __be__currentGreetingIdx $this.currentGreetingIdx;
    besharp.returningOf $greetings.suffix "${currentGreetingIdx}"
}
AppState.currentGreetingText () 
{ 
    __be__greetings $this.greetings;
    __be__currentGreetingIdx $this.currentGreetingIdx;
    besharp.returningOf $greetings.greeting "${currentGreetingIdx}"
}
AppState.currentSubjectText () 
{ 
    __be__subjects $this.subjects;
    __be__currentSubjectIdx $this.currentSubjectIdx;
    besharp.returningOf $subjects.subject "${currentSubjectIdx}"
}
AppState.greetingsList () 
{ 
    __be__greetings $this.greetings;
    besharp.returningOf $greetings.greetingsWithSuffixes
}
AppState.selectNextGreeting () 
{ 
    __be__greetings $this.greetings;
    __be__greetingNum $greetings.size;
    __be__idx $this.currentGreetingIdx;
    if (( ++idx >= greetingNum )); then
        idx=$(( greetingNum - 1 ));
    fi;
    $this.currentGreetingIdx = "${idx}"
}
AppState.selectNextSubject () 
{ 
    __be__subjects $this.subjects;
    __be__subjectNum $subjects.size;
    __be__idx $this.currentSubjectIdx;
    if (( ++idx >= subjectNum )); then
        idx="$(( subjectNum - 1 ))";
    fi;
    $this.currentSubjectIdx = "${idx}"
}
AppState.selectPrevGreeting () 
{ 
    __be__idx $this.currentGreetingIdx;
    if (( --idx < 0 )); then
        idx=0;
    fi;
    $this.currentGreetingIdx = "${idx}"
}
AppState.selectPrevSubject () 
{ 
    __be__idx $this.currentSubjectIdx;
    if (( --idx < 0 )); then
        idx=0;
    fi;
    $this.currentSubjectIdx = "${idx}"
}
AppState.subjectsList () 
{ 
    __be__subjects $this.subjects;
    besharp.returningOf $subjects.subjects
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
