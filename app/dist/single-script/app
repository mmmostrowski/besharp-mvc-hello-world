    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    while @iterate @of $this.widgets @in children; do
        $widgetsLayout.setupContextWindow $children "$(( x1 + 1 ))" "$(( y1 + 1 ))" "$(( width - 2 ))" "$(( height - 2 ))";
    done'
besharp_oop_type_PopupFrameWidget_method_locals["organizeChildrenOnLayout"]='local children
local height
