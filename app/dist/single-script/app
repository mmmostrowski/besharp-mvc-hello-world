function __be___this_greetings() {
  "${@}"
  eval $this"_greetings=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_greetingsWithSuffixes() {
  "${@}"
  eval $this"_greetingsWithSuffixes=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_suffixes() {
  "${@}"
  eval $this"_suffixes=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetingsWithSuffixes() {
  "${@}"
  greetingsWithSuffixes="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__suffixes() {
  "${@}"
  suffixes="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetingsWithSuffixes() {
  "${@}"
  greetingsWithSuffixes="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__greetings() {
  "${@}"
  greetings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__suffixes() {
  "${@}"
  suffixes="${besharp_rcrvs[besharp_rcsl + 1]}"
}
GreetingsList () 
{ 
    __be___this_greetings @vectors.make "${@}";
    __be___this_suffixes @vectors.make "${@}";
    __be___this_greetingsWithSuffixes @vectors.make "${@}"
}
GreetingsList.addGreeting () 
{ 
    local greeting="${1}";
    local suffix="${2}";
    __be__greetings $this.greetings;
    $greetings.add "${greeting}";
    __be__suffixes $this.suffixes;
    $suffixes.add "${suffix}";
    __be__greetingsWithSuffixes $this.greetingsWithSuffixes;
    $greetingsWithSuffixes.add "${greeting}${suffix}"
}
GreetingsList.addGreetings () 
{ 
    local even=true;
    local item;
    local greeting;
    for item in "${@}";
    do
        if $even; then
            even=false;
            greeting="${item}";
        else
            even=true;
            $this.addGreeting "${greeting}" "${item}";
        fi;
    done
}
GreetingsList.greeting () 
{ 
    local idx="${1}";
    __be__greetings $this.greetings;
    besharp.returningOf $greetings.get "${idx}"
}
GreetingsList.greetingWithSuffix () 
{ 
    local idx="${1}";
    __be__greetingsWithSuffixes $this.greetingsWithSuffixes;
    besharp.returningOf $greetingsWithSuffixes.get "${idx}"
}
GreetingsList.size () 
{ 
    __be__greetings $this.greetings;
    besharp.returningOf $greetings.size
}
GreetingsList.suffix () 
{ 
    local idx="${1}";
    __be__suffixes $this.suffixes;
    besharp.returningOf $suffixes.get "${idx}"
}

fi
