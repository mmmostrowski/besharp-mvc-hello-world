besharp_oop_type_FullscreenCanvas_method_is_returning["setupMinimalSize"]=false
besharp_oop_type_FullscreenCanvas_method_body["setupMinimalSize"]='    @parent "${@}";
    $this.setupTerminalWindow'
besharp_oop_type_FullscreenCanvas_method_locals["setupMinimalSize"]=''
besharp_oop_type_FullscreenCanvas_method_is_using_iterators["setupMinimalSize"]=false
besharp_oop_type_FullscreenCanvas_method_is_calling_parent["setupMinimalSize"]=true
