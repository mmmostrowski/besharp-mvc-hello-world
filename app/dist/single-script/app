WidgetsLayout () 
{ 
    @parent "${@}"
}
WidgetsLayout.addWidget () 
{ 
    local widget="${1}";
    local widgetId="${2}";
    __be__newContext @new WidgetDrawContext;
    __be__widget2FocusIndex $this.widget2FocusIndex;
    __be__widget2Context $this.widget2Context;
    __be__context2Widget $this.context2Widget;
    __be__widgetId2widget $this.widgetId2widget;
    $newContext.widgetId = "${widgetId}";
    if @true $widgetId2widget.hasKey "${widgetId}"; then
        __be__currentWidget $widgetId2widget.get "${widgetId}";
        __be__currentWidgetInfo @widgets.info $currentWidget;
        besharp.runtime.error "Widget id must be unique! '${widgetId}' widget id is already in use by: ${currentWidgetInfo}.";
    fi;
    $widgetId2widget.set "${widgetId}" $widget;
    $widget2FocusIndex.set $widget -1;
    $widget2Context.set $widget $newContext;
    $context2Widget.set $newContext $widget
}
WidgetsLayout.calculateCanvasMinimalSize () 
{ 
    local canvas="${1}";
    local widget="${2}";
    __be__calculator @new WidgetMinSizeCalculator;
    $calculator.calculateMinSize $widget;
    __be__minWidth $calculator.width;
    __be__minHeight $calculator.height;
    $canvas.setupMinimalSize "${minWidth}" "${minHeight}";
    @unset $calculator
}
WidgetsLayout.callWidgetAction () 
{ 
    local widget="${1}";
    local action="${2}";
    shift 2;
    __be__context $this.widgetContext $widget;
    $widget.${action} $context "${@}"
}
WidgetsLayout.drawWidget () 
{ 
    local widget="${1}";
    local canvas="${2}";
    __be__monitor $this.monitor;
    if @false $monitor.isWidgetVisible $widget; then
        return;
    fi;
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    if (( width <= 0 )) || (( height <= 0 )) || (( x1 < 0 )) || (( y1 < 0 )); then
        return;
    fi;
    $widget.draw $context $canvas $this
}
WidgetsLayout.initialize () 
{ 
    __be___this_widget2FocusIndex @maps.make;
    __be___this_widget2Context @maps.make;
    __be___this_context2Widget @maps.make;
    __be___this_widgetId2widget @maps.make
}
WidgetsLayout.setupContextWindow () 
{ 
    local widget="${1}";
    shift 1;
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    $context.setupWindow "${@}"
}
WidgetsLayout.setupWidgetFocusable () 
{ 
    local widget="${1}";
    local orderIndex="${2}";
    if ! @is $widget FocusableWidget; then
        besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) is expected to be implementing FocusableWidget interface!";
    fi;
    __be__widget2FocusIndex $this.widget2FocusIndex;
    if @true $widget2FocusIndex.has "${orderIndex}"; then
        besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) is expected to have unique focus order index! '${orderIndex}' is already taken!";
    fi;
    $widget2FocusIndex.set $widget "${orderIndex}"
}
WidgetsLayout.setupWidgetPlacement () 
{ 
    local widget="${1}";
    local placementLeft="${2}";
    local placementUp="${3}";
    local placementRight="${4}";
    local placementDown="${5}";
    local placementWidth="${6}";
    local placementHeight="${7}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    $context.placementLeft = "${placementLeft}";
    $context.placementRight = "${placementRight}";
    $context.placementUp = "${placementUp}";
    $context.placementDown = "${placementDown}";
    $context.placementWidth = "${placementWidth}";
    $context.placementHeight = "${placementHeight}"
}
WidgetsLayout.updateChildrenLayout () 
{ 
    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    __be__placementLeft $context.placementLeft;
    __be__placementUp $context.placementUp;
    __be__placementRight $context.placementRight;
    __be__placementDown $context.placementDown;
    if [[ "${placementLeft}" != '0' ]] || [[ "${placementRight}" != '0' ]] || [[ "${placementUp}" != '0' ]] || [[ "${placementDown}" != '0' ]]; then
        $this.updateChildrenPlacementHorizontal $widget $context "${placementLeft}" "${placementRight}";
        $this.updateChildrenPlacementVertical $widget $context "${placementUp}" "${placementDown}";
    fi;
    if @is $widget ContainerWidget; then
        $widget.organizeChildrenOnLayout $context $this;
    fi;
    if @is $widget LayoutChangesListeningWidget; then
        $widget.processLayoutChanges $context $this;
    fi
}
WidgetsLayout.updateChildrenPlacementHorizontal () 
{ 
    local widget="${1}";
    local context="${2}";
    local left="${3}";
    local right="${4}";
    if [[ "${left}" == '0' ]] && [[ "${right}" == '0' ]]; then
        return;
    fi;
    __be__ctxX1 $context.x1;
    if [[ "${left}" == 'auto' ]]; then
        __be__widgetWidth $context.placementWidth;
        if (( widgetWidth < 0 )); then
            if ! @is $widget WidgetWithSize; then
                besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use 'auto' placement!";
            fi;
            __be__widgetWidth $widget.widgetWidth;
        fi;
        if [[ "${right}" == 'auto' ]]; then
            __be__width $context.width;
            $context.x1 = $(( ctxX1 + ( ( width - widgetWidth ) / 2 )));
            $context.width = "${widgetWidth}";
        else
            __be__width $context.width;
            $context.x1 = $(( ctxX1 + width - widgetWidth - right ));
            $context.width = $(( right + widgetWidth ));
        fi;
    else
        if [[ "${right}" == 'auto' ]]; then
            __be__widgetWidth $context.placementWidth;
            if (( widgetWidth < 0 )); then
                if ! @is $widget WidgetWithSize; then
                    besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '*' placement!";
                fi;
                __be__widgetWidth $widget.widgetWidth;
            fi;
            $context.x1 = $(( ctxX1 + left ));
            __be__width $context.width;
            $context.width = $(( left + widgetWidth ));
        else
            __be__x1 $context.x1;
            $context.x1 = $(( x1 + left));
            __be__width $context.width;
            (( width -= ( left + right ) ));
            $context.width = "${width}";
        fi;
    fi
}
WidgetsLayout.updateChildrenPlacementVertical () 
{ 
    local widget="${1}";
    local context="${2}";
    local up="${3}";
    local down="${4}";
    if [[ "${up}" == '0' ]] && [[ "${down}" == '0' ]]; then
        return;
    fi;
    __be__ctxY1 $context.y1;
    if [[ "${up}" == 'auto' ]]; then
        __be__widgetHeight $context.placementHeight;
        if (( widgetHeight < 0 )); then
            if ! @is $widget WidgetWithSize; then
                besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '*' placement!";
            fi;
            __be__widgetHeight $widget.widgetHeight;
        fi;
        if [[ "${down}" == 'auto' ]]; then
            __be__height $context.height;
            $context.y1 = $(( ctxY1 + ( ( height - widgetHeight ) / 2 ) ));
            $context.height = "${widgetHeight}";
        else
            __be__height $context.height;
            $context.y1 = $(( ctxY1 + height - widgetHeight - down ));
            $context.height = $(( down + widgetHeight ));
        fi;
    else
        if [[ "${down}" == 'auto' ]]; then
            __be__widgetHeight $context.placementHeight;
            if (( widgetHeight < 0 )); then
                if ! @is $widget WidgetWithSize; then
                    besharp.runtime.error "Widget $widget ( $( besharp.rtti.objectType $widget ) ) must implement WidgetWithSize interface to be able to use '*' placement!";
                fi;
                __be__widgetHeight $widget.widgetHeight;
            fi;
            $context.y1 = $(( ctxY1 + up ));
            __be__height $context.height;
            $context.height = $(( up + widgetHeight ));
        else
            __be__y1 $context.y1;
            $context.y1 = $(( y1 + up ));
            __be__height $context.height;
            (( height -= ( up + down ) ));
            $context.height = "${height}";
        fi;
    fi
}
WidgetsLayout.updateLayoutFor () 
{ 
    local widget="${1}";
    local canvas="${2}";
    __be__w $canvas.width;
    __be__h $canvas.height;
    if @false $this.needsUpdate; then
        __be__lastSize $this.lastSize;
        $this.lastSize = "${w}x${h}";
        if [[ "${w}x${h}" == "${lastSize}" ]]; then
            return;
        fi;
    fi;
    $this.needsUpdate = false;
    __be__widget2Context $this.widget2Context;
    __be__context $this.widgetContext $widget;
    $context.setupWindow 0 0 "${w}" "${h}";
    __be__tree $this.tree;
    $tree.walkDownTree $widget $this.updateChildrenLayout
}
WidgetsLayout.widgetContext () 
{ 
    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    besharp.returningOf $widget2Context.get $widget
}
WidgetsLayout.widgetId () 
{ 
    local widget="${1}";
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    besharp.returningOf $context.widgetId
}
WidgetsLayout.widgetSortIndex () 
{ 
    local widget="${1}";
    __be__widget2FocusIndex $this.widget2FocusIndex;
    besharp.returningOf $widget2FocusIndex.get $widget
}

fi
