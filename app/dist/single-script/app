AppLayout () 
{ 
    @parent "${@}"
}
AppLayout.aboutUsWidget () 
{ 
    besharp.returningOf $this.createWidget id: 'about_us_widget' construct: AboutUsWidget placement: 0 0 0 0
}
AppLayout.appSettingsDialog () 
{ 
    __be__closeButton $this.closeSettingsButton;
    __be__isAnimationCheckbox $this.isAnimationCheckbox;
    __be__isPopupShadowsCheckbox $this.isPopupShadowsCheckbox;
    __be__themeNameInput $this.themeNameInput;
    __be__appState $this.appState;
    __be__appController $this.appController;
    $this.settingsInitialWidget = $isAnimationCheckbox;
    besharp.returningOf $this.createWidget id: 'popup_frame_widget' bindAppState: on_escape:$appController.closeAppSettings title:$appState.appSettingPopupTitle shadowsEnabled:$appState.isPopupShadowsEnabled construct: PopupFrameWidget $isAnimationCheckbox $isPopupShadowsCheckbox $themeNameInput $closeButton placement: auto auto auto auto placementSize: 50 13
}
AppLayout.areYouSureQuitDialog () 
{ 
    __be__yesButton $this.areYouSureQuitYesButton;
    __be__noButton $this.areYouSureQuitNoButton;
    __be__confirmationText $this.confirmationTextWidget;
    __be__appState $this.appState;
    __be__appController $this.appController;
    $this.areYouSureQuitInitialWidget = $noButton;
    besharp.returningOf $this.createWidget id: 'popup_are_you_sure_quit' bindAppState: on_escape:$appController.closeAreYouSureQuit title:$appState.areYouSureQuitPopupTitle shadowsEnabled:$appState.isPopupShadowsEnabled construct: PopupFrameWidget $confirmationText $yesButton $noButton placement: auto auto auto auto placementSize: 40 9
}
