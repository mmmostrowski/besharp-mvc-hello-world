besharp_oop_type_BaseListWidget_method_locals["processAnimationFrame"]='local slideAnimationFrame'
besharp_oop_type_BaseListWidget_method_is_using_iterators["processAnimationFrame"]=false
besharp_oop_type_BaseListWidget_method_is_calling_parent["processAnimationFrame"]=false
besharp_oop_type_BaseListWidget_method_is_calling_this["processAnimationFrame"]=true
besharp_oop_type_BaseListWidget_methods['processKeyboardEvent']='processKeyboardEvent'
besharp_oop_type_BaseListWidget_method_is_returning["processKeyboardEvent"]=false
besharp_oop_type_BaseListWidget_method_body["processKeyboardEvent"]='    local event="${1}";
    __be__nextSlideCommand $this.nextSlideCommand;
    __be__prevSlideCommand $this.prevSlideCommand;
    if @true $event.consumeWhenCommandRequested "${nextSlideCommand}"; then
        __be__state $this.state;
        $state.requestWidgetEvent $this '"'"'on_next'"'"';
    else
        if @true $event.consumeWhenCommandRequested "${prevSlideCommand}"; then
            __be__state $this.state;
            $state.requestWidgetEvent $this '"'"'on_prev'"'"';
        fi;
    fi'
besharp_oop_type_BaseListWidget_method_locals["processKeyboardEvent"]='local nextSlideCommand
local prevSlideCommand
local state'
besharp_oop_type_BaseListWidget_method_is_using_iterators["processKeyboardEvent"]=false
besharp_oop_type_BaseListWidget_method_is_calling_parent["processKeyboardEvent"]=false
besharp_oop_type_BaseListWidget_method_is_calling_this["processKeyboardEvent"]=true
besharp_oop_type_BaseListWidget_methods['slideAnimationBy']='slideAnimationBy'
besharp_oop_type_BaseListWidget_method_is_returning["slideAnimationBy"]=false
besharp_oop_type_BaseListWidget_method_body["slideAnimationBy"]='    local by="${1}";
    __be__from $this.currentSlide;
    local to="$(( from + by ))";
    $this.slideAnimationTo "${to}"'
besharp_oop_type_BaseListWidget_method_locals["slideAnimationBy"]='local from'
besharp_oop_type_BaseListWidget_method_is_using_iterators["slideAnimationBy"]=false
besharp_oop_type_BaseListWidget_method_is_calling_parent["slideAnimationBy"]=false
besharp_oop_type_BaseListWidget_method_is_calling_this["slideAnimationBy"]=true
besharp_oop_type_BaseListWidget_methods['slideAnimationTo']='slideAnimationTo'
besharp_oop_type_BaseListWidget_method_is_returning["slideAnimationTo"]=false
besharp_oop_type_BaseListWidget_method_body["slideAnimationTo"]='    local to="${1}";
    __be__maxSlide $this.maxSlide;
    if (( to > maxSlide )); then
        to="${maxSlide}";
    fi;
    __be__from $this.currentSlide;
    local shift="$(( to - from ))";
    $this.slideAnimationFrame = 3;
    __be__slideAnimationData $this.slideAnimationData;
    $slideAnimationData.setPlain "${to}" "$(( from + 2 * shift / 3 ))" "$(( from + shift / 3 ))" "${from}"'
besharp_oop_type_BaseListWidget_method_locals["slideAnimationTo"]='local from
local maxSlide
local slideAnimationData'
besharp_oop_type_BaseListWidget_method_is_using_iterators["slideAnimationTo"]=false
besharp_oop_type_BaseListWidget_method_is_calling_parent["slideAnimationTo"]=false
besharp_oop_type_BaseListWidget_method_is_calling_this["slideAnimationTo"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___this_slideAnimationData() {
  "${@}"
  eval $this"_slideAnimationData=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__slideAnimationData() {
  "${@}"
  slideAnimationData="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__emptyList() {
  "${@}"
  emptyList="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__slideAnimationFrame() {
  "${@}"
  slideAnimationFrame="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__nextSlideCommand() {
  "${@}"
  nextSlideCommand="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__prevSlideCommand() {
  "${@}"
  prevSlideCommand="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__from() {
  "${@}"
  from="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__from() {
  "${@}"
  from="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__maxSlide() {
  "${@}"
  maxSlide="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__slideAnimationData() {
  "${@}"
  slideAnimationData="${besharp_rcrvs[besharp_rcsl + 1]}"
}
BaseListWidget () 
{ 
    $this.prevSlideCommand = "${1}";
    $this.nextSlideCommand = "${2}"
}
BaseListWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
BaseListWidget.currentItemIdx () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'current' 0
}
BaseListWidget.initialize () 
{ 
    __be___this_slideAnimationData @vectors.make
}
BaseListWidget.initializeAnimation () 
{ 
    __be__slideAnimationData $this.slideAnimationData;
    $slideAnimationData.setPlain 0
}
BaseListWidget.itemsList () 
{ 
    __be__state $this.state;
    __be__emptyList @vectors.empty;
    besharp.returningOf $state.readWidgetState $this 'list' $emptyList
}
BaseListWidget.processAnimationFrame () 
{ 
    __be__slideAnimationFrame $this.slideAnimationFrame;
    if (( slideAnimationFrame > 0 )); then
        $this.slideAnimationFrame = $(( --slideAnimationFrame ));
    else
        $this.slideAnimationFrame = 0;
    fi;
    besharp_rcrvs[besharp_rcsl]="${slideAnimationFrame}"
}
BaseListWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    __be__nextSlideCommand $this.nextSlideCommand;
    __be__prevSlideCommand $this.prevSlideCommand;
    if @true $event.consumeWhenCommandRequested "${nextSlideCommand}"; then
        __be__state $this.state;
        $state.requestWidgetEvent $this 'on_next';
    else
        if @true $event.consumeWhenCommandRequested "${prevSlideCommand}"; then
            __be__state $this.state;
            $state.requestWidgetEvent $this 'on_prev';
        fi;
    fi
}
BaseListWidget.slideAnimationBy () 
{ 
    local by="${1}";
    __be__from $this.currentSlide;
    local to="$(( from + by ))";
    $this.slideAnimationTo "${to}"
}
BaseListWidget.slideAnimationTo () 
{ 
    local to="${1}";
    __be__maxSlide $this.maxSlide;
    if (( to > maxSlide )); then
        to="${maxSlide}";
    fi;
    __be__from $this.currentSlide;
    local shift="$(( to - from ))";
    $this.slideAnimationFrame = 3;
    __be__slideAnimationData $this.slideAnimationData;
    $slideAnimationData.setPlain "${to}" "$(( from + 2 * shift / 3 ))" "$(( from + shift / 3 ))" "${from}"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["HorizontalListWidget"]='class'
besharp_oop_types["HorizontalListWidget"]="HorizontalListWidget"
declare -Ag besharp_oop_type_HorizontalListWidget_interfaces
besharp_oop_classes["HorizontalListWidget"]="HorizontalListWidget"
besharp_oop_type_parent["HorizontalListWidget"]="BaseListWidget"
declare -Ag besharp_oop_type_HorizontalListWidget_fields
declare -Ag besharp_oop_type_HorizontalListWidget_injectable_fields
declare -Ag besharp_oop_type_HorizontalListWidget_field_type
declare -Ag besharp_oop_type_HorizontalListWidget_field_default
declare -Ag besharp_oop_type_HorizontalListWidget_methods
declare -Ag besharp_oop_type_HorizontalListWidget_method_body
declare -Ag besharp_oop_type_HorizontalListWidget_method_locals
declare -Ag besharp_oop_type_HorizontalListWidget_method_is_returning
declare -Ag besharp_oop_type_HorizontalListWidget_method_is_abstract
declare -Ag besharp_oop_type_HorizontalListWidget_method_is_using_iterators
declare -Ag besharp_oop_type_HorizontalListWidget_method_is_calling_parent
declare -Ag besharp_oop_type_HorizontalListWidget_method_is_calling_this
besharp_oop_type_HorizontalListWidget_fields['slideChunks']='slideChunks'
besharp_oop_type_HorizontalListWidget_field_type['slideChunks']="Stack"
besharp_oop_type_HorizontalListWidget_field_default['slideChunks']=""
besharp_oop_type_HorizontalListWidget_methods['HorizontalListWidget']='HorizontalListWidget'
besharp_oop_type_HorizontalListWidget_method_is_returning["HorizontalListWidget"]=false
besharp_oop_type_HorizontalListWidget_method_body["HorizontalListWidget"]='    @parent "left" "right"'
besharp_oop_type_HorizontalListWidget_method_locals["HorizontalListWidget"]=''
besharp_oop_type_HorizontalListWidget_method_is_using_iterators["HorizontalListWidget"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_parent["HorizontalListWidget"]=true
besharp_oop_type_HorizontalListWidget_method_is_calling_this["HorizontalListWidget"]=false
besharp_oop_type_HorizontalListWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_HorizontalListWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_HorizontalListWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    $calculator.setupSize 0 3'
besharp_oop_type_HorizontalListWidget_method_locals["calculateMinSize"]=''
besharp_oop_type_HorizontalListWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_this["calculateMinSize"]=false
besharp_oop_type_HorizontalListWidget_methods['draw']='draw'
besharp_oop_type_HorizontalListWidget_method_is_returning["draw"]=false
besharp_oop_type_HorizontalListWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__canvasPrinter $this.canvasPrinter;
    __be__x1 $context.x1;
    __be__width $context.width;
    __be__y0 $context.y1;
    local y1="$(( y0 + 1 ))";
    __be__buttonStyle @pixel_modes.bgDGray;
    __be__selButtonStyle @pixel_modes.bgLGray;
    __be__color1 @pixel_modes.lYellow;
    __be__color2 @pixel_modes.dGray;
    __be__dotsColor @pixel_modes.dGray;
    local style="${buttonStyle}${color1}";
    local selectedStyle="${selButtonStyle}${color2}";
    __be__currentItemIdx $this.currentItemIdx;
    __be__isFocused $context.focusedFlag;
    __be__slideAnimationFrame $this.slideAnimationFrame;
    __be__slideAnimationData $this.slideAnimationData;
    __be__slide $slideAnimationData.get "${slideAnimationFrame}";
    $this.currentSlide = "${slide}";
    local moreIndicatorPlacement="0";
    (( width -= 4 ));
    local toSlideRight=0;
    local toSlide=0;
    local x="$(( x1 + 4  ))";
    local idx=-1;
    local showDots=false;
    __be__itemsList $this.itemsList;
    __be__itemsListSize $itemsList.size;
    local itemsListIdx=0;
    while (( itemsListIdx < itemsListSize )); do
        __be__item $itemsList.get "$(( itemsListIdx++ ))";
        local itemSize="$(( "${#item}" + 10 ))";
        if (( ++idx == currentItemIdx )); then
            local isCurrentItem=true;
        else
            local isCurrentItem=false;
        fi;
        if (( x - slide + itemSize > x1 + width )); then
            showDots=true;
            if $isCurrentItem && (( slideAnimationFrame == 0 )); then
                $this.slideAnimationTo "${toSlide}";
                $widgetsLayout.needsRedraw = true;
            fi;
            (( x+= itemSize ));
            continue;
        fi;
        if (( x - slide < x1 )); then
            if $isCurrentItem && (( slideAnimationFrame == 0 )); then
                $this.slideAnimationTo "${toSlide}";
                $widgetsLayout.needsRedraw = true;
            fi;
        else
            if $isCurrentItem; then
                if $isFocused; then
                    $canvasPrinter.printText $canvas "$(( x - slide - 1 ))" "${y1}" " -${item}- " "${selectedStyle}";
                else
                    $canvasPrinter.printText $canvas "$(( x - slide - 1 ))" "${y1}" "  ${item}  " "${selectedStyle}";
                fi;
            else
                $canvasPrinter.printText $canvas "$(( x - slide + 1 - 2 ))" "${y1}" "  ${item}  " "${style}";
            fi;
            moreIndicatorPlacement="$(( x - slide + 1 + itemSize ))";
        fi;
        (( toSlide += itemSize ));
        (( x+= itemSize ));
    done;
    if $showDots; then
        $canvasPrinter.printText $canvas "${moreIndicatorPlacement}" "${y1}" "... " "${dotsColor}";
    fi'
besharp_oop_type_HorizontalListWidget_method_locals["draw"]='local buttonStyle
local canvasPrinter
local color1
local color2
local currentItemIdx
local dotsColor
local isFocused
local item
local itemsList
local itemsListSize
local selButtonStyle
local slide
local slideAnimationData
local slideAnimationFrame
local width
local x1
local y0'
besharp_oop_type_HorizontalListWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_this["draw"]=true
besharp_oop_type_HorizontalListWidget_methods['initialize']='initialize'
besharp_oop_type_HorizontalListWidget_method_is_returning["initialize"]=false
besharp_oop_type_HorizontalListWidget_method_body["initialize"]='    @parent;
    __be___this_slideChunks @collections.makeStack'
besharp_oop_type_HorizontalListWidget_method_locals["initialize"]=''
besharp_oop_type_HorizontalListWidget_method_is_using_iterators["initialize"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_parent["initialize"]=true
besharp_oop_type_HorizontalListWidget_method_is_calling_this["initialize"]=true
besharp_oop_type_HorizontalListWidget_methods['processLayoutChanges']='processLayoutChanges'
besharp_oop_type_HorizontalListWidget_method_is_returning["processLayoutChanges"]=false
besharp_oop_type_HorizontalListWidget_method_body["processLayoutChanges"]='    local context="${1}";
    local layout="${2}";
    __be__x1 $context.x1;
    __be__width $context.width;
    (( width -= 8 )) || true;
    __be__slideChunks $this.slideChunks;
    $slideChunks.removeAll;
    local totalSlide=0;
    while @iterate @of $this.itemsList @in item; do
        local itemSize="$(( "${#item}" + 10 ))";
        $slideChunks.add "${itemSize}";
        (( totalSlide += itemSize ));
    done;
    while @false $slideChunks.isEmpty; do
        __be__itemSize $slideChunks.current;
        (( width -= itemSize )) || true;
        if (( width < 0 )); then
            break;
        fi;
        $slideChunks.pull;
    done;
    local maxSlide=0;
    while @false $slideChunks.isEmpty; do
        __be__itemSize $slideChunks.pull;
        (( maxSlide += itemSize ));
    done;
    $this.maxSlide = "${maxSlide}";
    __be__currentSlide $this.currentSlide;
    if (( currentSlide > maxSlide )); then
        $this.slideAnimationTo "${maxSlide}";
    fi'
besharp_oop_type_HorizontalListWidget_method_locals["processLayoutChanges"]='local currentSlide
local item
local itemSize
local slideChunks
local width
local x1'
besharp_oop_type_HorizontalListWidget_method_is_using_iterators["processLayoutChanges"]=true
besharp_oop_type_HorizontalListWidget_method_is_calling_parent["processLayoutChanges"]=false
besharp_oop_type_HorizontalListWidget_method_is_calling_this["processLayoutChanges"]=true

fi
