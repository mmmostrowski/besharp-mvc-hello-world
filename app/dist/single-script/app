AppLayout.mainTabs () 
{ 
    __be__helloWorld $this.helloWorldTab;
    __be__realHelloWorld $this.realHelloWorldTab;
    __be__balls $this.ballsTab;
    __be__fastBalls $this.fastBallsTab;
    __be__color @pixel_modes.lRed;
    __be__idleWidget $this.createWidget id: "idle_tab_red" construct: FlatWidget "${color}";
    besharp.returningOf $this.createWidget id: 'main_tabs' construct: TabsWidget 'MVC Hello World' $helloWorld 'Hello World' $realHelloWorld 'Performance test' $fastBalls 'GUI test' $balls 'Idle' $idleWidget placement: 0 0 0 0 focusIndex: 10
}
AppLayout.makeNewBallsSection () 
{ 
    local wrapperWidgetClass="${1}";
    __be__currentBalls $this.ballsHandlerWidget;
    __be__container $this.ballsContainerWidget;
    __be__newBalls $this.createWidget id: "balls_tab_balls_$(date +%s)_${RANDOM}" construct: BallsWidget 1 placement: 0 0 0 0;
    __be__wrapper $this.createWidget id: "balls_tab_balls_wrapper_$(date +%s)_${RANDOM}" construct: ${wrapperWidgetClass} $currentBalls 50% $newBalls auto placement: 0 0 0 0;
    $this.ballsHandlerWidget = $wrapper;
    $container.replaceDownWidget $wrapper;
    besharp_rcrvs[besharp_rcsl]=$newBalls
}
AppLayout.makeNewHorizontalBallsSection () 
{ 
    besharp.returningOf $this.makeNewBallsSection TwoRowsWidget
}
AppLayout.makeNewVerticalBallsSection () 
{ 
    besharp.returningOf $this.makeNewBallsSection TwoColumnsWidget
}
AppLayout.openSettingsButton () 
{ 
    __be__appState $this.appState;
    __be__appController $this.appController;
    besharp.returningOf $this.createWidget id: 'main_settings_open_button' construct: ButtonWidget bindAppState: text:$appState.openSettingsButtonText clicked:$appController.openAppSettings placement: auto 0 0 auto focusIndex: 0
}
AppLayout.prepareRootWidget () 
{ 
    __be__fps $this.fpsWidget;
    __be__mainFrame $this.mainFrame;
    __be__appSettings $this.appSettingsDialog;
    __be__areYouSureQuit $this.areYouSureQuitDialog;
    __be__helpDialog $this.helpDialog;
    __be__layers $this.createWidget id: 'main_layers' construct: LayersWidget 'main' $mainFrame 'are_you_sure_quit' $areYouSureQuit 'settings' $appSettings 'help' $helpDialog 'fps' $fps placement: 0 0 0 0;
    __be__areYouSureQuitInitialWidget $this.areYouSureQuitInitialWidget;
    $layers.setupInitialFocusWidget 'are_you_sure_quit' $areYouSureQuitInitialWidget;
    __be__helpDialogInitialWidget $this.helpDialogInitialWidget;
    $layers.setupInitialFocusWidget 'help' $helpDialogInitialWidget;
    __be__settingsInitialWidget $this.settingsInitialWidget;
    $layers.setupInitialFocusWidget 'settings' $settingsInitialWidget;
    $layers.turnOnLayer 'main';
    $layers.turnOnLayer 'fps';
    $layers.turnOffLayer 'are_you_sure_quit';
    $layers.turnOffLayer 'settings';
    $this.layers = $layers;
    besharp_rcrvs[besharp_rcsl]=$layers
}
AppLayout.realHelloWorldTab () 
{ 
    besharp.returningOf $this.createWidget id: 'real_hello_world' construct: RealHelloWorldWidget placement: auto auto auto auto
}
AppLayout.replaceTopBallsSection () 
{ 
    local newWrapper="${1}";
    __be__container $this.ballsContainerWidget;
    $container.replaceDownWidget $newWrapper;
    $this.ballsHandlerWidget = $newWrapper
}
AppLayout.themeNameInput () 
{ 
    __be__appState $this.appState;
    besharp.returningOf $this.createWidget id: 'main_settings_popup_theme_name_input' construct: InputWidget 20 bindAppState: label:$appState.themeNameLabel text:$appState.themeName placement: 0 5 0 0 focusIndex: 140
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_abstract["BaseController"]='true'
besharp_oop_type_is["BaseController"]='class'
besharp_oop_types["BaseController"]="BaseController"
declare -Ag besharp_oop_type_BaseController_interfaces
besharp_oop_classes["BaseController"]="BaseController"
besharp_oop_type_parent["BaseController"]="Object"
declare -Ag besharp_oop_type_BaseController_fields
declare -Ag besharp_oop_type_BaseController_injectable_fields
declare -Ag besharp_oop_type_BaseController_field_type
declare -Ag besharp_oop_type_BaseController_field_default
declare -Ag besharp_oop_type_BaseController_methods
declare -Ag besharp_oop_type_BaseController_method_body
declare -Ag besharp_oop_type_BaseController_method_locals
declare -Ag besharp_oop_type_BaseController_method_is_returning
declare -Ag besharp_oop_type_BaseController_method_is_abstract
declare -Ag besharp_oop_type_BaseController_method_is_using_iterators
declare -Ag besharp_oop_type_BaseController_method_is_calling_parent
declare -Ag besharp_oop_type_BaseController_method_is_calling_this
