    local x="${2}";
    local y="${3}";
    local text="${4}";
    shift 4;
    __be__bgBlack @pixel_modes.bgBlack;
    local i;
    local c=;
    for ((i=0; i < ${#text}; i++ ))
    do
        c="${text:$i:1}";
        if [[ "${c}" == '"'"' '"'"' ]]; then
            (( x++ ));
            continue;
        fi;
        $canvas.putPixel "$(( x++ ))" "${y}" "${c}" "${bgBlack}" "${@}";
    done'
besharp_oop_type_CanvasPrinter_method_locals["printSparseText"]='local bgBlack'
besharp_oop_type_CanvasPrinter_method_is_using_iterators["printSparseText"]=false
