if ${beshfile_section__code:-false}; then :; 
function __be___this_parentWidgets() {
  "${@}"
  eval $this"_parentWidgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_widgets() {
  "${@}"
  eval $this"_widgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__widgets() {
  "${@}"
  widgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__parentWidgets() {
  "${@}"
  parentWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__parent() {
  "${@}"
  parent="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__parentWidgets() {
  "${@}"
  parentWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__continue() {
  "${@}"
  continue="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__parent() {
  "${@}"
  parent="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__parentWidgets() {
  "${@}"
  parentWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetsTree () 
{ 
    __be___this_widgets @sets.make;
    __be___this_parentWidgets @maps.make
}
WidgetsTree.addWidget () 
{ 
    local widget="${1}";
    local active="${2:-true}";
    __be__widgets $this.widgets;
    $widgets.add $widget;
    if @is $widget ContainerWidget; then
        $this.setupContainerWidget $widget;
    fi
}
WidgetsTree.setupContainerWidget () 
{ 
    local parent="${1}";
    __be__parentWidgets $this.parentWidgets;
    while @iterate @of $parent.childrenWidgets @in child; do
        $parentWidgets.set $child $parent;
    done
}
WidgetsTree.walkDownTree () 
{ 
    local widget="${1}";
    local callback="${2}";
    shift 2;
    "${callback}" $widget "${@}";
    if @is $widget ContainerWidget; then
        while @iterate @of $widget.childrenWidgets @in child; do
            $this.walkDownTree $child "${callback}" "${@}";
        done;
    fi
}
WidgetsTree.walkDownTreeOperatives () 
{ 
    local widget="${1}";
    local callback="${2}";
    shift 2;
    "${callback}" $widget "${@}";
    if @is $widget ContainerWidget; then
        while @iterate @of $widget.operativeChildrenWidget @in child; do
            $this.walkDownTreeOperatives $child "${callback}" "${@}";
        done;
    fi
}
WidgetsTree.walkUpTree () 
{ 
    local widget="${1}";
    local callback="${2}";
    shift 2;
    "${callback}" $widget "${@}";
    __be__parentWidgets $this.parentWidgets;
    if @true $parentWidgets.hasKey $widget; then
        __be__parent $parentWidgets.get $widget;
        $this.walkUpTree $parent "${callback}" "${@}";
    fi
}
WidgetsTree.walkUpTreeUntil () 
{ 
    local widget="${1}";
    local callback="${2}";
    shift 2;
    __be__continue "${callback}" $widget "${@}";
    if $continue; then
        __be__parentWidgets $this.parentWidgets;
        if @true $parentWidgets.hasKey $widget; then
            __be__parent $parentWidgets.get $widget;
            $this.walkUpTree $parent "${callback}" "${@}";
        fi;
    fi
}

fi
