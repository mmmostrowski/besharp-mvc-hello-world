  "${@}"
  highlightStyle1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__highlightStyle2() {
  "${@}"
  highlightStyle2="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__normalStyle() {
  "${@}"
  normalStyle="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__resetStyle() {
  "${@}"
  resetStyle="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x() {
  "${@}"
  x="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y() {
  "${@}"
  y="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___this_clickAnimationDataLength() {
  "${@}"
  eval $this"_clickAnimationDataLength=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_clickAnimationDataStart() {
  "${@}"
  eval $this"_clickAnimationDataStart=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__clickAnimationFrame() {
  "${@}"
  clickAnimationFrame="${besharp_rcrvs[besharp_rcsl + 1]}"
}
