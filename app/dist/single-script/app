    else
        if [[ "${downSize%\%}" != "${downSize}" ]]; then
            downSize=$(( ( "${downSize%\%}" * height ) / 100 ));
        fi;
        $widgetsLayout.setupContextWindow $upWidget "${x1}" "${y1}" "${width}" $(( height - downSize ));
        $widgetsLayout.setupContextWindow $downWidget "${x1}" $(( y1 + height - downSize )) "${width}" "${downSize}";
    fi
}
TwoRowsWidget.replaceDownWidget () 
{ 
    local widget="${1}";
    $this.downWidget = $widget;
    __be__bothWidgets $this.bothWidgets;
    $bothWidgets.set 1 $widget
}
TwoRowsWidget.replaceUpWidget () 
{ 
    local widget="${1}";
    $this.upWidget = $widget;
    __be__bothWidgets $this.bothWidgets;
    $bothWidgets.set 0 $widget
}

fi
