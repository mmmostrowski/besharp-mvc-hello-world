besharp_oop_type_AbstractCanvas_fields['minWidth']='minWidth'
besharp_oop_type_AbstractCanvas_field_default['minWidth']="0"
besharp_oop_type_AbstractCanvas_fields['minHeight']='minHeight'
besharp_oop_type_AbstractCanvas_field_default['minHeight']="0"
besharp_oop_type_AbstractCanvas_fields['w2']='w2'
besharp_oop_type_AbstractCanvas_field_default['w2']="0"
besharp_oop_type_AbstractCanvas_methods['render']='render'
besharp_oop_type_AbstractCanvas_method_is_abstract["render"]='true'
besharp_oop_type_AbstractCanvas_methods['AbstractCanvas']='AbstractCanvas'
besharp_oop_type_AbstractCanvas_method_is_returning["AbstractCanvas"]=false
besharp_oop_type_AbstractCanvas_method_body["AbstractCanvas"]='    __be___this_pixelBuffer @new ArrayVector;
    __be___this_pixelBufferClean @new ArrayVector'
besharp_oop_type_AbstractCanvas_method_locals["AbstractCanvas"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["AbstractCanvas"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["AbstractCanvas"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["AbstractCanvas"]=true
besharp_oop_type_AbstractCanvas_methods['beginFrame']='beginFrame'
besharp_oop_type_AbstractCanvas_method_is_returning["beginFrame"]=false
besharp_oop_type_AbstractCanvas_method_body["beginFrame"]='    $this.clearBuffer'
besharp_oop_type_AbstractCanvas_method_locals["beginFrame"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["beginFrame"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["beginFrame"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["beginFrame"]=true
besharp_oop_type_AbstractCanvas_methods['clearBuffer']='clearBuffer'
besharp_oop_type_AbstractCanvas_method_is_returning["clearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_body["clearBuffer"]='    __be__pixelBufferClean $this.pixelBufferClean;
    __be__pixelBuffer $this.pixelBuffer;
    eval "$pixelBuffer=( \"\${$pixelBufferClean[@]}\" )"'
besharp_oop_type_AbstractCanvas_method_locals["clearBuffer"]='local pixelBuffer
local pixelBufferClean'
besharp_oop_type_AbstractCanvas_method_is_using_iterators["clearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["clearBuffer"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["clearBuffer"]=true
besharp_oop_type_AbstractCanvas_methods['clearTerminal']='clearTerminal'
besharp_oop_type_AbstractCanvas_method_is_returning["clearTerminal"]=false
besharp_oop_type_AbstractCanvas_method_body["clearTerminal"]='    local cursorMode="${1:-cursor-hide}";
    __be__cursor $this.cursor;
    $cursor.clearAndMoveTo0x0;
    if [[ "${cursorMode}" == "cursor-hide" ]]; then
        $cursor.hide;
    else
        $cursor.show;
    fi'
besharp_oop_type_AbstractCanvas_method_locals["clearTerminal"]='local cursor'
besharp_oop_type_AbstractCanvas_method_is_using_iterators["clearTerminal"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["clearTerminal"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["clearTerminal"]=true
besharp_oop_type_AbstractCanvas_methods['initialize']='initialize'
besharp_oop_type_AbstractCanvas_method_is_returning["initialize"]=false
besharp_oop_type_AbstractCanvas_method_body["initialize"]='    besharp.addShutdownCallback '"'"'canvas_restore_terminal'"'"' $this.restoreTerminal;
    $this.clearTerminal'
besharp_oop_type_AbstractCanvas_method_locals["initialize"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["initialize"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["initialize"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["initialize"]=true
besharp_oop_type_AbstractCanvas_methods['putPixel']='putPixel'
besharp_oop_type_AbstractCanvas_method_is_returning["putPixel"]=false
besharp_oop_type_AbstractCanvas_method_body["putPixel"]='    local x="${1}";
    local y="${2}";
    local char="${3}";
    shift 3;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
        ";
    eval "$pixelBuffer[\${cell}]=\"\${modes// /}\"";
    eval "$pixelBuffer[\${cell} + 1]=\"\${char}\""'
besharp_oop_type_AbstractCanvas_method_locals["putPixel"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["putPixel"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["putPixel"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["putPixel"]=true
besharp_oop_type_AbstractCanvas_methods['putPixelChar']='putPixelChar'
besharp_oop_type_AbstractCanvas_method_is_returning["putPixelChar"]=false
besharp_oop_type_AbstractCanvas_method_body["putPixelChar"]='    local x="${1}";
    local y="${2}";
    local char="${3}";
    eval "
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "$pixelBuffer[\$(( ( y * ${this}_width + x ) * 2 + 1 ))]=\"\${char}\""'
besharp_oop_type_AbstractCanvas_method_locals["putPixelChar"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["putPixelChar"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["putPixelChar"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["putPixelChar"]=true
besharp_oop_type_AbstractCanvas_methods['putPixelModes']='putPixelModes'
besharp_oop_type_AbstractCanvas_method_is_returning["putPixelModes"]=false
besharp_oop_type_AbstractCanvas_method_body["putPixelModes"]='    local x="${1}";
    local y="${2}";
    shift 2;
    eval "
            local modes=\"\${*:-\e[0m}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    eval "$pixelBuffer[\$(( ( y * ${this}_width + x ) * 2 ))]=\"\${modes// /}\""'
besharp_oop_type_AbstractCanvas_method_locals["putPixelModes"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["putPixelModes"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["putPixelModes"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["putPixelModes"]=true
besharp_oop_type_AbstractCanvas_methods['putPixelsBox']='putPixelsBox'
besharp_oop_type_AbstractCanvas_method_is_returning["putPixelsBox"]=true
besharp_oop_type_AbstractCanvas_method_body["putPixelsBox"]='    local x="${1}";
    local y="${2}";
    local width="${3}";
    local height="${4}";
    local char="${5}";
    shift 5;
    eval "
            local modes=\"\${*:-\e[0m}\"
            modes=\"\${modes// /}\"
            local pixelBuffer=\"\${${this}_pixelBuffer}\"
        ";
    local rest=$(( width % 8 ));
    width=$(( width / 8 ));
    local line=$height;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( line-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes}\"
                   $pixelBuffer[ c + 2 ]=\"\${modes}\"
                   $pixelBuffer[ c + 4 ]=\"\${modes}\"
                   $pixelBuffer[ c + 6 ]=\"\${modes}\"
                   $pixelBuffer[ c + 8 ]=\"\${modes}\"
                   $pixelBuffer[ c + 10 ]=\"\${modes}\"
                   $pixelBuffer[ c + 12 ]=\"\${modes}\"
                   $pixelBuffer[ c + 14 ]=\"\${modes}\"
                   $pixelBuffer[ c + 1 ]=\"\${char}\"
                   $pixelBuffer[ c + 3 ]=\"\${char}\"
                   $pixelBuffer[ c + 5 ]=\"\${char}\"
                   $pixelBuffer[ c + 7 ]=\"\${char}\"
                   $pixelBuffer[ c + 9 ]=\"\${char}\"
                   $pixelBuffer[ c + 11 ]=\"\${char}\"
                   $pixelBuffer[ c + 13 ]=\"\${char}\"
                   $pixelBuffer[ c + 15 ]=\"\${char}\"
                   (( c += 16 ))
                done
                (( cell += ${this}_w2 ))
            done
        ";
    if (( rest == 0 )); then
        return;
    fi;
    (( x += ( width * 8 ) ));
    width=$rest;
    eval "
            local cell=\$(( ( y * ${this}_width + x ) * 2 ))
            while (( height-- )); do
                local i=\${width}
                local c=\${cell}
                while (( i-- )); do
                   $pixelBuffer[ c + 0 ]=\"\${modes// /}\"
                   $pixelBuffer[ c + 1 ]=\"\${char}\"
                   (( c += 2 ))
                done
                (( cell += ${this}_w2 ))
            done
        "'
besharp_oop_type_AbstractCanvas_method_locals["putPixelsBox"]=''
besharp_oop_type_AbstractCanvas_method_is_using_iterators["putPixelsBox"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_parent["putPixelsBox"]=false
besharp_oop_type_AbstractCanvas_method_is_calling_this["putPixelsBox"]=true
besharp_oop_type_AbstractCanvas_methods['putPixelsBoxChar']='putPixelsBoxChar'
besharp_oop_type_AbstractCanvas_method_is_returning["putPixelsBoxChar"]=true
besharp_oop_type_AbstractCanvas_method_body["putPixelsBoxChar"]='    local x="${1}";
    local y="${2}";
    local width="${3}";
