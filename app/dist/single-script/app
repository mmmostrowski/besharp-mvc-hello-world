declare -Ag besharp_oop_type_VerticalListWidget_method_body
declare -Ag besharp_oop_type_VerticalListWidget_method_locals
declare -Ag besharp_oop_type_VerticalListWidget_method_is_returning
declare -Ag besharp_oop_type_VerticalListWidget_method_is_abstract
declare -Ag besharp_oop_type_VerticalListWidget_method_is_using_iterators
declare -Ag besharp_oop_type_VerticalListWidget_method_is_calling_parent
