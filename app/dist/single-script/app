  "${@}"
  x1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y1() {
  "${@}"
  y1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
ButtonWidget () 
{ 
    @parent "${@}"
}
ButtonWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
ButtonWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "$(( ${#text} + 6 ))" 1
}
ButtonWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__clickAnimationDataStart $this.clickAnimationDataStart;
    __be__clickAnimationDataLength $this.clickAnimationDataLength;
    __be__canvasPrinter $this.canvasPrinter;
    __be__resetStyle @pixel_modes.reset;
    __be__focusedStyle @pixel_modes.white;
    __be__normalStyle @pixel_modes.dGray;
    __be__highlightStyle1 @pixel_modes.white;
    __be__highlightStyle2 @pixel_modes.bold;
    local highlightStyle="${highlightStyle1}${highlightStyle2}";
    __be__clickAnimationFrame $this.clickAnimationFrame;
    __be__x $context.x1;
    __be__y $context.y1;
    if @true $context.focusedFlag; then
        if (( clickAnimationFrame == 3 )); then
            $canvasPrinter.printText $canvas "${x}" "${y}" " -[${text}]- " "${resetStyle}${focusedStyle}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" "-[ ${text} ]-" "${resetStyle}${focusedStyle}";
        fi;
    else
        if (( clickAnimationFrame == 3 )); then
            $canvasPrinter.printText $canvas "${x}" "${y}" "  [${text}]  " "${resetStyle}${normalStyle}";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" " [ ${text} ] " "${resetStyle}${normalStyle}";
        fi;
    fi;
    if (( clickAnimationFrame >= 0 )) && (( clickAnimationFrame < 4 )); then
        __be__highlightStart $clickAnimationDataStart.get "${clickAnimationFrame}";
        if (( highlightStart > 0 )); then
            __be__highlightLength $clickAnimationDataLength.get "${clickAnimationFrame}";
            $canvas.putPixelsHorizModes "${highlightStart}" "${y}" "${highlightLength}" "${resetStyle}${highlightStyle}";
        fi;
    fi
}
ButtonWidget.initialize () 
{ 
    __be___this_clickAnimationDataStart @vectors.make;
    __be___this_clickAnimationDataLength @vectors.make
}
ButtonWidget.initializeAnimation () 
{ 
    :
}
ButtonWidget.processAnimationFrame () 
{ 
    __be__clickAnimationFrame $this.clickAnimationFrame;
    $this.clickAnimationFrame = $(( --clickAnimationFrame ));
    if (( clickAnimationFrame == 1 )); then
        if @true $this.isActionRequested; then
            $this.isActionRequested = fale;
            __be__state $this.state;
            $state.requestWidgetEvent $this 'clicked';
        fi;
    fi;
    besharp_rcrvs[besharp_rcsl]="${clickAnimationFrame}"
}
ButtonWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    if @true $this.isActionRequested; then
        return;
    fi;
    if @true $event.consumeWhenCommandRequested 'action'; then
        __be__state $this.state;
        $this.isActionRequested = true;
        $this.clickAnimationFrame = 4;
    fi
}
ButtonWidget.processLayoutChanges () 
{ 
    local context="${1}";
    local layout="${2}";
    __be__clickAnimationDataStart $this.clickAnimationDataStart;
    __be__clickAnimationDataLength $this.clickAnimationDataLength;
    __be__text $this.text;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    local buttonWidth="$(( ${#text} + 6 ))";
    local buttonWidth2="$(( buttonWidth / 2 ))";
    $clickAnimationDataStart.setPlain -1 "$(( x1 + buttonWidth2 ))" "${x1}" "${x1}";
    $clickAnimationDataLength.setPlain -1 "${buttonWidth2}" "${buttonWidth}" "${buttonWidth2}"
}
ButtonWidget.text () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'text' ''
}
ButtonWidget.widgetHeight () 
{ 
    besharp_rcrvs[besharp_rcsl]=1
}
ButtonWidget.widgetWidth () 
{ 
    __be__text $this.text;
    besharp_rcrvs[besharp_rcsl]="$(( ${#text} + 6 ))"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
