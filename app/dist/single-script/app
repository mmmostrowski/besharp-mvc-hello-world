if ${beshfile_section__code:-false}; then :; 
KeyboardDriver () 
{ 
    @parent "${@}"
}
KeyboardDriver.scanKeys () 
{ 
    local finalInput=;
    local input=;
    while true; do
        read -s -r -d '' -t 0.00005 -N1 input || true;
        if [[ "${input}" == '' ]]; then
            local input2;
            read -s -r -d '' -t 0.00005 -N2 input2 || true;
            input+="${input2}";
            if [[ "${input2}" == '[M' ]]; then
                local input3;
                read -s -r -d '' -t 0.00005 -N3 input3 || true;
                input+="${input3}";
            fi;
        fi;
        if [[ -n "${input}" ]]; then
            finalInput="${input}";
        else
            break;
        fi;
    done;
    besharp_rcrvs[besharp_rcsl]="${finalInput}"
}
KeyboardDriver.waitForKeys () 
{ 
    local finalInput=;
    read -s -r -N1 finalInput || true;
    if [[ "${finalInput}" == '' ]]; then
        local input2;
        read -s -r -d '' -t 0.00005 -N2 input2 || true;
        finalInput+="${input2}";
        if [[ "${input2}" == '[M' ]]; then
            local input3;
            read -s -r -d '' -t 0.00005 -N3 input3 || true;
            finalInput+="${input3}";
        fi;
    fi;
    besharp_rcrvs[besharp_rcsl]="${finalInput}"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["KeyboardKeys"]='class'
besharp_oop_types["KeyboardKeys"]="KeyboardKeys"
declare -Ag besharp_oop_type_KeyboardKeys_interfaces
besharp_oop_classes["KeyboardKeys"]="KeyboardKeys"
besharp_oop_type_parent["KeyboardKeys"]="Object"
declare -Ag besharp_oop_type_KeyboardKeys_fields
declare -Ag besharp_oop_type_KeyboardKeys_injectable_fields
declare -Ag besharp_oop_type_KeyboardKeys_field_type
declare -Ag besharp_oop_type_KeyboardKeys_field_default
declare -Ag besharp_oop_type_KeyboardKeys_methods
declare -Ag besharp_oop_type_KeyboardKeys_method_body
declare -Ag besharp_oop_type_KeyboardKeys_method_locals
declare -Ag besharp_oop_type_KeyboardKeys_method_is_returning
declare -Ag besharp_oop_type_KeyboardKeys_method_is_abstract
declare -Ag besharp_oop_type_KeyboardKeys_method_is_using_iterators
declare -Ag besharp_oop_type_KeyboardKeys_method_is_calling_parent
declare -Ag besharp_oop_type_KeyboardKeys_method_is_calling_this
besharp_oop_type_KeyboardKeys_fields['codeToKeys']='codeToKeys'
besharp_oop_type_KeyboardKeys_field_type['codeToKeys']="Map"
besharp_oop_type_KeyboardKeys_field_default['codeToKeys']=""
besharp_oop_type_KeyboardKeys_fields['keys2commands']='keys2commands'
besharp_oop_type_KeyboardKeys_field_type['keys2commands']="Map"
besharp_oop_type_KeyboardKeys_field_default['keys2commands']=""
besharp_oop_type_KeyboardKeys_methods['KeyboardKeys']='KeyboardKeys'
besharp_oop_type_KeyboardKeys_method_is_returning["KeyboardKeys"]=false
besharp_oop_type_KeyboardKeys_method_body["KeyboardKeys"]='    __be___this_codeToKeys @maps.make '"'"''"'"' "esc" '"'"''"'"' "esc" '"'"''"'"' "esc" '"'"'[A'"'"' "up_arrow" '"'"'[B'"'"' "down_arrow" '"'"'[C'"'"' "right_arrow" '"'"'[D'"'"' "left_arrow" '"'"' '"'"' "space" '"'"'
'"'"' "enter" '"'"'	'"'"' "tab" '"'"'[Z'"'"' "shift_tab" '"'"'OP'"'"' "f1";
    __be___this_keys2commands @maps.make "esc" "escape" "up_arrow" "up" "down_arrow" "down" "right_arrow" "right" "left_arrow" "left" "space" "action" "enter" "action" "tab" "next" "shift_tab" "prev" "f1" "help"'
besharp_oop_type_KeyboardKeys_method_locals["KeyboardKeys"]=''
besharp_oop_type_KeyboardKeys_method_is_using_iterators["KeyboardKeys"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_parent["KeyboardKeys"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_this["KeyboardKeys"]=true
besharp_oop_type_KeyboardKeys_methods['commandFromKey']='commandFromKey'
besharp_oop_type_KeyboardKeys_method_is_returning["commandFromKey"]=true
besharp_oop_type_KeyboardKeys_method_body["commandFromKey"]='    local key="${1}";
    __be__keys2commands $this.keys2commands;
    if @true $keys2commands.hasKey "${key}"; then
        besharp.returningOf $keys2commands.get "${key}";
    else
        besharp_rcrvs[besharp_rcsl]='"'"''"'"';
    fi'
besharp_oop_type_KeyboardKeys_method_locals["commandFromKey"]='local keys2commands'
besharp_oop_type_KeyboardKeys_method_is_using_iterators["commandFromKey"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_parent["commandFromKey"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_this["commandFromKey"]=true
besharp_oop_type_KeyboardKeys_methods['keyFromInput']='keyFromInput'
besharp_oop_type_KeyboardKeys_method_is_returning["keyFromInput"]=true
besharp_oop_type_KeyboardKeys_method_body["keyFromInput"]='    local code="${1}";
    if [[ "${code}" =~ '"'"'`'"'"' ]] || [[ "${code}" =~ '"'"'"'"'"' ]]; then
        besharp_rcrvs[besharp_rcsl]="${code}";
        return;
    fi;
    __be__codeToKeys $this.codeToKeys;
    if @false $codeToKeys.hasKey "${code}"; then
        besharp_rcrvs[besharp_rcsl]="${code}";
        return;
    fi;
    besharp.returningOf $codeToKeys.get "${code}"'
besharp_oop_type_KeyboardKeys_method_locals["keyFromInput"]='local codeToKeys'
besharp_oop_type_KeyboardKeys_method_is_using_iterators["keyFromInput"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_parent["keyFromInput"]=false
besharp_oop_type_KeyboardKeys_method_is_calling_this["keyFromInput"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be___this_codeToKeys() {
  "${@}"
  eval $this"_codeToKeys=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_keys2commands() {
  "${@}"
  eval $this"_keys2commands=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__keys2commands() {
  "${@}"
  keys2commands="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__codeToKeys() {
  "${@}"
  codeToKeys="${besharp_rcrvs[besharp_rcsl + 1]}"
}
KeyboardKeys () 
{ 
    __be___this_codeToKeys @maps.make '' "esc" '' "esc" '' "esc" '[A' "up_arrow" '[B' "down_arrow" '[C' "right_arrow" '[D' "left_arrow" ' ' "space" '
' "enter" '	' "tab" '[Z' "shift_tab" 'OP' "f1";
    __be___this_keys2commands @maps.make "esc" "escape" "up_arrow" "up" "down_arrow" "down" "right_arrow" "right" "left_arrow" "left" "space" "action" "enter" "action" "tab" "next" "shift_tab" "prev" "f1" "help"
}
KeyboardKeys.commandFromKey () 
{ 
    local key="${1}";
    __be__keys2commands $this.keys2commands;
    if @true $keys2commands.hasKey "${key}"; then
        besharp.returningOf $keys2commands.get "${key}";
    else
        besharp_rcrvs[besharp_rcsl]='';
    fi
}
KeyboardKeys.keyFromInput () 
{ 
    local code="${1}";
    if [[ "${code}" =~ '`' ]] || [[ "${code}" =~ '"' ]]; then
        besharp_rcrvs[besharp_rcsl]="${code}";
        return;
    fi;
    __be__codeToKeys $this.codeToKeys;
    if @false $codeToKeys.hasKey "${code}"; then
        besharp_rcrvs[besharp_rcsl]="${code}";
        return;
    fi;
    besharp.returningOf $codeToKeys.get "${code}"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["MouseDriver"]='class'
besharp_oop_types["MouseDriver"]="MouseDriver"
declare -Ag besharp_oop_type_MouseDriver_interfaces
besharp_oop_classes["MouseDriver"]="MouseDriver"
besharp_oop_type_parent["MouseDriver"]="Object"
