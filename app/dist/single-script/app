besharp_oop_type_LayersWidget_method_is_calling_this["hideLayer"]=true
besharp_oop_type_LayersWidget_methods['initialize']='initialize'
besharp_oop_type_LayersWidget_method_is_returning["initialize"]=false
besharp_oop_type_LayersWidget_method_body["initialize"]='    __be__coordinator $this.coordinator;
    while @iterate @of $this.layersWidget @in widget; do
        $coordinator.deactivateWidget $widget;
        $coordinator.makeWidgetInvisible $widget;
    done'
besharp_oop_type_LayersWidget_method_locals["initialize"]='local coordinator
local widget'
besharp_oop_type_LayersWidget_method_is_using_iterators["initialize"]=true
besharp_oop_type_LayersWidget_method_is_calling_parent["initialize"]=false
besharp_oop_type_LayersWidget_method_is_calling_this["initialize"]=true
