besharp_oop_type_LayersWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_LayersWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_LayersWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    local maxWidth=-1;
    local maxHeight=-1;
    while @iterate @of $this.layersWidget @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    $calculator.setupSize "${maxWidth}" "${maxHeight}"'
besharp_oop_type_LayersWidget_method_locals["calculateMinSize"]='local children
