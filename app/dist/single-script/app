if ${beshfile_section__code:-false}; then :; 
function __be___this_activeAnimatedWidgets() {
  "${@}"
  eval $this"_activeAnimatedWidgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___this_cloneOfActiveWidgets() {
  "${@}"
  eval $this"_cloneOfActiveWidgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__activeAnimatedWidgets() {
  "${@}"
  activeAnimatedWidgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
