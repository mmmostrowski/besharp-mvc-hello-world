besharp_oop_type_HelloWorldWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_HelloWorldWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_HelloWorldWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "${#text}" 1'
besharp_oop_type_HelloWorldWidget_method_locals["calculateMinSize"]='local text'
besharp_oop_type_HelloWorldWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_HelloWorldWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_HelloWorldWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_HelloWorldWidget_methods['draw']='draw'
besharp_oop_type_HelloWorldWidget_method_is_returning["draw"]=false
besharp_oop_type_HelloWorldWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__canvasPrinter $this.canvasPrinter;
    __be__state $this.state;
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    local format='"'"''"'"';
    if @true $this.isBoldEnabled; then
        __be__format $this.boldFormat;
    fi;
    if @true $this.isColorsEnabled; then
        local i;
        for ((i=0; i < ${#text}; ++i ))
        do
            __be__color @pixel_modes.randomColor;
            $canvas.putPixel "$(( x++ ))" "${y}" "${text:$i:1}" "${format}${color}";
        done;
    else
        __be__color @pixel_modes.white;
        $canvasPrinter.printText $canvas "${x}" "${y}" "${text}" "${format}${color}";
    fi'
besharp_oop_type_HelloWorldWidget_method_locals["draw"]='local canvasPrinter
local color
local format
local state
local text
local x
local y'
