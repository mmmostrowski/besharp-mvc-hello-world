  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
RadioWidget () 
{ 
    $this.valueKey = "${1}"
}
RadioWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
RadioWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__text $this.text;
    $calculator.setupSize "${#text}" 1
}
RadioWidget.check () 
{ 
    __be__state $this.state;
    __be__valueKey $this.valueKey;
    $state.writeWidgetState $this 'value' "${valueKey}"
}
RadioWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__canvasPrinter $this.canvasPrinter;
    if @true $context.focusedFlag; then
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" "-(*) ${text}-";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" "-( ) ${text}-";
        fi;
    else
        if @true $this.isChecked; then
            $canvasPrinter.printText $canvas "${x}" "${y}" " (*) ${text} ";
        else
            $canvasPrinter.printText $canvas "${x}" "${y}" " ( ) ${text} ";
        fi;
    fi
}
RadioWidget.initialize () 
{ 
    :
}
RadioWidget.isChecked () 
{ 
    __be__state $this.state;
    __be__valueKey $this.valueKey;
    if @returned @of $state.readWidgetState $this 'value' '' == "${valueKey}"; then
        besharp_rcrvs[besharp_rcsl]=true;
    else
        besharp_rcrvs[besharp_rcsl]=false;
    fi
}
RadioWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    if @true $event.consumeWhenCommandRequested 'action'; then
        $this.check;
    fi
}
RadioWidget.text () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'text' ''
}
RadioWidget.widgetHeight () 
{ 
    besharp_rcrvs[besharp_rcsl]=1
}
RadioWidget.widgetWidth () 
{ 
    __be__text $this.text;
    besharp_rcrvs[besharp_rcsl]=$(( ${#text} + 6 ))
}

fi
