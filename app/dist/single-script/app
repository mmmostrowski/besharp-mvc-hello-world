    local temp;
    declare -n temp="${this}";
    local idx;
    local found=false;
    for idx in "${!temp[@]}";
    do
        if (( maxIndex >= 0 && idx > maxIndex )); then
            continue;
        fi;
        if @same "${temp[$idx]}" "${item}"; then
            found=true;
