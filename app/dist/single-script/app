if ${beshfile_section__code:-false}; then :; 
function __be__text() {
  "${@}"
  text="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__canvasPrinter() {
  "${@}"
  canvasPrinter="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__clickAnimationDataLength() {
  "${@}"
  clickAnimationDataLength="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__clickAnimationDataStart() {
  "${@}"
  clickAnimationDataStart="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__clickAnimationFrame() {
  "${@}"
  clickAnimationFrame="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__focusedStyle() {
  "${@}"
  focusedStyle="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__highlightLength() {
  "${@}"
  highlightLength="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__highlightStart() {
  "${@}"
  highlightStart="${besharp_rcrvs[besharp_rcsl + 1]}"
}
