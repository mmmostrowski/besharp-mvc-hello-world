if ${beshfile_section__code:-false}; then :; 
function __be___this_widgets() {
  "${@}"
  eval $this"_widgets=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__maxHeight() {
  "${@}"
  maxHeight="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__maxWidth() {
  "${@}"
  maxWidth="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__backgroundColor() {
  "${@}"
  backgroundColor="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__dimReset() {
  "${@}"
  dimReset="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__shadowColor() {
  "${@}"
  shadowColor="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__title() {
  "${@}"
  title="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widget() {
  "${@}"
  widget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgets() {
  "${@}"
  widgets="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widgetsSize() {
  "${@}"
  widgetsSize="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__height() {
  "${@}"
  height="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__width() {
  "${@}"
  width="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x1() {
  "${@}"
  x1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y1() {
  "${@}"
  y1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__state() {
  "${@}"
  state="${besharp_rcrvs[besharp_rcsl + 1]}"
}
PopupFrameWidget () 
{ 
    __be___this_widgets @vectors.make "${@}"
}
PopupFrameWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
PopupFrameWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    local maxWidth=-1;
    local maxHeight=-1;
    while @iterate @of $this.widgets @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    (( ++maxWidth));
    (( ++maxHeight ));
    $calculator.setupSize "${maxWidth}" "${maxHeight}"
}
PopupFrameWidget.childrenWidgets () 
{ 
    besharp.returningOf $this.widgets
}
PopupFrameWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    eval "
            local x1=\"\${${context}_x1}\"
            local y1=\"\${${context}_y1}\"
            local width=\"\${${context}_width}\"
            local height=\"\${${context}_height}\"
            local canvasPrinter=\"\${${this}_canvasPrinter}\"
        ";
    __be__dimReset @pixel_modes.dimReset;
    __be__backgroundColor @pixel_modes.blue;
    $canvas.putPixelsBox "$(( x1 + 1 ))" "$(( y1 + 1 ))" "$(( width - 2 ))" "$(( height - 2 ))" "." "${backgroundColor}";
    if @true $this.isShadowsRenderingEnabled; then
        __be__shadowColor @pixel_modes.dGray;
        $canvas.putPixelsHorizModes "$(( x1 + 1  ))" "$(( y1 + height ))" "${width}" "${shadowColor}";
        $canvas.putPixelsVertModes "$(( x1 + width ))" "$(( y1 + 1 ))" "${height}" "${shadowColor}";
        $canvas.putPixelsVertModes "$(( x1 + width + 1))" "$(( y1 + 1 ))" "${height}" "${shadowColor}";
        $canvas.putPixelModes "$(( x1 + width - 1 ))" "$(( y1 + height - 1 ))" "${shadowColor}";
    fi;
    __be__title $this.title;
    if @true $context.isHighlighted; then
        $canvasPrinter.printHighlightFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    else
        $canvasPrinter.printFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    fi;
    $canvasPrinter.printText $canvas $(( x1 + 3 )) "${y1}" " ${title} ";
    __be__widgets $this.widgets;
    __be__widgetsSize $widgets.size;
    local idx=0;
    while (( idx < widgetsSize )); do
        __be__widget $widgets.get "$(( idx++ ))";
        $widgetsLayout.drawWidget $widget $canvas;
    done
}
PopupFrameWidget.initialize () 
{ 
    :
}
PopupFrameWidget.isShadowsRenderingEnabled () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'shadowsEnabled' true
}
PopupFrameWidget.operativeChildrenWidget () 
{ 
    besharp.returningOf $this.widgets
}
PopupFrameWidget.organizeChildrenOnLayout () 
{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    while @iterate @of $this.widgets @in children; do
        $widgetsLayout.setupContextWindow $children "$(( x1 + 1 ))" "$(( y1 + 1 ))" "$(( width - 2 ))" "$(( height - 2 ))";
    done
}
PopupFrameWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    if @true $event.consumeWhenCommandRequested 'escape'; then
        __be__state $this.state;
        $state.requestWidgetEvent $this 'on_escape';
    fi
}
PopupFrameWidget.title () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'title' ''
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["TabsWidget"]='class'
besharp_oop_types["TabsWidget"]="TabsWidget"
declare -Ag besharp_oop_type_TabsWidget_interfaces
besharp_oop_type_TabsWidget_interfaces['Widget']='Widget'
besharp_oop_type_TabsWidget_interfaces['ContainerWidget']='ContainerWidget'
besharp_oop_type_TabsWidget_interfaces['FocusableWidget']='FocusableWidget'
besharp_oop_classes["TabsWidget"]="TabsWidget"
besharp_oop_type_parent["TabsWidget"]="Object"
declare -Ag besharp_oop_type_TabsWidget_fields
declare -Ag besharp_oop_type_TabsWidget_injectable_fields
declare -Ag besharp_oop_type_TabsWidget_field_type
declare -Ag besharp_oop_type_TabsWidget_field_default
declare -Ag besharp_oop_type_TabsWidget_methods
declare -Ag besharp_oop_type_TabsWidget_method_body
declare -Ag besharp_oop_type_TabsWidget_method_locals
declare -Ag besharp_oop_type_TabsWidget_method_is_returning
declare -Ag besharp_oop_type_TabsWidget_method_is_abstract
declare -Ag besharp_oop_type_TabsWidget_method_is_using_iterators
declare -Ag besharp_oop_type_TabsWidget_method_is_calling_parent
declare -Ag besharp_oop_type_TabsWidget_method_is_calling_this
besharp_oop_type_TabsWidget_fields['printer']='printer'
besharp_oop_type_TabsWidget_field_type['printer']="CanvasPrinter"
besharp_oop_type_TabsWidget_injectable_fields['printer']='printer'
besharp_oop_type_TabsWidget_field_default['printer']=""
besharp_oop_type_TabsWidget_fields['coordinator']='coordinator'
besharp_oop_type_TabsWidget_field_type['coordinator']="WidgetsCoordinator"
besharp_oop_type_TabsWidget_injectable_fields['coordinator']='coordinator'
besharp_oop_type_TabsWidget_field_default['coordinator']=""
besharp_oop_type_TabsWidget_fields['widgets']='widgets'
besharp_oop_type_TabsWidget_field_type['widgets']="Vector"
besharp_oop_type_TabsWidget_field_default['widgets']=""
besharp_oop_type_TabsWidget_fields['widgetPositions']='widgetPositions'
besharp_oop_type_TabsWidget_field_type['widgetPositions']="Map"
besharp_oop_type_TabsWidget_field_default['widgetPositions']=""
besharp_oop_type_TabsWidget_fields['tabNames']='tabNames'
besharp_oop_type_TabsWidget_field_type['tabNames']="Vector"
besharp_oop_type_TabsWidget_field_default['tabNames']=""
besharp_oop_type_TabsWidget_fields['activeTab']='activeTab'
besharp_oop_type_TabsWidget_field_default['activeTab']="-1"
besharp_oop_type_TabsWidget_fields['activeOne']='activeOne'
besharp_oop_type_TabsWidget_field_type['activeOne']="Vector"
besharp_oop_type_TabsWidget_field_default['activeOne']=""
besharp_oop_type_TabsWidget_fields['activeWidget']='activeWidget'
besharp_oop_type_TabsWidget_field_type['activeWidget']="Widget"
besharp_oop_type_TabsWidget_field_default['activeWidget']=""
besharp_oop_type_TabsWidget_methods['TabsWidget']='TabsWidget'
besharp_oop_type_TabsWidget_method_is_returning["TabsWidget"]=false
besharp_oop_type_TabsWidget_method_body["TabsWidget"]='    __be___this_widgets @vectors.make;
    __be___this_widgetPositions @maps.make;
    __be___this_tabNames @vectors.make;
    __be___this_activeOne @vectors.make "";
    local name;
    local isEven=true;
    for item in "${@}";
    do
        if $isEven; then
            isEven=false;
            name="${item}";
        else
            isEven=true;
            $this.addTab "${name}" "${item}";
        fi;
    done'
besharp_oop_type_TabsWidget_method_locals["TabsWidget"]=''
besharp_oop_type_TabsWidget_method_is_using_iterators["TabsWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["TabsWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["TabsWidget"]=true
besharp_oop_type_TabsWidget_methods['addTab']='addTab'
besharp_oop_type_TabsWidget_method_is_returning["addTab"]=false
besharp_oop_type_TabsWidget_method_body["addTab"]='    local name="${1}";
    local widget="${2}";
    __be__widgets $this.widgets;
    if @true $widgets.isEmpty; then
        $this.activeTab = 0;
    fi;
    __be__tabNames $this.tabNames;
    __be__widgetPositions $this.widgetPositions;
    __be__idx $widgets.size;
    $widgets.add $widget;
    $tabNames.add "${name}";
    $widgetPositions.set $widget "${idx}"'
besharp_oop_type_TabsWidget_method_locals["addTab"]='local idx
local tabNames
local widgetPositions
local widgets'
besharp_oop_type_TabsWidget_method_is_using_iterators["addTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["addTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["addTab"]=true
besharp_oop_type_TabsWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_TabsWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_TabsWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    local maxWidth=0;
    while @iterate @of $this.tabNames @in tabName; do
        (( maxWidth += ( ${#tabName} + 10 ) ));
    done;
    local maxHeight=-1;
    while @iterate @of $this.widgets @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    (( ++maxHeight ));
    $calculator.setupSize "${maxWidth}" "${maxHeight}"'
besharp_oop_type_TabsWidget_method_locals["calculateMinSize"]='local children
local maxHeight
local maxWidth
local tabName'
besharp_oop_type_TabsWidget_method_is_using_iterators["calculateMinSize"]=true
besharp_oop_type_TabsWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_TabsWidget_methods['childrenWidgets']='childrenWidgets'
besharp_oop_type_TabsWidget_method_is_returning["childrenWidgets"]=true
besharp_oop_type_TabsWidget_method_body["childrenWidgets"]='    besharp.returningOf $this.widgets'
besharp_oop_type_TabsWidget_method_locals["childrenWidgets"]=''
besharp_oop_type_TabsWidget_method_is_using_iterators["childrenWidgets"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["childrenWidgets"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["childrenWidgets"]=true
besharp_oop_type_TabsWidget_methods['draw']='draw'
besharp_oop_type_TabsWidget_method_is_returning["draw"]=true
besharp_oop_type_TabsWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__activeWidget $this.activeWidget;
    __be__activeTab $this.activeTab;
    if [[ -z "${activeWidget}" ]] || [[ -z "${activeTab}" ]]; then
        return;
    fi;
    __be__printer $this.printer;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__isFocused $context.focusedFlag;
    __be__isHighlighted $context.isHighlighted;
    __be__activeColor @pixel_modes.white;
    __be__inactiveColor @pixel_modes.dGray;
    __be__tabNames $this.tabNames;
    __be__tabNamesSize $tabNames.size;
    (( ++x1 ));
    if $isHighlighted; then
        if $isFocused; then
            local idx=0;
            while (( idx < tabNamesSize )); do
                __be__tabName $tabNames.get "${idx}";
                if (( idx++ == activeTab )); then
                    $printer.printText $canvas "${x1}" "${y1}" "[ < ${tabName} > ]" "${activeColor}";
                    (( x1 += ${#tabName} ));
                    (( x1 += 10 ));
                else
                    $printer.printText $canvas "${x1}" "${y1}" "[ ${tabName} ]" "${inactiveColor}";
                    (( x1 += ${#tabName} ));
                    (( x1 += 6 ));
                fi;
            done;
        else
            local idx=0;
            while (( idx < tabNamesSize )); do
                __be__tabName $tabNames.get "$(( idx++ ))";
                $printer.printText $canvas "${x1}" "${y1}" "[ ${tabName} ]" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            done;
        fi;
    else
        local idx=0;
        while (( idx < tabNamesSize )); do
            __be__tabName $tabNames.get "${idx}";
            if (( idx++ == activeTab )); then
                $printer.printText $canvas "${x1}" "${y1}" "| ${tabName} |" "${activeColor}" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            else
                $printer.printText $canvas "${x1}" "${y1}" "| ${tabName} |" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            fi;
        done;
    fi;
    $widgetsLayout.drawWidget $activeWidget $canvas'
besharp_oop_type_TabsWidget_method_locals["draw"]='local activeColor
local activeTab
local activeWidget
local inactiveColor
local isFocused
local isHighlighted
local printer
local tabName
local tabNames
local tabNamesSize
local x1
local y1'
besharp_oop_type_TabsWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["draw"]=true
besharp_oop_type_TabsWidget_methods['initialize']='initialize'
besharp_oop_type_TabsWidget_method_is_returning["initialize"]=false
besharp_oop_type_TabsWidget_method_body["initialize"]='    __be__coordinator $this.coordinator;
    __be__widgets $this.widgets;
    while @iterate @of $this.widgets @in widget; do
        $coordinator.deactivateWidget $widget;
        $coordinator.makeWidgetInvisible $widget;
    done;
    __be__activeTab $this.activeTab;
    if (( activeTab >= 0 )); then
        $this.selectTab "${activeTab}";
    fi'
besharp_oop_type_TabsWidget_method_locals["initialize"]='local activeTab
local coordinator
local widget
local widgets'
besharp_oop_type_TabsWidget_method_is_using_iterators["initialize"]=true
besharp_oop_type_TabsWidget_method_is_calling_parent["initialize"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["initialize"]=true
besharp_oop_type_TabsWidget_methods['operativeChildrenWidget']='operativeChildrenWidget'
besharp_oop_type_TabsWidget_method_is_returning["operativeChildrenWidget"]=true
besharp_oop_type_TabsWidget_method_body["operativeChildrenWidget"]='    besharp.returningOf $this.activeOne'
besharp_oop_type_TabsWidget_method_locals["operativeChildrenWidget"]=''
besharp_oop_type_TabsWidget_method_is_using_iterators["operativeChildrenWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["operativeChildrenWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["operativeChildrenWidget"]=true
besharp_oop_type_TabsWidget_methods['organizeChildrenOnLayout']='organizeChildrenOnLayout'
besharp_oop_type_TabsWidget_method_is_returning["organizeChildrenOnLayout"]=false
besharp_oop_type_TabsWidget_method_body["organizeChildrenOnLayout"]='    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    (( ++y1 ));
    (( --height ));
    while @iterate @of $this.widgets @in children; do
        $widgetsLayout.setupContextWindow $children "${x1}" "${y1}" "${width}" "${height}";
    done'
besharp_oop_type_TabsWidget_method_locals["organizeChildrenOnLayout"]='local children
local height
local width
local x1
local y1'
besharp_oop_type_TabsWidget_method_is_using_iterators["organizeChildrenOnLayout"]=true
besharp_oop_type_TabsWidget_method_is_calling_parent["organizeChildrenOnLayout"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["organizeChildrenOnLayout"]=true
besharp_oop_type_TabsWidget_methods['processKeyboardEvent']='processKeyboardEvent'
besharp_oop_type_TabsWidget_method_is_returning["processKeyboardEvent"]=false
besharp_oop_type_TabsWidget_method_body["processKeyboardEvent"]='    local event="${1}";
    local isDirect="${2}";
    if $isDirect && @true $event.consumeWhenCommandRequested '"'"'down'"'"' '"'"'right'"'"'; then
        $this.selectNextTab;
    else
        if $isDirect && @true $event.consumeWhenCommandRequested '"'"'up'"'"' '"'"'left'"'"'; then
            $this.selectPrevTab;
        fi;
    fi'
besharp_oop_type_TabsWidget_method_locals["processKeyboardEvent"]=''
besharp_oop_type_TabsWidget_method_is_using_iterators["processKeyboardEvent"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["processKeyboardEvent"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["processKeyboardEvent"]=true
besharp_oop_type_TabsWidget_methods['selectNextTab']='selectNextTab'
besharp_oop_type_TabsWidget_method_is_returning["selectNextTab"]=false
besharp_oop_type_TabsWidget_method_body["selectNextTab"]='    __be__tab $this.activeTab;
    __be__widgets $this.widgets;
    __be__tabsNum $widgets.size;
    if (( ++tab >= tabsNum )); then
        tab=0;
    fi;
    $this.selectTab "${tab}"'
besharp_oop_type_TabsWidget_method_locals["selectNextTab"]='local tab
local tabsNum
local widgets'
besharp_oop_type_TabsWidget_method_is_using_iterators["selectNextTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["selectNextTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["selectNextTab"]=true
besharp_oop_type_TabsWidget_methods['selectPrevTab']='selectPrevTab'
besharp_oop_type_TabsWidget_method_is_returning["selectPrevTab"]=false
besharp_oop_type_TabsWidget_method_body["selectPrevTab"]='    __be__tab $this.activeTab;
    __be__widgets $this.widgets;
    __be__tabsNum $widgets.size;
    if (( --tab < 0 )); then
        tab=$(( tabsNum - 1 ));
    fi;
    $this.selectTab "${tab}"'
besharp_oop_type_TabsWidget_method_locals["selectPrevTab"]='local tab
local tabsNum
local widgets'
besharp_oop_type_TabsWidget_method_is_using_iterators["selectPrevTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["selectPrevTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["selectPrevTab"]=true
besharp_oop_type_TabsWidget_methods['selectTab']='selectTab'
besharp_oop_type_TabsWidget_method_is_returning["selectTab"]=false
besharp_oop_type_TabsWidget_method_body["selectTab"]='    local tab="${1}";
    __be__coordinator $this.coordinator;
    __be__previousActive $this.activeTab;
    if (( previousActive >= 0 )); then
        __be__previousWidget $this.activeWidget;
        if [[ -n $previousWidget ]]; then
            $coordinator.deactivateWidget $previousWidget;
            $coordinator.makeWidgetInvisible $previousWidget;
        fi;
    fi;
    __be__widgets $this.widgets;
    __be__newWidget $widgets.get "${tab}";
    $this.activeTab = "${tab}";
    __be__activeOne $this.activeOne;
    $activeOne.set 0 $newWidget;
    $this.activeWidget = $newWidget;
    $coordinator.activateWidget $newWidget;
    $coordinator.makeWidgetVisible $newWidget'
besharp_oop_type_TabsWidget_method_locals["selectTab"]='local activeOne
local coordinator
local newWidget
local previousActive
local previousWidget
local widgets'
besharp_oop_type_TabsWidget_method_is_using_iterators["selectTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["selectTab"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["selectTab"]=true
besharp_oop_type_TabsWidget_methods['selectWidget']='selectWidget'
besharp_oop_type_TabsWidget_method_is_returning["selectWidget"]=false
besharp_oop_type_TabsWidget_method_body["selectWidget"]='    local widget="${1}";
    __be__idx $widgetPositions.get $widget;
    $this.selectTab "${idx}"'
besharp_oop_type_TabsWidget_method_locals["selectWidget"]='local idx'
besharp_oop_type_TabsWidget_method_is_using_iterators["selectWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_parent["selectWidget"]=false
besharp_oop_type_TabsWidget_method_is_calling_this["selectWidget"]=true

fi
