    local x1="${2}";
    local y1="${3}";
    local width="${4}";
    local height="${5}";
    shift 5;
    __be__color @pixel_modes.dGray;
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" "${y1}" "$(( width - 2 ))" "-" "${@}" "${color}";
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" $(( y1 + height - 1)) "$(( width - 2 ))" "-" "${@}" "${color}";
    $canvas.putPixelsVert "${x1}" "$(( y1 + 1 ))" "$(( height - 2 ))" "|" "${@}" "${color}";
    $canvas.putPixelsVert $(( x1 + width - 1 )) "$(( y1 + 1 ))" "$(( height - 2 ))" "|" "${@}" "${color}"'
besharp_oop_type_CanvasPrinter_method_locals["printFrame"]='local color'
besharp_oop_type_CanvasPrinter_method_is_using_iterators["printFrame"]=false
besharp_oop_type_CanvasPrinter_method_is_calling_parent["printFrame"]=false
besharp_oop_type_CanvasPrinter_method_is_calling_this["printFrame"]=false
besharp_oop_type_CanvasPrinter_methods['printHighlightFrame']='printHighlightFrame'
besharp_oop_type_CanvasPrinter_method_is_returning["printHighlightFrame"]=false
besharp_oop_type_CanvasPrinter_method_body["printHighlightFrame"]='    local canvas="${1}";
    local x1="${2}";
    local y1="${3}";
    local width="${4}";
    local height="${5}";
    shift 5;
    __be__color @pixel_modes.white;
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" "${y1}" "$(( width - 2 ))" "=" "${@}" "${color}";
    $canvas.putPixelsHoriz "$(( x1 + 1 ))" $(( y1 + height - 1)) "$(( width - 2 ))" "=" "${@}" "${color}";
    $canvas.putPixelsVert "${x1}" "$(( y1 + 1 ))" "$(( height - 2 ))" "[" "${@}" "${color}";
    $canvas.putPixelsVert $(( x1 + width - 1 )) "$(( y1 + 1 ))" "$(( height - 2 ))" "]" "${@}" "${color}"'
besharp_oop_type_CanvasPrinter_method_locals["printHighlightFrame"]='local color'
