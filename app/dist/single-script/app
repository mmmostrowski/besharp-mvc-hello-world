if ${beshfile_section__code:-false}; then :; 
function __be__bgReset() {
  "${@}"
  bgReset="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__reset() {
  "${@}"
  reset="${besharp_rcrvs[besharp_rcsl + 1]}"
}
PixelModes () 
{ 
    __be__bgReset $this.bgBlack;
    __be__reset $this.reset;
    $this.reset = "${reset}${bgReset}"
}
PixelModes.randomColor () 
{ 
    case $(( RANDOM % 14 )) in 
        0)
            besharp.returningOf $this.red
        ;;
        1)
            besharp.returningOf $this.green
        ;;
        2)
            besharp.returningOf $this.blue
        ;;
        3)
            besharp.returningOf $this.yellow
        ;;
        4)
            besharp.returningOf $this.magenta
        ;;
        5)
            besharp.returningOf $this.cyan
        ;;
        6)
            besharp.returningOf $this.lRed
        ;;
        7)
            besharp.returningOf $this.lGreen
        ;;
        8)
            besharp.returningOf $this.lBlue
        ;;
        9)
            besharp.returningOf $this.lYellow
        ;;
        10)
            besharp.returningOf $this.lMagenta
        ;;
        11)
            besharp.returningOf $this.lCyan
        ;;
        12)
            besharp.returningOf $this.lGray
        ;;
        13)
            besharp.returningOf $this.dGray
        ;;
    esac
}

fi
