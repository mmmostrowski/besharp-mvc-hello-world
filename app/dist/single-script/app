if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["InputWidget"]='class'
besharp_oop_types["InputWidget"]="InputWidget"
declare -Ag besharp_oop_type_InputWidget_interfaces
besharp_oop_type_InputWidget_interfaces['Widget']='Widget'
besharp_oop_type_InputWidget_interfaces['WidgetWithSize']='WidgetWithSize'
besharp_oop_type_InputWidget_interfaces['FocusableWidget']='FocusableWidget'
besharp_oop_type_InputWidget_interfaces['StatefulWidget']='StatefulWidget'
besharp_oop_classes["InputWidget"]="InputWidget"
besharp_oop_type_parent["InputWidget"]="Object"
declare -Ag besharp_oop_type_InputWidget_fields
declare -Ag besharp_oop_type_InputWidget_injectable_fields
declare -Ag besharp_oop_type_InputWidget_field_type
declare -Ag besharp_oop_type_InputWidget_field_default
declare -Ag besharp_oop_type_InputWidget_methods
declare -Ag besharp_oop_type_InputWidget_method_body
declare -Ag besharp_oop_type_InputWidget_method_locals
declare -Ag besharp_oop_type_InputWidget_method_is_returning
declare -Ag besharp_oop_type_InputWidget_method_is_abstract
declare -Ag besharp_oop_type_InputWidget_method_is_using_iterators
declare -Ag besharp_oop_type_InputWidget_method_is_calling_parent
declare -Ag besharp_oop_type_InputWidget_method_is_calling_this
besharp_oop_type_InputWidget_fields['canvasPrinter']='canvasPrinter'
besharp_oop_type_InputWidget_field_type['canvasPrinter']="CanvasPrinter"
besharp_oop_type_InputWidget_injectable_fields['canvasPrinter']='canvasPrinter'
besharp_oop_type_InputWidget_field_default['canvasPrinter']=""
besharp_oop_type_InputWidget_fields['cursor']='cursor'
besharp_oop_type_InputWidget_field_type['cursor']="Cursor"
besharp_oop_type_InputWidget_injectable_fields['cursor']='cursor'
besharp_oop_type_InputWidget_field_default['cursor']=""
besharp_oop_type_InputWidget_fields['length']='length'
besharp_oop_type_InputWidget_field_default['length']=""
besharp_oop_type_InputWidget_fields['isEditing']='isEditing'
besharp_oop_type_InputWidget_field_default['isEditing']=""
besharp_oop_type_InputWidget_fields['emptyText']='emptyText'
besharp_oop_type_InputWidget_field_default['emptyText']=""
besharp_oop_type_InputWidget_fields['state']='state'
besharp_oop_type_InputWidget_field_type['state']="WidgetStateKeeper"
besharp_oop_type_InputWidget_field_default['state']=""
besharp_oop_type_InputWidget_methods['InputWidget']='InputWidget'
besharp_oop_type_InputWidget_method_is_returning["InputWidget"]=false
besharp_oop_type_InputWidget_method_body["InputWidget"]='    $this.length = "${1}";
    local emptyText='"'"''"'"';
    local i;
    for ((i = 0; i < ${1}; ++i ))
    do
        emptyText+=" ";
    done;
    $this.emptyText = "${emptyText}"'
besharp_oop_type_InputWidget_method_locals["InputWidget"]=''
besharp_oop_type_InputWidget_method_is_using_iterators["InputWidget"]=false
besharp_oop_type_InputWidget_method_is_calling_parent["InputWidget"]=false
besharp_oop_type_InputWidget_method_is_calling_this["InputWidget"]=true
besharp_oop_type_InputWidget_methods['bindWidgetState']='bindWidgetState'
besharp_oop_type_InputWidget_method_is_returning["bindWidgetState"]=false
besharp_oop_type_InputWidget_method_body["bindWidgetState"]='    $this.state = "${1}"'
besharp_oop_type_InputWidget_method_locals["bindWidgetState"]=''
besharp_oop_type_InputWidget_method_is_using_iterators["bindWidgetState"]=false
besharp_oop_type_InputWidget_method_is_calling_parent["bindWidgetState"]=false
besharp_oop_type_InputWidget_method_is_calling_this["bindWidgetState"]=true
besharp_oop_type_InputWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_InputWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_InputWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    __be__width $this.widgetWidth;
    $calculator.setupSize "${width}" 1'
besharp_oop_type_InputWidget_method_locals["calculateMinSize"]='local width'
besharp_oop_type_InputWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_InputWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_InputWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_InputWidget_methods['draw']='draw'
besharp_oop_type_InputWidget_method_is_returning["draw"]=false
besharp_oop_type_InputWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__length $this.length;
    __be__label $this.label;
    __be__text $this.text;
    __be__x $context.x1;
    __be__y $context.y1;
    __be__cursor $this.cursor;
    __be__emptyText $this.emptyText;
    __be__canvasPrinter $this.canvasPrinter;
    if @true $context.focusedFlag; then
        __be__color @pixel_modes.white;
        $canvasPrinter.printText $canvas "${x}" "${y}" " ${label}:-| ${emptyText} |-" "${color}";
    else
        __be__color @pixel_modes.dGray;
        $canvasPrinter.printText $canvas "${x}" "${y}" " ${label}: | ${emptyText} |" "${color}";
    fi;
    local caretPos="$(( x + ${#label} + 5 ))";
    $canvasPrinter.printText $canvas "${caretPos}" "${y}" "${text}" "${color}";
    if @true $this.isEditing; then
        $cursor.show;
        local input="${text}";
        local previousIFS="${IFS}";
        IFS='"'"''"'"';
        while true; do
            $cursor.moveTo "${caretPos}" $(( y - 1 ));
            read -r -e -i "${input}" -n $(( length + 1 )) input;
            if (( "${#input}" <= length )); then
                break;
            fi;
            input="${input::-1}";
            $cursor.moveTo "$(( caretPos + length ))" $(( y - 1 ));
            echo -ne '"'"' '"'"';
        done;
        IFS="${previousIFS}";
        $this.updateText "${input}";
        $cursor.hide;
        $this.isEditing = false;
        $widgetsLayout.needsRedraw = true;
    fi'
besharp_oop_type_InputWidget_method_locals["draw"]='local canvasPrinter
local color
local cursor
local emptyText
local label
local length
local text
local x
