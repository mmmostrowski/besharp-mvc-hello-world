    local size;
    eval "size=\${#${varName}[@]}";
    if (( $2 < size - 1 )); then
        besharp_rcrvs[besharp_rcsl]=$(( $2 + 1 ));
    else
        besharp_rcrvs[besharp_rcsl]='"'"''"'"';
    fi'
