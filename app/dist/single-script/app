    local value="${2}";
    if @is $iterable @a Container; then
        besharp.returningOf $iterable.has "${value}";
    else
        local subjectItem;
        while @iterate $iterable @in subjectItem; do
            if [[ "${subjectItem}" == "${value}" ]]; then
                besharp_rcrvs[besharp_rcsl]=true;
                return;
            fi;
        done;
        besharp_rcrvs[besharp_rcsl]=false;
    fi'
