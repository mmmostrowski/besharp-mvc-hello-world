    __be__activeWidgets $this.activeWidgets;
    __be__key2widget $this.key2widget;
    __be__widget $key2widget.get "${layerKey}";
    $activeWidgets.add $widget;
    __be__coordinator $this.coordinator;
    $coordinator.activateWidget $widget;
    __be__activeLayers $this.activeLayers;
    $activeLayers.add "${layerKey}"'
besharp_oop_type_LayersWidget_method_locals["activateLayer"]='local activeLayers
local activeWidgets
