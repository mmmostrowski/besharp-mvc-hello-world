besharp_oop_type_KeyboardDriver_method_is_calling_this["scanKeys"]=false
besharp_oop_type_KeyboardDriver_methods['waitForKeys']='waitForKeys'
besharp_oop_type_KeyboardDriver_method_is_returning["waitForKeys"]=true
besharp_oop_type_KeyboardDriver_method_body["waitForKeys"]='    local finalInput=;
    read -s -r -N1 finalInput || true;
    if [[ "${finalInput}" == '"'"''"'"' ]]; then
        local input2;
        read -s -r -d '"'"''"'"' -t 0.00005 -N2 input2 || true;
        finalInput+="${input2}";
        if [[ "${input2}" == '"'"'[M'"'"' ]]; then
            local input3;
            read -s -r -d '"'"''"'"' -t 0.00005 -N3 input3 || true;
            finalInput+="${input3}";
        fi;
    fi;
    besharp_rcrvs[besharp_rcsl]="${finalInput}"'
besharp_oop_type_KeyboardDriver_method_locals["waitForKeys"]=''
besharp_oop_type_KeyboardDriver_method_is_using_iterators["waitForKeys"]=false
besharp_oop_type_KeyboardDriver_method_is_calling_parent["waitForKeys"]=false
besharp_oop_type_KeyboardDriver_method_is_calling_this["waitForKeys"]=false

