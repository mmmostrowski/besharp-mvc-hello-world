    besharp.returningOf $state.readWidgetState $this '"'"'text'"'"' '"'"''"'"''
besharp_oop_type_RadioWidget_method_locals["text"]='local state'
besharp_oop_type_RadioWidget_method_is_using_iterators["text"]=false
