besharp_oop_type_Ball_method_is_returning["move"]=false
besharp_oop_type_Ball_method_body["move"]='    local w="${1}";
    local h="${2}";
    eval "
          local x=\"\${${this}_x}\"
          local y=\"\${${this}_y}\"
          local dx=\"\${${this}_dx}\"
          local dy=\"\${${this}_dy}\"
        ";
    (( x += dx )) || true;
    (( y += dy )) || true;
    if (( x < 0 )); then
        x=0;
        dx="-${dx}";
    else
        if (( x >= w )); then
            x=$(( w - 1 ));
            dx="-${dx}";
        fi;
    fi;
    if (( y < 0 )); then
        y=0;
        dy="-${dy}";
    else
        if (( y >= h )); then
            y=$(( h - 1 ));
            dy="-${dy}";
        fi;
    fi;
    eval "
            ${this}_x=\"\${x}\"
            ${this}_y=\"\${y}\"
            ${this}_dx=\"\${dx}\"
            ${this}_dy=\"\${dy}\"
        "'
besharp_oop_type_Ball_method_locals["move"]=''
besharp_oop_type_Ball_method_is_using_iterators["move"]=false
besharp_oop_type_Ball_method_is_calling_parent["move"]=false
besharp_oop_type_Ball_method_is_calling_this["move"]=true
besharp_oop_type_Ball_methods['place']='place'
besharp_oop_type_Ball_method_is_returning["place"]=true
besharp_oop_type_Ball_method_body["place"]='    local w="${1}";
    local h="${2}";
    if (( w == 0 )) || (( h == 0 )); then
        return;
    fi;
    $this.x = $(( RANDOM % w ));
    $this.y = $(( RANDOM % h ));
    if (( RANDOM % 2 )); then
        $this.dx = 1;
    else
        $this.dx = -1;
    fi;
    if (( RANDOM % 2 )); then
        $this.dy = 1;
    else
        $this.dy = -1;
    fi'
besharp_oop_type_Ball_method_locals["place"]=''
besharp_oop_type_Ball_method_is_using_iterators["place"]=false
besharp_oop_type_Ball_method_is_calling_parent["place"]=false
besharp_oop_type_Ball_method_is_calling_this["place"]=true

fi
if ${beshfile_section__code:-false}; then :; 
Ball () 
{ 
    @parent "${@}"
}
Ball.move () 
{ 
    local w="${1}";
    local h="${2}";
    eval "
          local x=\"\${${this}_x}\"
          local y=\"\${${this}_y}\"
          local dx=\"\${${this}_dx}\"
          local dy=\"\${${this}_dy}\"
        ";
    (( x += dx )) || true;
    (( y += dy )) || true;
    if (( x < 0 )); then
        x=0;
        dx="-${dx}";
    else
        if (( x >= w )); then
            x=$(( w - 1 ));
            dx="-${dx}";
        fi;
    fi;
    if (( y < 0 )); then
        y=0;
        dy="-${dy}";
    else
        if (( y >= h )); then
            y=$(( h - 1 ));
            dy="-${dy}";
        fi;
    fi;
    eval "
            ${this}_x=\"\${x}\"
            ${this}_y=\"\${y}\"
            ${this}_dx=\"\${dx}\"
            ${this}_dy=\"\${dy}\"
        "
}
Ball.place () 
{ 
    local w="${1}";
    local h="${2}";
    if (( w == 0 )) || (( h == 0 )); then
        return;
    fi;
    $this.x = $(( RANDOM % w ));
    $this.y = $(( RANDOM % h ));
    if (( RANDOM % 2 )); then
        $this.dx = 1;
    else
        $this.dx = -1;
    fi;
    if (( RANDOM % 2 )); then
        $this.dy = 1;
    else
        $this.dy = -1;
    fi
}

fi
