    __be__layout $this.layout;
    __be__focusIndex $layout.widgetSortIndex $widget;
    $this.switchToFocusIndex "${focusIndex}"'
besharp_oop_type_WidgetsCoordinator_method_locals["switchFocusWidget"]='local focusIndex
local layout'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["switchFocusWidget"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["switchFocusWidget"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["switchFocusWidget"]=true
besharp_oop_type_WidgetsCoordinator_methods['switchToFocusIndex']='switchToFocusIndex'
besharp_oop_type_WidgetsCoordinator_method_is_returning["switchToFocusIndex"]=true
besharp_oop_type_WidgetsCoordinator_method_body["switchToFocusIndex"]='    local sortIndex="${1}";
    __be__layout $this.layout;
    $this.currentSortIndex = "${sortIndex}";
    while @iterate @of $this.activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == sortIndex )); then
            $this.focusWidget $widget;
            $this.highlightWidget $widget;
            return;
        fi;
    done'
besharp_oop_type_WidgetsCoordinator_method_locals["switchToFocusIndex"]='local layout
local widget
local widgetSortIndex'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["switchToFocusIndex"]=true
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["switchToFocusIndex"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["switchToFocusIndex"]=true
besharp_oop_type_WidgetsCoordinator_methods['switchWidgetFocusToNext']='switchWidgetFocusToNext'
besharp_oop_type_WidgetsCoordinator_method_is_returning["switchWidgetFocusToNext"]=false
besharp_oop_type_WidgetsCoordinator_method_body["switchWidgetFocusToNext"]='    __be__currentSortIndex $this.currentSortIndex;
    __be__activeWidgets $this.activeWidgets;
    __be__layout $this.layout;
    local firstFocusable=;
    local lowestFoundSortIndex=100000000000000;
    local closestGreaterIndex=100000000000000;
    local nextWidget=;
    while @iterate $activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == -1 )); then
            continue;
        fi;
        if (( widgetSortIndex < lowestFoundSortIndex )); then
            lowestFoundSortIndex="${widgetSortIndex}";
            firstFocusable=$widget;
        fi;
        if (( widgetSortIndex > currentSortIndex )) && (( widgetSortIndex < closestGreaterIndex )); then
            closestGreaterIndex="${widgetSortIndex}";
            nextWidget=$widget;
        fi;
    done;
    if [[ -n $nextWidget ]]; then
        $this.focusWidget $nextWidget;
        $this.highlightWidget $nextWidget;
    else
        if [[ -n $firstFocusable ]]; then
            $this.focusWidget $firstFocusable;
            $this.highlightWidget $firstFocusable;
        fi;
    fi'
besharp_oop_type_WidgetsCoordinator_method_locals["switchWidgetFocusToNext"]='local activeWidgets
local currentSortIndex
local layout
local widget
local widgetSortIndex'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["switchWidgetFocusToNext"]=true
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["switchWidgetFocusToNext"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["switchWidgetFocusToNext"]=true
besharp_oop_type_WidgetsCoordinator_methods['switchWidgetFocusToPrev']='switchWidgetFocusToPrev'
besharp_oop_type_WidgetsCoordinator_method_is_returning["switchWidgetFocusToPrev"]=false
besharp_oop_type_WidgetsCoordinator_method_body["switchWidgetFocusToPrev"]='    __be__currentSortIndex $this.currentSortIndex;
    __be__activeWidgets $this.activeWidgets;
    __be__layout $this.layout;
    local lastFocusable=;
    local highestFoundSortIndex=-1;
    local closestLowerIndex=-1;
    local prevWidget=;
    while @iterate $activeWidgets @in widget; do
        __be__widgetSortIndex $layout.widgetSortIndex $widget;
        if (( widgetSortIndex == -1 )); then
            continue;
        fi;
        if (( widgetSortIndex > highestFoundSortIndex )); then
            highestFoundSortIndex="${widgetSortIndex}";
            lastFocusable=$widget;
        fi;
        if (( widgetSortIndex < currentSortIndex )) && (( widgetSortIndex > closestLowerIndex )); then
            closestLowerIndex="${widgetSortIndex}";
            prevWidget=$widget;
        fi;
    done;
    if [[ -n $prevWidget ]]; then
        $this.focusWidget $prevWidget;
        $this.highlightWidget $prevWidget;
    else
        if [[ -n $lastFocusable ]]; then
            $this.focusWidget $lastFocusable;
            $this.highlightWidget $lastFocusable;
        fi;
    fi'
besharp_oop_type_WidgetsCoordinator_method_locals["switchWidgetFocusToPrev"]='local activeWidgets
local currentSortIndex
local layout
local widget
local widgetSortIndex'
besharp_oop_type_WidgetsCoordinator_method_is_using_iterators["switchWidgetFocusToPrev"]=true
besharp_oop_type_WidgetsCoordinator_method_is_calling_parent["switchWidgetFocusToPrev"]=false
besharp_oop_type_WidgetsCoordinator_method_is_calling_this["switchWidgetFocusToPrev"]=true

fi
if ${beshfile_section__code:-false}; then :; 
