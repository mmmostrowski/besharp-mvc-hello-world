besharp_oop_type_WidgetsLayout_method_body["addWidget"]='    local widget="${1}";
    local widgetId="${2}";
    __be__newContext @new WidgetDrawContext;
    __be__widget2FocusIndex $this.widget2FocusIndex;
    __be__widget2Context $this.widget2Context;
    __be__context2Widget $this.context2Widget;
    __be__widgetId2widget $this.widgetId2widget;
    $newContext.widgetId = "${widgetId}";
    if @true $widgetId2widget.hasKey "${widgetId}"; then
        __be__currentWidget $widgetId2widget.get "${widgetId}";
        __be__currentWidgetInfo @widgets.info $currentWidget;
        besharp.runtime.error "Widget id must be unique! '"'"'${widgetId}'"'"' widget id is already in use by: ${currentWidgetInfo}.";
    fi;
    $widgetId2widget.set "${widgetId}" $widget;
    $widget2FocusIndex.set $widget -1;
    $widget2Context.set $widget $newContext;
    $context2Widget.set $newContext $widget'
besharp_oop_type_WidgetsLayout_method_locals["addWidget"]='local context2Widget
local currentWidget
local currentWidgetInfo
local newContext
local widget2Context
local widget2FocusIndex
local widgetId2widget'
