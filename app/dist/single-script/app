    @parent "${@}"
}
TextWidget.bindWidgetState () 
{ 
    $this.state = "${1}"
}
TextWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__text $this.text;
    local line;
    local linesCount=0;
    while IFS= read -r line; do
        $calculator.takeGreaterWidth "${#line}";
        (( ++linesCount ));
    done <<< "${text}";
    $calculator.takeGreaterHeight ${linesCount}
}
TextWidget.color () 
{ 
    __be__state $this.state;
    besharp.returningOf $state.readWidgetState $this 'color' ''
}
TextWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__text $this.text;
    __be__color $this.color;
    __be__x $context.x1;
    __be__y $context.y1;
    if [[ -n "${color}" ]]; then
        __be__color @pixel_modes.${color};
    fi;
    __be__canvasPrinter $this.canvasPrinter;
    local line;
    while IFS= read -r line; do
        $canvasPrinter.printSparseText $canvas "${x}" "$(( y++ ))" "${line}" ${color};
    done <<< "${text}"
}
