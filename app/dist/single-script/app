TabsWidget () 
{ 
    __be___this_widgets @vectors.make;
    __be___this_widgetPositions @maps.make;
    __be___this_tabNames @vectors.make;
    __be___this_activeOne @vectors.make "";
    local name;
    local isEven=true;
    for item in "${@}";
    do
        if $isEven; then
            isEven=false;
            name="${item}";
        else
            isEven=true;
            $this.addTab "${name}" "${item}";
        fi;
    done
}
TabsWidget.addTab () 
{ 
    local name="${1}";
    local widget="${2}";
    __be__widgets $this.widgets;
    if @true $widgets.isEmpty; then
        $this.activeTab = 0;
    fi;
    __be__tabNames $this.tabNames;
    __be__widgetPositions $this.widgetPositions;
    __be__idx $widgets.size;
    $widgets.add $widget;
    $tabNames.add "${name}";
    $widgetPositions.set $widget "${idx}"
}
TabsWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    local maxWidth=0;
    while @iterate @of $this.tabNames @in tabName; do
        (( maxWidth += ( ${#tabName} + 10 ) ));
    done;
    local maxHeight=-1;
    while @iterate @of $this.widgets @in children; do
        $calculator.calculateMinSize $children;
        __be__maxWidth $calculator.greaterWidth "${maxWidth}";
        __be__maxHeight $calculator.greaterHeight "${maxHeight}";
    done;
    (( ++maxHeight ));
    $calculator.setupSize "${maxWidth}" "${maxHeight}"
}
TabsWidget.childrenWidgets () 
{ 
    besharp.returningOf $this.widgets
}
TabsWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__activeWidget $this.activeWidget;
    __be__activeTab $this.activeTab;
    if [[ -z "${activeWidget}" ]] || [[ -z "${activeTab}" ]]; then
        return;
    fi;
    __be__printer $this.printer;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__isFocused $context.focusedFlag;
    __be__isHighlighted $context.isHighlighted;
    __be__activeColor @pixel_modes.white;
    __be__inactiveColor @pixel_modes.dGray;
    __be__tabNames $this.tabNames;
    __be__tabNamesSize $tabNames.size;
    (( ++x1 ));
    if $isHighlighted; then
        if $isFocused; then
            local idx=0;
            while (( idx < tabNamesSize )); do
                __be__tabName $tabNames.get "${idx}";
                if (( idx++ == activeTab )); then
                    $printer.printText $canvas "${x1}" "${y1}" "[ < ${tabName} > ]" "${activeColor}";
                    (( x1 += ${#tabName} ));
                    (( x1 += 10 ));
                else
                    $printer.printText $canvas "${x1}" "${y1}" "[ ${tabName} ]" "${inactiveColor}";
                    (( x1 += ${#tabName} ));
                    (( x1 += 6 ));
                fi;
            done;
        else
            local idx=0;
            while (( idx < tabNamesSize )); do
                __be__tabName $tabNames.get "$(( idx++ ))";
                $printer.printText $canvas "${x1}" "${y1}" "[ ${tabName} ]" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            done;
        fi;
    else
        local idx=0;
        while (( idx < tabNamesSize )); do
            __be__tabName $tabNames.get "${idx}";
            if (( idx++ == activeTab )); then
                $printer.printText $canvas "${x1}" "${y1}" "| ${tabName} |" "${activeColor}" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            else
                $printer.printText $canvas "${x1}" "${y1}" "| ${tabName} |" "${inactiveColor}";
                (( x1 += ${#tabName} ));
                (( x1 += 6 ));
            fi;
        done;
    fi;
    $widgetsLayout.drawWidget $activeWidget $canvas
}
TabsWidget.initialize () 
{ 
    __be__coordinator $this.coordinator;
    __be__widgets $this.widgets;
    while @iterate @of $this.widgets @in widget; do
        $coordinator.deactivateWidget $widget;
        $coordinator.makeWidgetInvisible $widget;
    done;
    __be__activeTab $this.activeTab;
    if (( activeTab >= 0 )); then
        $this.selectTab "${activeTab}";
    fi
}
TabsWidget.operativeChildrenWidget () 
{ 
    besharp.returningOf $this.activeOne
}
TabsWidget.organizeChildrenOnLayout () 
{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    (( ++y1 ));
    (( --height ));
    while @iterate @of $this.widgets @in children; do
        $widgetsLayout.setupContextWindow $children "${x1}" "${y1}" "${width}" "${height}";
    done
}
TabsWidget.processKeyboardEvent () 
{ 
    local event="${1}";
    local isDirect="${2}";
    if $isDirect && @true $event.consumeWhenCommandRequested 'down' 'right'; then
        $this.selectNextTab;
    else
        if $isDirect && @true $event.consumeWhenCommandRequested 'up' 'left'; then
            $this.selectPrevTab;
        fi;
    fi
}
TabsWidget.selectNextTab () 
{ 
    __be__tab $this.activeTab;
    __be__widgets $this.widgets;
    __be__tabsNum $widgets.size;
    if (( ++tab >= tabsNum )); then
        tab=0;
    fi;
    $this.selectTab "${tab}"
}
TabsWidget.selectPrevTab () 
{ 
    __be__tab $this.activeTab;
    __be__widgets $this.widgets;
    __be__tabsNum $widgets.size;
    if (( --tab < 0 )); then
        tab=$(( tabsNum - 1 ));
    fi;
    $this.selectTab "${tab}"
}
TabsWidget.selectTab () 
{ 
    local tab="${1}";
    __be__coordinator $this.coordinator;
    __be__previousActive $this.activeTab;
    if (( previousActive >= 0 )); then
        __be__previousWidget $this.activeWidget;
        if [[ -n $previousWidget ]]; then
            $coordinator.deactivateWidget $previousWidget;
            $coordinator.makeWidgetInvisible $previousWidget;
        fi;
    fi;
    __be__widgets $this.widgets;
    __be__newWidget $widgets.get "${tab}";
    $this.activeTab = "${tab}";
    __be__activeOne $this.activeOne;
    $activeOne.set 0 $newWidget;
    $this.activeWidget = $newWidget;
    $coordinator.activateWidget $newWidget;
    $coordinator.makeWidgetVisible $newWidget
}
TabsWidget.selectWidget () 
{ 
    local widget="${1}";
    __be__idx $widgetPositions.get $widget;
    $this.selectTab "${idx}"
}

fi
if ${beshfile_section__oop_meta:-false}; then :; 
besharp_oop_type_is["TwoColumnsWidget"]='class'
besharp_oop_types["TwoColumnsWidget"]="TwoColumnsWidget"
declare -Ag besharp_oop_type_TwoColumnsWidget_interfaces
besharp_oop_type_TwoColumnsWidget_interfaces['Widget']='Widget'
besharp_oop_type_TwoColumnsWidget_interfaces['ContainerWidget']='ContainerWidget'
besharp_oop_classes["TwoColumnsWidget"]="TwoColumnsWidget"
besharp_oop_type_parent["TwoColumnsWidget"]="Object"
declare -Ag besharp_oop_type_TwoColumnsWidget_fields
declare -Ag besharp_oop_type_TwoColumnsWidget_injectable_fields
declare -Ag besharp_oop_type_TwoColumnsWidget_field_type
declare -Ag besharp_oop_type_TwoColumnsWidget_field_default
declare -Ag besharp_oop_type_TwoColumnsWidget_methods
declare -Ag besharp_oop_type_TwoColumnsWidget_method_body
declare -Ag besharp_oop_type_TwoColumnsWidget_method_locals
declare -Ag besharp_oop_type_TwoColumnsWidget_method_is_returning
declare -Ag besharp_oop_type_TwoColumnsWidget_method_is_abstract
declare -Ag besharp_oop_type_TwoColumnsWidget_method_is_using_iterators
declare -Ag besharp_oop_type_TwoColumnsWidget_method_is_calling_parent
declare -Ag besharp_oop_type_TwoColumnsWidget_method_is_calling_this
besharp_oop_type_TwoColumnsWidget_fields['leftWidget']='leftWidget'
besharp_oop_type_TwoColumnsWidget_field_type['leftWidget']="Widget"
besharp_oop_type_TwoColumnsWidget_field_default['leftWidget']=""
besharp_oop_type_TwoColumnsWidget_fields['rightWidget']='rightWidget'
besharp_oop_type_TwoColumnsWidget_field_type['rightWidget']="Widget"
besharp_oop_type_TwoColumnsWidget_field_default['rightWidget']=""
besharp_oop_type_TwoColumnsWidget_fields['leftSize']='leftSize'
besharp_oop_type_TwoColumnsWidget_field_default['leftSize']=""
besharp_oop_type_TwoColumnsWidget_fields['rightSize']='rightSize'
besharp_oop_type_TwoColumnsWidget_field_default['rightSize']=""
besharp_oop_type_TwoColumnsWidget_fields['bothWidgets']='bothWidgets'
besharp_oop_type_TwoColumnsWidget_field_type['bothWidgets']="Vector"
besharp_oop_type_TwoColumnsWidget_field_default['bothWidgets']=""
besharp_oop_type_TwoColumnsWidget_methods['TwoColumnsWidget']='TwoColumnsWidget'
besharp_oop_type_TwoColumnsWidget_method_is_returning["TwoColumnsWidget"]=false
besharp_oop_type_TwoColumnsWidget_method_body["TwoColumnsWidget"]='    $this.leftWidget = "${1}";
    $this.leftSize = "${2}";
    $this.rightWidget = "${3}";
    $this.rightSize = "${4}";
    __be___this_bothWidgets @vectors.makeImmutable "${1}" "${3}"'
besharp_oop_type_TwoColumnsWidget_method_locals["TwoColumnsWidget"]=''
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["TwoColumnsWidget"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["TwoColumnsWidget"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["TwoColumnsWidget"]=true
besharp_oop_type_TwoColumnsWidget_methods['calculateMinSize']='calculateMinSize'
besharp_oop_type_TwoColumnsWidget_method_is_returning["calculateMinSize"]=false
besharp_oop_type_TwoColumnsWidget_method_body["calculateMinSize"]='    local calculator="${1}";
    __be__leftWidget $this.leftWidget;
    $calculator.calculateMinSize $leftWidget;
    __be__leftWidth $calculator.width;
    __be__leftHeight $calculator.height;
    __be__rightWidget $this.rightWidget;
    $calculator.calculateMinSize $rightWidget;
    $calculator.resizeWidthBy "${leftWidth}";
    $calculator.takeGreaterHeight "${leftHeight}"'
besharp_oop_type_TwoColumnsWidget_method_locals["calculateMinSize"]='local leftHeight
local leftWidget
local leftWidth
local rightWidget'
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["calculateMinSize"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["calculateMinSize"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["calculateMinSize"]=true
besharp_oop_type_TwoColumnsWidget_methods['childrenWidgets']='childrenWidgets'
besharp_oop_type_TwoColumnsWidget_method_is_returning["childrenWidgets"]=true
besharp_oop_type_TwoColumnsWidget_method_body["childrenWidgets"]='    besharp.returningOf $this.bothWidgets'
besharp_oop_type_TwoColumnsWidget_method_locals["childrenWidgets"]=''
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["childrenWidgets"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["childrenWidgets"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["childrenWidgets"]=true
besharp_oop_type_TwoColumnsWidget_methods['draw']='draw'
besharp_oop_type_TwoColumnsWidget_method_is_returning["draw"]=false
besharp_oop_type_TwoColumnsWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__leftWidget $this.leftWidget;
    __be__rightWidget $this.rightWidget;
    $widgetsLayout.drawWidget $leftWidget $canvas;
    $widgetsLayout.drawWidget $rightWidget $canvas'
besharp_oop_type_TwoColumnsWidget_method_locals["draw"]='local leftWidget
local rightWidget'
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["draw"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["draw"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["draw"]=true
besharp_oop_type_TwoColumnsWidget_methods['initialize']='initialize'
besharp_oop_type_TwoColumnsWidget_method_is_returning["initialize"]=false
besharp_oop_type_TwoColumnsWidget_method_body["initialize"]='    __be__leftWidget $this.leftWidget;
    __be__rightWidget $this.rightWidget;
    __be__leftSize $this.leftSize;
    __be__rightSize $this.rightSize;
    if [[ "${leftSize}" == '"'"'auto'"'"' ]] && [[ "${rightSize}" == '"'"'auto'"'"' ]]; then
        besharp.runtime.error "One of two columns in TwoColumnsWidget must have '"'"'auto'"'"' set as column size!";
    fi;
    if [[ "${leftSize}" != '"'"'auto'"'"' ]] && [[ "${rightSize}" != '"'"'auto'"'"' ]]; then
        besharp.runtime.error "There only one of two columns in TwoColumnsWidget can have '"'"'auto'"'"' set as column size!";
    fi'
besharp_oop_type_TwoColumnsWidget_method_locals["initialize"]='local leftSize
local leftWidget
local rightSize
local rightWidget'
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["initialize"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["initialize"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["initialize"]=true
besharp_oop_type_TwoColumnsWidget_methods['operativeChildrenWidget']='operativeChildrenWidget'
besharp_oop_type_TwoColumnsWidget_method_is_returning["operativeChildrenWidget"]=true
besharp_oop_type_TwoColumnsWidget_method_body["operativeChildrenWidget"]='    besharp.returningOf $this.bothWidgets'
besharp_oop_type_TwoColumnsWidget_method_locals["operativeChildrenWidget"]=''
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["operativeChildrenWidget"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["operativeChildrenWidget"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["operativeChildrenWidget"]=true
besharp_oop_type_TwoColumnsWidget_methods['organizeChildrenOnLayout']='organizeChildrenOnLayout'
besharp_oop_type_TwoColumnsWidget_method_is_returning["organizeChildrenOnLayout"]=false
besharp_oop_type_TwoColumnsWidget_method_body["organizeChildrenOnLayout"]='    local context="${1}";
    local widgetsLayout="${2}";
    __be__leftWidget $this.leftWidget;
    __be__rightWidget $this.rightWidget;
    __be__leftSize $this.leftSize;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    if [[ "${leftSize}" == '"'"'auto'"'"' ]]; then
        __be__rightSize $this.rightSize;
        if [[ "${rightSize%\%}" != "${rightSize}" ]]; then
            rightSize=$(( ( "${rightSize%\%}" * width ) / 100 ));
        fi;
        $widgetsLayout.setupContextWindow $leftWidget "${x1}" "${y1}" "$(( width - rightSize ))" "${height}";
        $widgetsLayout.setupContextWindow $rightWidget "$(( width - rightSize + x1 ))" "${y1}" "${rightSize}" "${height}";
    else
        if [[ "${leftSize%\%}" != "${leftSize}" ]]; then
            leftSize="$(( ( "${leftSize%\%}" * width ) / 100 ))";
        fi;
        $widgetsLayout.setupContextWindow $leftWidget "${x1}" "${y1}" "${leftSize}" "${height}";
        $widgetsLayout.setupContextWindow $rightWidget "$(( leftSize + x1 ))" "${y1}" "$(( width - leftSize ))" "${height}";
    fi'
besharp_oop_type_TwoColumnsWidget_method_locals["organizeChildrenOnLayout"]='local height
local leftSize
local leftWidget
local rightSize
local rightWidget
local width
local x1
local y1'
besharp_oop_type_TwoColumnsWidget_method_is_using_iterators["organizeChildrenOnLayout"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_parent["organizeChildrenOnLayout"]=false
besharp_oop_type_TwoColumnsWidget_method_is_calling_this["organizeChildrenOnLayout"]=true

