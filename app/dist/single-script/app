if ${beshfile_section__code:-false}; then :; 
function __be__app() {
  "${@}"
  app="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__appFactory() {
  "${@}"
  appFactory="${besharp_rcrvs[besharp_rcsl + 1]}"
}
AppEntrypoint () 
{ 
    @parent "${@}"
}
AppEntrypoint.main () 
{ 
    __be__appFactory $this.appFactory;
    __be__app $appFactory.create;
    $app.initialize;
    $app.run
}

fi
