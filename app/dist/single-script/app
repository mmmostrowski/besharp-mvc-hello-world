        besharp.returningOf $iterable.isEmpty;
    else
        if @returned @of $iterable.iterationNew == ""; then
            besharp_rcrvs[besharp_rcsl]=true;
        else
            besharp_rcrvs[besharp_rcsl]=true;
        fi;
    fi'
