besharp_oop_type_Cursor_method_is_calling_parent["Cursor"]=true
besharp_oop_type_Cursor_method_is_calling_this["Cursor"]=false
besharp_oop_type_Cursor_methods['clearAndMoveTo0x0']='clearAndMoveTo0x0'
besharp_oop_type_Cursor_method_is_returning["clearAndMoveTo0x0"]=false
besharp_oop_type_Cursor_method_body["clearAndMoveTo0x0"]='    echo -e "\033[2J";
    $this.moveTo 0 0;
    tput rmam'
besharp_oop_type_Cursor_method_locals["clearAndMoveTo0x0"]=''
besharp_oop_type_Cursor_method_is_using_iterators["clearAndMoveTo0x0"]=false
