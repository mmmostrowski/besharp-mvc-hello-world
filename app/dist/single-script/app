if ${beshfile_section__code:-false}; then :; 
function __be___this_widget2Bindings() {
  "${@}"
  eval $this"_widget2Bindings=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__bindingMap() {
  "${@}"
  bindingMap="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__field() {
  "${@}"
  field="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widget2Bindings() {
  "${@}"
  widget2Bindings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__bindingMap() {
  "${@}"
  bindingMap="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__callback() {
  "${@}"
  callback="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widget2Bindings() {
  "${@}"
  widget2Bindings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widget2Bindings() {
  "${@}"
  widget2Bindings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__bindingMap() {
  "${@}"
  bindingMap="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__field() {
  "${@}"
  field="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__widget2Bindings() {
  "${@}"
  widget2Bindings="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetStateKeeperAdapter () 
{ 
    __be___this_widget2Bindings @maps.make
}
WidgetStateKeeperAdapter.readWidgetState () 
{ 
    local widget="${1}";
    local key="${2}";
    local default="${3}";
    __be__widget2Bindings $this.widget2Bindings;
    __be__bindingMap $widget2Bindings.get $widget;
    if @false $bindingMap.hasKey "${key}"; then
        besharp_rcrvs[besharp_rcsl]="${default}";
        return;
    fi;
    __be__field $bindingMap.get "${key}";
    besharp.returningOf ${field}
}
WidgetStateKeeperAdapter.requestWidgetEvent () 
{ 
    local widget="${1}";
    local key="${2}";
    local payload="${3:-}";
    __be__widget2Bindings $this.widget2Bindings;
    __be__bindingMap $widget2Bindings.get $widget;
    __be__callback $bindingMap.get "${key}";
    ${callback} "${payload}"
}
WidgetStateKeeperAdapter.setupWidgetBindings () 
{ 
    local widget="${1}";
    local bindingMap="${2}";
    __be__widget2Bindings $this.widget2Bindings;
    $widget2Bindings.set $widget $bindingMap;
    if ! @is $widget StatefulWidget; then
        besharp.runtime.error "$widget ( $( besharp.rtti.objectType $widget ) ) is expected to implement StatefulWidget interface!";
    fi;
    $widget.bindWidgetState $this
}
WidgetStateKeeperAdapter.writeWidgetState () 
{ 
    local widget="${1}";
    local key="${2}";
    local value="${3}";
    __be__widget2Bindings $this.widget2Bindings;
    __be__bindingMap $widget2Bindings.get $widget;
    __be__field $bindingMap.get "${key}";
    ${field} = "${value}"
}

fi
