  "${@}"
  eval $this"_ballsNum=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
BallsWidget () 
{ 
    $this.ballsNum = "${1:-2}";
    __be___this_balls @new ArrayVector
}
BallsWidget.addBall () 
{ 
    local context="${1}";
    __be__w $context.width;
    __be__h $context.height;
    __be__balls $this.balls;
    __be__ball @new Ball;
    $ball.place "${w}" "${h}";
    $balls.add $ball;
    __be___this_ballsNum $balls.size
}
BallsWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    $calculator.setupSize 1 1
}
BallsWidget.draw () 
{ 
    local context="${1}";
    __be__offsetX $context.x1;
    __be__offsetY $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    __be__canvasPrinter $this.canvasPrinter;
    __be__canvas $this.canvas;
    __be__balls $this.balls;
    __be__ballsNum $this.ballsNum;
    if (( width == 1 )) || (( height == 1 )); then
        $canvasPrinter.printFrame $canvas ${offsetX} ${offsetY} ${width} ${height};
        return;
    fi;
    if (( width <= 0 )) || (( height <= 0 )); then
        return;
    fi;
    local ballChar='*';
    while (( ballsNum-- )); do
        __be__ball $balls.get $ballsNum;
        eval "
                local x=\"\$(( ${ball}_x + offsetX + 1 ))\"
                local y=\"\$(( ${ball}_y + offsetY + 1 ))\"
            ";
        $canvas.putPixel "${x}" "${y}" "${ballChar}" "\e[32m";
    done;
    $canvasPrinter.printFrame $canvas ${offsetX} ${offsetY} ${width} ${height}
}
BallsWidget.initialize () 
{ 
    :
}
BallsWidget.initializeAnimation () 
{ 
    local context="${1}";
    local layout="${2}";
    __be__w $context.width;
    __be__h $context.height;
    __be__ballsNum $this.ballsNum;
    __be__balls $this.balls;
    while (( ballsNum-- )); do
        __be__ball @new Ball;
        $ball.place "${w}" "${h}";
        $balls.add $ball;
    done
}
BallsWidget.processAnimationFrame () 
{ 
    local context="${1}";
    local layout="${2}";
    __be__w $context.width;
    __be__h $context.height;
    __be__balls $this.balls;
    __be__ballsNum $this.ballsNum;
    (( w-= 2)) || true;
    (( h-= 2)) || true;
    while (( ballsNum-- )); do
        __be__ball $balls.get $ballsNum;
        $ball.move "${w}" "${h}";
    done;
    besharp_rcrvs[besharp_rcsl]=1
}
BallsWidget.removeAllBalls () 
{ 
    __be__balls $this.balls;
    if @true $balls.isEmpty; then
        return;
    fi;
    $balls.removeAll;
    $this.ballsNum = 0
}
BallsWidget.removeBall () 
{ 
    __be__balls $this.balls;
    if @true $balls.isEmpty; then
        return;
    fi;
    $balls.removeIndex 0;
    __be___this_ballsNum $balls.size
}

fi
