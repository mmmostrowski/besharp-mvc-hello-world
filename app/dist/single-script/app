{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__upWidget $this.upWidget;
    __be__downWidget $this.downWidget;
    __be__downSize $this.downSize;
    __be__x1 $context.x1;
    __be__y1 $context.y1;
