besharp_oop_type_FpsWidget_method_body["draw"]='    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__x $context.x1;
    __be__y $context.y1;
    __be__CanvasPrinter $this.CanvasPrinter;
    __be__fps $this.fps;
    __be__color @pixel_modes.dGray;
    $CanvasPrinter.printText $canvas "${x}" "${y}" "FPS ${fps}" "${color}";
    local currentSecond="";
    currentSecond="$( date +%s 2> /dev/null || true )";
    if [[ -z "${currentSecond}" ]]; then
        return;
    fi;
    __be__frameCounter $this.frameCounter;
    __be__lastSecond $this.lastSecond;
    (( ++frameCounter ));
    if (( currentSecond > lastSecond )); then
        $this.fps = $(( frameCounter / ( currentSecond - lastSecond ) ));
        $this.frameCounter = 0;
        $this.lastSecond = "${currentSecond}";
    else
        $this.frameCounter = "${frameCounter}";
    fi'
besharp_oop_type_FpsWidget_method_locals["draw"]='local CanvasPrinter
local color
local fps
local frameCounter
local lastSecond
