WidgetMinSizeCalculator () 
{ 
    :
}
WidgetMinSizeCalculator.calculateMinSize () 
{ 
    local widget="${1}";
    $this.width = 0;
    $this.height = 0;
    $widget.calculateMinSize $this;
    __be__contexts $this.contexts;
    __be__context $contexts.widgetContext $widget;
    __be__placementLeft $context.placementLeft;
    __be__placementRight $context.placementRight;
    __be__placementUp $context.placementUp;
    __be__placementDown $context.placementDown;
    __be__placementWidth $context.placementWidth;
    __be__placementHeight $context.placementHeight;
    if [[ "${placementUp}" == 'auto' ]] || (( placementUp < 0 )); then
        placementUp=0;
    fi;
    if [[ "${placementDown}" == 'auto' ]] || (( placementDown < 0 )); then
        placementDown=0;
    fi;
    if [[ "${placementLeft}" == 'auto' ]] || (( placementLeft < 0 )); then
        placementLeft=0;
    fi;
    if [[ "${placementRight}" == 'auto' ]] || (( placementRight < 0 )); then
        placementRight=0;
    fi;
    $this.takeGreaterWidth "${placementWidth}";
    $this.takeGreaterHeight "${placementHeight}";
    $this.resizeWidthBy $(( placementLeft + placementRight ));
    $this.resizeHeightBy $(( placementUp + placementDown ))
}
WidgetMinSizeCalculator.greaterHeight () 
{ 
    local toCompare="${1}";
    __be__h $this.height;
    if (( toCompare > h )); then
        besharp_rcrvs[besharp_rcsl]="${toCompare}";
    else
        besharp_rcrvs[besharp_rcsl]="${h}";
    fi
}
WidgetMinSizeCalculator.greaterWidth () 
{ 
    local toCompare="${1}";
    __be__w $this.width;
    if (( toCompare > w )); then
        besharp_rcrvs[besharp_rcsl]="${toCompare}";
    else
        besharp_rcrvs[besharp_rcsl]="${w}";
    fi
}
WidgetMinSizeCalculator.resizeHeightBy () 
{ 
    local resizeBy="${1}";
    __be__h $this.height;
    $this.height = "$(( h + resizeBy ))"
}
WidgetMinSizeCalculator.resizeWidthBy () 
{ 
    local resizeBy="${1}";
    __be__w $this.width;
    $this.width = "$(( w + resizeBy ))"
}
WidgetMinSizeCalculator.setupSize () 
{ 
    $this.width = "${1}";
    $this.height = "${2}"
}
WidgetMinSizeCalculator.takeGreaterHeight () 
{ 
    local toCompare="${1}";
    __be__h $this.height;
    if (( toCompare > h )); then
        $this.height = "${toCompare}";
    fi
}
WidgetMinSizeCalculator.takeGreaterWidth () 
{ 
    local toCompare="${1}";
    __be__w $this.width;
    if (( toCompare > w )); then
        $this.width = "${toCompare}";
    fi
}

fi
