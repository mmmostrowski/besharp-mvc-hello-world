besharp_oop_type_LayersWidget_method_is_calling_this["showLayer"]=true
besharp_oop_type_LayersWidget_methods['switchLayerFocusIndex']='switchLayerFocusIndex'
besharp_oop_type_LayersWidget_method_is_returning["switchLayerFocusIndex"]=false
