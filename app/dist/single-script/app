        if @is $iterable @a PrimitiveObject && ( @is $iterable @a List || @is $iterable @a Set || @is $iterable @a Map ); then
            eval "${targetList}+=( \"\${${iterable}[@]}\" )";
        else
            $targetList.addMany $iterable;
        fi;
    done'
