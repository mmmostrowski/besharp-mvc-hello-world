    __be__offsetX $context.x1;
    __be__offsetY $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    __be__canvasPrinter $this.canvasPrinter;
    __be__canvas $this.canvas;
    __be__balls $this.balls;
    __be__ballsNum $this.ballsNum;
    if (( width == 1 )) || (( height == 1 )); then
        $canvasPrinter.printFrame $canvas ${offsetX} ${offsetY} ${width} ${height};
        return;
    fi;
    if (( width <= 0 )) || (( height <= 0 )); then
        return;
    fi;
    local ballChar='"'"'*'"'"';
    while (( ballsNum-- )); do
        __be__ball $balls.get $ballsNum;
        eval "
                local x=\"\$(( ${ball}_x + offsetX + 1 ))\"
                local y=\"\$(( ${ball}_y + offsetY + 1 ))\"
            ";
        $canvas.putPixel "${x}" "${y}" "${ballChar}" "\e[32m";
    done;
    $canvasPrinter.printFrame $canvas ${offsetX} ${offsetY} ${width} ${height}'
