function __be__width() {
  "${@}"
  width="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x1() {
  "${@}"
  x1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__height() {
  "${@}"
  height="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y1() {
  "${@}"
  y1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
WidgetDrawContext () 
{ 
    @parent "${@}"
}
WidgetDrawContext.isHighlighted () 
{ 
    besharp.returningOf $this.highlightedFlag
}
WidgetDrawContext.setupWindow () 
{ 
    $this.x1 = "${1}";
    $this.y1 = "${2}";
    $this.width = "${3}";
    $this.height = "${4}"
}
WidgetDrawContext.turnHighlightOff () 
{ 
    $this.highlightedFlag = false
}
WidgetDrawContext.turnHighlightOn () 
{ 
    $this.highlightedFlag = true
}
WidgetDrawContext.x2 () 
{ 
    __be__x1 $this.x1;
    __be__width $this.width;
    besharp_rcrvs[besharp_rcsl]=$(( x1 + width - 1 ))
}
WidgetDrawContext.y2 () 
{ 
    __be__y1 $this.y1;
    __be__height $this.height;
    besharp_rcrvs[besharp_rcsl]=$(( y1 + height - 1 ))
}

fi
