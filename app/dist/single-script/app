besharp_oop_type_LayersWidget_method_body["isLayerActive"]='    local layerKey="${1}";
    __be__activeLayers $this.activeLayers;
    besharp.returningOf $activeLayers.has "${layerKey}"'
besharp_oop_type_LayersWidget_method_locals["isLayerActive"]='local activeLayers'
besharp_oop_type_LayersWidget_method_is_using_iterators["isLayerActive"]=false
