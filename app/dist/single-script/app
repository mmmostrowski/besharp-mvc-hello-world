    while true; do
        read -s -r -d '"'"''"'"' -t 0.00005 -N1 input || true;
        if [[ "${input}" == '"'"''"'"' ]]; then
            local input2;
            read -s -r -d '"'"''"'"' -t 0.00005 -N2 input2 || true;
            input+="${input2}";
            if [[ "${input2}" == '"'"'[M'"'"' ]]; then
                local input3;
                read -s -r -d '"'"''"'"' -t 0.00005 -N3 input3 || true;
                input+="${input3}";
            fi;
        fi;
        if [[ -n "${input}" ]]; then
            finalInput="${input}";
        else
            break;
        fi;
    done;
    besharp_rcrvs[besharp_rcsl]="${finalInput}"'
besharp_oop_type_KeyboardDriver_method_locals["scanKeys"]=''
besharp_oop_type_KeyboardDriver_method_is_using_iterators["scanKeys"]=false
