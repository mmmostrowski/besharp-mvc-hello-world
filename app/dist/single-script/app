besharp_oop_type_PrimitiveCollection_method_is_returning["addMany"]=false
besharp_oop_type_PrimitiveCollection_method_body["addMany"]='    local item;
    while @iterate "${@}" @in item; do
        $this.add "${item}";
    done'
