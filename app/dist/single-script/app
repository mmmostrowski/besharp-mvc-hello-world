besharp_oop_type_BaseController_methods['run']='run'
besharp_oop_type_BaseController_method_is_returning["run"]=false
besharp_oop_type_BaseController_method_body["run"]='    $this.prepareRootWidget;
    $this.initializeWidgets;
    __be__widgetsLayout $this.widgetsLayout;
    while @true $this.active; do
        $this.newFrame;
        $this.stepFrame;
        if @true $widgetsLayout.needsUpdate; then
            continue;
        fi;
        $this.renderFrame;
        $this.processInput;
    done;
    $this.resetTerminal'
besharp_oop_type_BaseController_method_locals["run"]='local widgetsLayout'
