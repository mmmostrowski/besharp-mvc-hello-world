declare -Ag besharp_oop_type_PopupFrameWidget_method_is_calling_parent
declare -Ag besharp_oop_type_PopupFrameWidget_method_is_calling_this
besharp_oop_type_PopupFrameWidget_fields['canvasPrinter']='canvasPrinter'
besharp_oop_type_PopupFrameWidget_field_type['canvasPrinter']="CanvasPrinter"
besharp_oop_type_PopupFrameWidget_injectable_fields['canvasPrinter']='canvasPrinter'
besharp_oop_type_PopupFrameWidget_field_default['canvasPrinter']=""
