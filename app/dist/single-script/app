besharp_oop_type_WidgetsLayout_method_body["setupContextWindow"]='    local widget="${1}";
    shift 1;
    __be__widget2Context $this.widget2Context;
    __be__context $widget2Context.get $widget;
    $context.setupWindow "${@}"'
besharp_oop_type_WidgetsLayout_method_locals["setupContextWindow"]='local context
local widget2Context'
besharp_oop_type_WidgetsLayout_method_is_using_iterators["setupContextWindow"]=false
