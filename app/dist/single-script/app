besharp_oop_type_PopupFrameWidget_method_is_calling_this["organizeChildrenOnLayout"]=true
besharp_oop_type_PopupFrameWidget_methods['processKeyboardEvent']='processKeyboardEvent'
besharp_oop_type_PopupFrameWidget_method_is_returning["processKeyboardEvent"]=false
besharp_oop_type_PopupFrameWidget_method_body["processKeyboardEvent"]='    local event="${1}";
    if @true $event.consumeWhenCommandRequested '"'"'escape'"'"'; then
        __be__state $this.state;
        $state.requestWidgetEvent $this '"'"'on_escape'"'"';
    fi'
besharp_oop_type_PopupFrameWidget_method_locals["processKeyboardEvent"]='local state'
besharp_oop_type_PopupFrameWidget_method_is_using_iterators["processKeyboardEvent"]=false
