    if @true $layers.isLayerActive "${layer}"; then
        return;
    fi;
    __be__widgetsLayout $this.widgetsLayout;
    $layers.deactivateLayer '"'"'main'"'"';
    $layers.turnOnLayer "${layer}";
    $layers.pushLayerFocusIndex "${layer}";
    $widgetsLayout.needsUpdate = true'
