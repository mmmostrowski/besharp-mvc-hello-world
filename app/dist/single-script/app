besharp_oop_type_LayersWidget_field_type['activeWidgets']="Set"
besharp_oop_type_LayersWidget_field_default['activeWidgets']=""
besharp_oop_type_LayersWidget_fields['visibleWidgets']='visibleWidgets'
besharp_oop_type_LayersWidget_field_type['visibleWidgets']="Vector"
