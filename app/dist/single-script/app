besharp_oop_type_BallsWidget_method_is_returning["processAnimationFrame"]=true
besharp_oop_type_BallsWidget_method_body["processAnimationFrame"]='    local context="${1}";
    local layout="${2}";
    __be__w $context.width;
    __be__h $context.height;
    __be__balls $this.balls;
    __be__ballsNum $this.ballsNum;
    (( w-= 2)) || true;
    (( h-= 2)) || true;
    while (( ballsNum-- )); do
        __be__ball $balls.get $ballsNum;
        $ball.move "${w}" "${h}";
    done;
    besharp_rcrvs[besharp_rcsl]=1'
besharp_oop_type_BallsWidget_method_locals["processAnimationFrame"]='local ball
local balls
local ballsNum
local h
local w'
besharp_oop_type_BallsWidget_method_is_using_iterators["processAnimationFrame"]=false
besharp_oop_type_BallsWidget_method_is_calling_parent["processAnimationFrame"]=false
besharp_oop_type_BallsWidget_method_is_calling_this["processAnimationFrame"]=true
besharp_oop_type_BallsWidget_methods['removeAllBalls']='removeAllBalls'
besharp_oop_type_BallsWidget_method_is_returning["removeAllBalls"]=true
