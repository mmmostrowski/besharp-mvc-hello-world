besharp_oop_type_MouseDriver_methods['disableMouseCapture']='disableMouseCapture'
besharp_oop_type_MouseDriver_method_is_returning["disableMouseCapture"]=false
besharp_oop_type_MouseDriver_method_body["disableMouseCapture"]='    echo -e "\e[?1000l";
    echo -e "\e[?1002l";
    echo -e "\e[?1003l";
    echo -e "\e[?9l"'
besharp_oop_type_MouseDriver_method_locals["disableMouseCapture"]=''
besharp_oop_type_MouseDriver_method_is_using_iterators["disableMouseCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["disableMouseCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["disableMouseCapture"]=false
besharp_oop_type_MouseDriver_methods['enableMouseButtonsCapture']='enableMouseButtonsCapture'
besharp_oop_type_MouseDriver_method_is_returning["enableMouseButtonsCapture"]=false
besharp_oop_type_MouseDriver_method_body["enableMouseButtonsCapture"]='    echo -e "\e[?1000h";
    echo -e "\e[?9h";
    $this.subscribeToShutdown'
besharp_oop_type_MouseDriver_method_locals["enableMouseButtonsCapture"]=''
besharp_oop_type_MouseDriver_method_is_using_iterators["enableMouseButtonsCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["enableMouseButtonsCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["enableMouseButtonsCapture"]=true
besharp_oop_type_MouseDriver_methods['enableMouseDragCapture']='enableMouseDragCapture'
besharp_oop_type_MouseDriver_method_is_returning["enableMouseDragCapture"]=false
besharp_oop_type_MouseDriver_method_body["enableMouseDragCapture"]='    echo -e "\e[?1002h";
    $this.subscribeToShutdown'
besharp_oop_type_MouseDriver_method_locals["enableMouseDragCapture"]=''
besharp_oop_type_MouseDriver_method_is_using_iterators["enableMouseDragCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["enableMouseDragCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["enableMouseDragCapture"]=true
besharp_oop_type_MouseDriver_methods['enableMouseMovementCapture']='enableMouseMovementCapture'
besharp_oop_type_MouseDriver_method_is_returning["enableMouseMovementCapture"]=false
besharp_oop_type_MouseDriver_method_body["enableMouseMovementCapture"]='    echo -e "\e[?1003h";
    $this.subscribeToShutdown'
besharp_oop_type_MouseDriver_method_locals["enableMouseMovementCapture"]=''
besharp_oop_type_MouseDriver_method_is_using_iterators["enableMouseMovementCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["enableMouseMovementCapture"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["enableMouseMovementCapture"]=true
besharp_oop_type_MouseDriver_methods['subscribeToShutdown']='subscribeToShutdown'
besharp_oop_type_MouseDriver_method_is_returning["subscribeToShutdown"]=false
besharp_oop_type_MouseDriver_method_body["subscribeToShutdown"]='    besharp.addShutdownCallback '"'"'mouse_disable_capture'"'"' $this.disableMouseCapture'
besharp_oop_type_MouseDriver_method_locals["subscribeToShutdown"]=''
besharp_oop_type_MouseDriver_method_is_using_iterators["subscribeToShutdown"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["subscribeToShutdown"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["subscribeToShutdown"]=true
besharp_oop_type_MouseDriver_methods['updateMouseStatus']='updateMouseStatus'
besharp_oop_type_MouseDriver_method_is_returning["updateMouseStatus"]=false
besharp_oop_type_MouseDriver_method_body["updateMouseStatus"]='    local input="${1}";
    local target="${2}";
    __be__buttonStatus $this.chrToInt "${input:3:1}";
    __be___target_mouseX $this.chrToInt "${input:4:1}";
    __be___target_mouseY $this.chrToInt "${input:5:1}";
    case ${buttonStatus} in 
        0)
            $target.mouseButtonLeft = true;
            $target.mouseButtonRight = false
        ;;
        1)
            $target.mouseButtonLeft = false;
            $target.mouseButtonRight = true
        ;;
        2)
            $target.mouseButtonLeft = false;
            $target.mouseButtonRight = false
        ;;
    esac'
besharp_oop_type_MouseDriver_method_locals["updateMouseStatus"]='local buttonStatus'
besharp_oop_type_MouseDriver_method_is_using_iterators["updateMouseStatus"]=false
besharp_oop_type_MouseDriver_method_is_calling_parent["updateMouseStatus"]=false
besharp_oop_type_MouseDriver_method_is_calling_this["updateMouseStatus"]=true

fi
if ${beshfile_section__code:-false}; then :; 
function __be__buttonStatus() {
  "${@}"
  buttonStatus="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be___target_mouseX() {
  "${@}"
  eval $target"_mouseX=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be___target_mouseY() {
  "${@}"
  eval $target"_mouseY=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
MouseDriver () 
{ 
    @parent "${@}"
}
MouseDriver.chrToInt () 
{ 
    case "${1}" in 
        '!')
            besharp_rcrvs[besharp_rcsl]=0
        ;;
        '"')
            besharp_rcrvs[besharp_rcsl]=1
        ;;
        '#')
            besharp_rcrvs[besharp_rcsl]=2
        ;;
        '$')
            besharp_rcrvs[besharp_rcsl]=3
        ;;
        '%')
            besharp_rcrvs[besharp_rcsl]=4
        ;;
        '&')
            besharp_rcrvs[besharp_rcsl]=5
        ;;
        \')
            besharp_rcrvs[besharp_rcsl]=6
        ;;
        '(')
            besharp_rcrvs[besharp_rcsl]=7
        ;;
        ')')
            besharp_rcrvs[besharp_rcsl]=8
        ;;
        '*')
            besharp_rcrvs[besharp_rcsl]=9
        ;;
        '+')
            besharp_rcrvs[besharp_rcsl]=10
        ;;
        ',')
            besharp_rcrvs[besharp_rcsl]=11
        ;;
        '-')
            besharp_rcrvs[besharp_rcsl]=12
        ;;
        '.')
            besharp_rcrvs[besharp_rcsl]=13
        ;;
        '/')
            besharp_rcrvs[besharp_rcsl]=14
        ;;
        '0')
            besharp_rcrvs[besharp_rcsl]=15
        ;;
        '1')
            besharp_rcrvs[besharp_rcsl]=16
        ;;
        '2')
            besharp_rcrvs[besharp_rcsl]=17
        ;;
        '3')
            besharp_rcrvs[besharp_rcsl]=18
        ;;
        '4')
            besharp_rcrvs[besharp_rcsl]=19
        ;;
        '5')
            besharp_rcrvs[besharp_rcsl]=20
        ;;
        '6')
            besharp_rcrvs[besharp_rcsl]=21
