if ${beshfile_section__code:-false}; then :; 
function __be___this_children() {
  "${@}"
  eval $this"_children=\"\${besharp_rcrvs[besharp_rcsl + 1]}\""
}
function __be__composedWidget() {
  "${@}"
  composedWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__composedWidget() {
  "${@}"
  composedWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__composedWidget() {
  "${@}"
  composedWidget="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__height() {
  "${@}"
  height="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__width() {
  "${@}"
  width="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__x1() {
  "${@}"
  x1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
function __be__y1() {
  "${@}"
  y1="${besharp_rcrvs[besharp_rcsl + 1]}"
}
FrameWidget () 
{ 
    $this.composedWidget = "${1}";
    __be___this_children @vectors.single "${1}"
}
FrameWidget.calculateMinSize () 
{ 
    local calculator="${1}";
    __be__composedWidget $this.composedWidget;
    $calculator.calculateMinSize $composedWidget;
    $calculator.resizeWidthBy 2;
    $calculator.resizeHeightBy 2
}
FrameWidget.childrenWidgets () 
{ 
    besharp.returningOf $this.children
}
FrameWidget.draw () 
{ 
    local context="${1}";
    local canvas="${2}";
    local widgetsLayout="${3}";
    __be__composedWidget $this.composedWidget;
    $widgetsLayout.drawWidget $composedWidget $canvas;
    eval "
            local x1=\"\${${context}_x1}\"
            local y1=\"\${${context}_y1}\"
            local width=\"\${${context}_width}\"
            local height=\"\${${context}_height}\"
            local canvasPrinter=\"\${${this}_canvasPrinter}\"
        ";
    if @true $context.isHighlighted; then
        $canvasPrinter.printHighlightFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    else
        $canvasPrinter.printFrame $canvas "${x1}" "${y1}" "${width}" "${height}";
    fi
}
FrameWidget.initialize () 
{ 
    :
}
FrameWidget.operativeChildrenWidget () 
{ 
    besharp.returningOf $this.children
}
FrameWidget.organizeChildrenOnLayout () 
{ 
    local context="${1}";
    local widgetsLayout="${2}";
    __be__x1 $context.x1;
    __be__y1 $context.y1;
    __be__width $context.width;
    __be__height $context.height;
    __be__composedWidget $this.composedWidget;
    $widgetsLayout.setupContextWindow $composedWidget "$(( x1 + 1 ))" "$(( y1 + 1 ))" "$(( width - 2 ))" "$(( height - 2 ))"
}

fi
